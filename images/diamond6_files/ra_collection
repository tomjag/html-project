		//Product Details Page: Tech Specs
		techSpecsEventHandler: function (selectedData, productViewData) {
			if (typeof (selectedData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				if (adobeDataLayerJS.brandCode == "ft") {
					adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				} else {
					adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructSelectedProductObject(selectedData, productViewData);
				}
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Tech_Specs_Link');

			}
		},
		//Product Details Page: Product Description
		productDescriptionEventHandler: function (selectedData, productViewData) {
			if (typeof (selectedData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				if (adobeDataLayerJS.brandCode == "ft") {
					adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
