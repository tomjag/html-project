		// Util function to traverse catalog hierarchy service response
		traverseCatalogHierarchy: function(){
			if(typeof(analyticsJS.catalogHierarchy) != "undefined"){

				analyticsJS.flattenedCategoryList = {};
				window.flattenedCategoryList = {};
				var category = {};
				category.identifier = "home-page";
				category.name = "Home Page";
				category.urlKeyword = "home";
				category.uniqueID = "-1";

				analyticsJS.flattenedCategoryList["-1"] = category;
				window.flattenedCategoryList["-1"] = category;

				for(var i = 0; i < analyticsJS.catalogHierarchy.catalogGroupView.length; i++){
					analyticsJS.parseCatalogGroupView(analyticsJS.catalogHierarchy.catalogGroupView[i], "-1");
				}
			}
		},
			
	
		// Util function to add catgroup info from WCS format to the new map
		parseCatalogGroupView: function(catalogGroupView, parentCategoryID){
			if(typeof(catalogGroupView) != "undefined" && typeof(catalogGroupView.uniqueID) != "undefined"){

				var category = {};
				if(catalogGroupView.parentCatalogGroupID == "-1"){
					category.topCategory = "true";
				}else{
					category.topCategory = "false";
				}
				category.identifier = catalogGroupView.identifier;
				category.name = catalogGroupView.name;
				category.urlKeyword = catalogGroupView.urlKeyword;
				if(category.urlKeyword!= null && typeof(category.urlKeyword) != "undefined" && category.urlKeyword.indexOf(window.pageProperties['analytics.brand.code']+'\-') == 0){
					category.urlKeyword=category.urlKeyword.replace((window.pageProperties['analytics.brand.code']+'\-'),"");
				}
				category.uniqueID = catalogGroupView.uniqueID;
				category.parentCategoryID = parentCategoryID;
				category.storeFrontUrl = catalogGroupView.storeFrontUrl;

				if(typeof(catalogGroupView.catalogGroupView) == "undefined"){
					category.leafCategory = "true";
				}else{
					category.leafCategory = "false";
				}

				analyticsJS.flattenedCategoryList[catalogGroupView.uniqueID] = category;
				window.flattenedCategoryList[catalogGroupView.uniqueID] = category;

				if(typeof(catalogGroupView.catalogGroupView) != "undefined"){
					for(var i = 0; i < catalogGroupView.catalogGroupView.length; i++){
						analyticsJS.parseCatalogGroupView(catalogGroupView.catalogGroupView[i], catalogGroupView.uniqueID);
					}
				}
			}
		},

		// Initializing all "Business Intelligence" integration on the storefront
		init: function(){

			//Ability to control switch on/off any business intelligence integration
			analyticsJS.isCoremetricsEnabled = typeof(window.pageProperties) != "undefined" && window.pageProperties['analytics.coremetrics.enable'] == 'true';
			analyticsJS.isYFRETEnabled = typeof(window.pageProperties) != "undefined" && window.pageProperties['analytics.YFRET.enable'] == 'true';
			analyticsJS.isGAEnabled = typeof(window.pageProperties) != "undefined" && window.pageProperties['analytics.ga.enable'] == 'true';
			analyticsJS.isYotpoEnabled = typeof(window.pageProperties) != "undefined" && !!window.pageProperties['analytics.yotpo.api.key'] && window.pageProperties['analytics.coremetrics.digitaldata.pageInstanceID'] == 'checkout';
			analyticsJS.isAdobeAnalyticsEnabled = typeof(window.pageProperties) != "undefined" && window.pageProperties['analytics.adobe.enable'] == 'true';

			//Initialize IBM DA / Coremetrics integration
			if(analyticsJS.isCoremetricsEnabled){
				var initProperties = [];
				initProperties.push({"name": "brand", "value": window.pageProperties['analytics.brand']});
				initProperties.push({"name": "brandCode", "value": window.pageProperties['analytics.brand.code']});
				initProperties.push({"name": "brandHostname", "value": window.pageProperties['analytics.brand.hostname']});
				initProperties.push({"name": "pageID", "value": window.pageProperties['analytics.coremetrics.digitaldata.pageInstanceID']});
				initProperties.push({"name": "defaultPrimaryCategory", "value": window.pageProperties['analytics.digitalData.page.category.primaryCategory']});
				initProperties.push({"name": "flattenedCategoryList", "value": analyticsJS.flattenedCategoryList});
				ddxDataLayerJS.init(initProperties);
			}
			if(analyticsJS.isYFRETEnabled){
				var initProperties = [];
				yfretDataLayerJS.init(initProperties);
			}
			if(analyticsJS.isGAEnabled){
				var initProperties = [];
				initProperties.push({"name": "brand", "value": window.pageProperties['analytics.brand']});
				gaDataLayerJS.init(initProperties);
			}

			if(analyticsJS.isYotpoEnabled && typeof(yotpoDataLayerJS) != "undefined"){
				var initProperties = [];
				initProperties.push({"name": "apiKey", "value": window.pageProperties['analytics.yotpo.api.key']});
				yotpoDataLayerJS.init(initProperties);
			}
            
            if(typeof(schemaJS) != "undefined"){
                var initProperties = [];
                initProperties.push({"name": "brand", "value": window.pageProperties['analytics.brand']});
                initProperties.push({"name": "brandHostname", "value": window.pageProperties['analytics.brand.hostname']});
                schemaJS.init(initProperties);
			}
			
						
			//Initialize Adobe Analytics Integration
			if(analyticsJS.isAdobeAnalyticsEnabled){
				var initProperties = [];
				initProperties.push({"name": "brand", "value": window.pageProperties['analytics.brand']});
				initProperties.push({"name": "brandCode", "value": window.pageProperties['analytics.brand.code']});
				initProperties.push({"name": "brandHostname", "value": window.pageProperties['analytics.brand.hostname']});
				initProperties.push({"name": "pageID", "value": window.pageProperties['analytics.coremetrics.digitaldata.pageInstanceID']});
				initProperties.push({"name": "defaultPrimaryCategory", "value": window.pageProperties['analytics.digitalData.page.category.primaryCategory']});
				initProperties.push({"name": "flattenedCategoryList", "value": analyticsJS.flattenedCategoryList});
				adobeDataLayerJS.init(initProperties);
			}

			//List of events being listened
			var eventsList = [];

			// Home page
			eventsList.push("homePageEvent");
			eventsList.push("headerCurrencyChangeEvent");
			eventsList.push("trackLinkClickEvent");	

			//Catalog Browsing events
			eventsList.push("catalogHierarchyUpdatedEvent");
			eventsList.push("productviewByTXCategoryEvent");
			eventsList.push("productviewByTXSearchTermEvent");
			eventsList.push("quickViewEvent");
			eventsList.push("productDetailsbyIdEvent");
			eventsList.push("searchEvent");			

			//Cart related events
			eventsList.push("addToCartEvent");
			eventsList.push("miniCartViewEvent");
			eventsList.push("cartViewEvent");
			eventsList.push("updateCartEvent");
			eventsList.push("continueShoppingEvent");
			eventsList.push("changeCartPincodeEvent");
				
			//Checkout related events
			eventsList.push("loginOrCheckoutAsGuestEvent");
			eventsList.push("userDeliveryInfoEvent");
			eventsList.push("orderReviewEvent");
			eventsList.push("continueToPaymentEvent");
			eventsList.push("usablePaymentInfoEvent");
			eventsList.push("paymentSuccessEvent");
			eventsList.push("paymentFailureEvent");
			eventsList.push("orderSummaryEvent");
			eventsList.push("deliveryMethodChangeEvent");
			eventsList.push("useSelectedAddressEvent");
			eventsList.push("deliveryToNewAddressEvent");
			eventsList.push("editAddressEvent");
			eventsList.push("registerEncircleEvent");			
			eventsList.push("verifyNumberEvent");
			eventsList.push("tncClickEvent");
			eventsList.push("privacyPolicyEvent");
			eventsList.push("checkBalanceEvent");			

			//User Registration and sign In events
			eventsList.push("userRegistrationEvent");
			eventsList.push("userSignInEvent");

			//User Logout events
			eventsList.push("userlogoutEvent");

			//My-Account events
			eventsList.push("userDetailsViewEvent");
			eventsList.push("changePasswordEvent");
			eventsList.push("addNewAddressEvent");

			//Share events
			eventsList.push("shareEvent");
			eventsList.push("addToCompareEvent");
			eventsList.push("clickShareEvent");
			eventsList.push("clickOnCompareEvent");
			eventsList.push("comparePageLoadEvent");
			eventsList.push("addToCompareAdobeEvent");

			eventsList.push("userPersonalInfoViewEvent");
			eventsList.push("userAddressBookViewEvent");
			eventsList.push("userWishListViewEvent");
			eventsList.push("userOrderHistoryViewEvent");
			eventsList.push("userOrderDetailsViewEvent");

			//Component and events
			//Store-locator related events
			eventsList.push("storeLocatorSearchEvent");
			eventsList.push("storeDetailsViewEvent");
			eventsList.push("useCurrentLocationEvent");
			
			// Wish-lists related events
			eventsList.push("addToWishListEvent");
			eventsList.push("addToCartFromWishListEvent");
			eventsList.push("deleteProductFromWishlistEvent");

			
			eventsList.push("bannerTrackingEvent");
			eventsList.push("trackFormStartEvent");
			eventsList.push("trackFormSubmissionEvent");
			eventsList.push("trackVideoPlayEvent");

			eventsList.push("newWishlistClicksEvent");
			eventsList.push("chatClickEvent");
			// Newsletter subscription event
			eventsList.push("newsltterSpecialOfferEvent");

			//TODO: Book Appointment related events

            eventsList.push("bookAnAppointmentClickedEvent");
			eventsList.push("bookAppointmentEvent");
			eventsList.push("bookAppointmentCompletionEvent");
            
            eventsList.push("notifyEvent");
            eventsList.push("notifyClickedEvent");
            
			// Gift message related events
			eventsList.push("addEditGiftMessageEvent");
			eventsList.push("deleteGiftMessageEvent");

			//"Servicability check" related events
			eventsList.push("servicabilityCheckEvent");
			
			//after Proceed to checkout Login
			eventsList.push("checkOutLoginEvent");

			//"Order Cancellation" related events
			eventsList.push("orderCancellationEvent");

			//"Forgot Password" related events
			eventsList.push("forgotPasswordEvent");

			//"Move to wishlist" from cart related events
			eventsList.push("moveToWishlistEvent");
			
			//"Track order" from order related events
			eventsList.push("trackOrderEvent");

			//"breadcrumb construction event" for structured data
			eventsList.push("breadCrumbFormedEvent");
            
			eventsList.push("storeLoginEvent"); 

			//Apply / View promo code
			eventsList.push("applyPromoCodeEvent"); 
			eventsList.push("viewPromoCodeEvent"); 
			eventsList.push("viewPromoCodeClickEvent"); 
			eventsList.push("cartShippingMethodEvent"); 

			//Proceed To Checkout code
			eventsList.push("proceedToCheckoutEvent"); 

			//PLP related events
			eventsList.push("filterByEvent"); 
			eventsList.push("sortingFilterEvent");
			eventsList.push("seeMoreEvent");
			eventsList.push("productClickthroughEvent");
			eventsList.push("imageScrollingEvent");
			eventsList.push("findOutMoreEvent");
			eventsList.push("quantityChangeEvent");
			eventsList.push("closeQuickViewOverlayEvent");
			
			//pdp related event
			eventsList.push("bookAnAppointmentEvent");
			eventsList.push("sizeChangeEvent");
			
 			//videoAnalyticsRelatedField
			eventsList.push("videoAnalyticsEvent");
            eventsList.push("clickZoomEvent");
			eventsList.push("videoAnalyticsPlayEvent");

			eventsList.push("tryOnEvent");
			eventsList.push("clickImageEvent");
			eventsList.push("clickSizeGuideEvent");
			eventsList.push("tangibleeAddToCartEvent");
			eventsList.push("clickOnReviewLinkEvent");
			eventsList.push("image360ViewEvent");
			eventsList.push("orderAndPISEvent");
			eventsList.push("emiLinkEvent");
			eventsList.push("techSpecsEvent");
			eventsList.push("productDescriptionEvent");
			eventsList.push("questionPostedEvent");
			eventsList.push("writeReviewEvent");
			eventsList.push("preOrderEvent");	
			eventsList.push("pdpCurrencySelectorEvent");
			eventsList.push("orderAndPickInStorePincodeCheckEvent");
			
			//DigiGold Events
			eventsList.push("digiGoldMenuClicksEvent");
			eventsList.push("digiGoldBuyInEvent");
			eventsList.push("digiGoldProceedToBuyEvent");
			eventsList.push("digiGoldPaymentMethodEvent");
			eventsList.push("digiGoldThankYouPageEvent");
			eventsList.push("digiGoldViewInvoiceEvent");
			eventsList.push("digiGoldCheckHistoryEvent");
			eventsList.push("digiGoldSellInEvent");
			eventsList.push("digiGoldProceedToSellEvent");
			eventsList.push("digiGoldConfirmSellEvent");
			eventsList.push("digiGoldConfirmBuyEvent");

			//MirrAR Events
			eventsList.push("mirrarJewllerySelectedEvent");
			eventsList.push("mirrarImageCaptureEvent");
			eventsList.push("mirrarUIElementClickedEvent");
			eventsList.push("mirrarPopupCloseEvent");
			eventsList.push("mirrarAddToCartEvent");

			//SEO schema specifc events
			eventsList.push("fastrackListFaqSchemaEvent");
			eventsList.push("titanListFaqSchemaEvent");	


			//Error handling event
			eventsList.push("errorEvent");			
			
			//Events and its handlers are dynamically binded based on a pattern

			for(var i = 0; i < eventsList.length; i++){
				var event = eventsList[i];
				/*$( document).on(event, function( e, data){
					if(typeof(e)!= "undefined"){
						//console.log("Event published: " + e.type, data);
						var handler = e.type.replace("Event", "Handler");
						handlerFunction = eval("analyticsJS." + handler);
						try {
							handlerFunction(data);
						}catch(err) {
							console.error("Error with analytics handler: (for event: " + e.type + " )",err);
						}
					}
				});*/
			}

		}
	}
