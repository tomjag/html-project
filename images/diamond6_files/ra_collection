try{if (typeof (adobeDataLayerJS) == "undefined" || !adobeDataLayerJS) {
	adobeDataLayerJS = {
		constants: {
			"plpPage": "product-listing-page",
			"pdpPage": "product-details-page",
			"currencyRate": 70,
			"cartPage": "cart-page",
			"wishlistPage": "wishlist-page",
			"orderhistoryPage": "order-history-page"
		},
		filterApplied: false,
		internalSearch: false,
		adobeDigitalData: {},

		createAdobeDataLayer: function () {
			if (typeof (window.adobeDigitalData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData = window.adobeDigitalData;
			} else {
				adobeDataLayerJS.adobeDigitalData = {};
				window.adobeDigitalData = adobeDataLayerJS.adobeDigitalData;
			}
		},
		homePageHandler: function () {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
		},
		//Internal Search: Track Internal Search on Website
		parseProductViewBySearchTerm: function (productViewData) {
			if (!adobeDataLayerJS.filterApplied) {
				if (typeof (productViewData) != "undefined") {
					adobeDataLayerJS.adobeDigitalData.page = {};
					adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
					adobeDataLayerJS.adobeDigitalData.account = {};
					adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
					adobeDataLayerJS.adobeDigitalData.page.search = {};
					var search = {};
					if (typeof (productViewData.resourceId) != "undefined" && productViewData.resourceId.includes("searchTerm=")) {
						var resourceId = productViewData.resourceId.split("searchTerm=");
						search.term = resourceId[1];
						search.results = productViewData.recordSetTotalMatches;
						adobeDataLayerJS.adobeDigitalData.page.search = search;

						if (productViewData.recordSetTotalMatches > 0) {
							_satellite.track('Internal_Search');
						} else {
							//For Null Search Results
							if (productViewData.nullSearch) {
								_satellite.track('Null_Search');
							} else {
								//Spell check - Auto correction scenario
								search.term = productViewData.correctedSearchTerm;
								adobeDataLayerJS.adobeDigitalData.page.search = search;
								_satellite.track('Internal_Search');
							}
						}
					} else {
						//Search - Landing Page Rule
						search.results = 0;
						localStorage.setItem("internalSearch", true);
						var res = localStorage.getItem('searchTerm');
						search.term = res;
						adobeDataLayerJS.adobeDigitalData.page.search = search;
						adobeDataLayerJS.internalSearch = true;
						_satellite.track('Internal_Search');
					}
				} else {
				}
			} else {
				adobeDataLayerJS.filterApplied = false;
			}
		},
		//Quick View: Track clicks on 'Quick View' button
		parseQuickViewData: function (quickViewData, productViewData) {
			if (typeof (quickViewData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(quickViewData);
				adobeDataLayerJS.adobeDigitalData.product.productInfo.findingMethod = adobeDataLayerJS.constants.plpPage;
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('Quick_View');
				//var res = localStorage.getItem('internalSearch');
				//Internal Search: Clickthroughs - From QuickView
				// if ((window.location.href).includes('/search-results/') || adobeDataLayerJS.internalSearch || res) {
				// 	adobeDataLayerJS.adobeDigitalData.page.search = {};
				// 	if (productViewData.recordSetStartNumber <= productViewData.recordSetCount) {
				// 		for (var i = 0; i < productViewData.catalogEntryView.length; i++) {
				// 			for (var j = 0; j < quickViewData.catalogEntryView.length; j++) {
				// 				if (quickViewData.catalogEntryView[j].skuPartNumber == productViewData.catalogEntryView[i].skuPartNumber) {
