				// 				}
				// 			}
				// 		}
				// 	}
				// 	var search = localStorage.getItem('searchTerm');
				// 	adobeDataLayerJS.adobeDigitalData.page.search.term = search;
				// 	_satellite.track('Search_Clickthrough');
				// }
			}
		},
		parseAddToCartEventResponse: function (addToCartEventData, productViewData) {
			var pageName = "";
			if (adobeDataLayerJS.adobeDigitalData.pageInstanceID != null) {
				var pageInstanceID = adobeDataLayerJS.adobeDigitalData.pageInstanceID;
				pageName = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
			} else {
				var pageInstanceID = window.adobeDigitalData.pageInstanceID;
				pageName = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
			}
			if (pageName == adobeDataLayerJS.constants.plpPage) {
				//Quick View - Add to Cart
				adobeDataLayerJS.parseAddToCartQuickViewEventResponse(addToCartEventData, productViewData);
			} else if (pageName == adobeDataLayerJS.constants.pdpPage) {
				//PDP - Add to Cart
				var isATCTangiblee = localStorage.getItem('atcTangiblee');
				//If invocation is happening from Tangiblee then isATCTangiblee in local storage will have value : true
				//If invocation is happening from PDP ATC then isATCTangiblee in local storage will have value : null
				if (isATCTangiblee == null || !isATCTangiblee) {
					adobeDataLayerJS.parseAddToCartPDPEventResponse(addToCartEventData, productViewData);
				} else {
					localStorage.removeItem("atcTangiblee");
				}
			} else {
				//TODO - Wishlist - Add to Cart
			}

		},
		// Quick View: Add to Cart
		parseAddToCartQuickViewEventResponse: function (addToCartEventData, productViewData) {
			var product = {};
			product.productInfo = {};
			product.productInfo.ID = [];
			product.productInfo.parentID = [];
			product.productInfo.quantity = [];
			product.productInfo.price = [];
			product.productInfo.actualPrice = [];
			product.productInfo.totalPrice = [];
			product.productInfo.category = [];
			product.productInfo.name = [];
			product.productInfo.url = [];
			product.productInfo.currency = [];
			product.productInfo.brand = [];
			product.productInfo.stockStatus = [];
			product.productInfo.weight = [];
			product.productInfo.platform = [];
			product.productInfo.domain = [];
			product.productInfo.actualPriceUSD = [];
			product.productInfo.totalPriceUSD = [];
			product.productInfo.discount = [];
			product.productInfo.discountUSD = [];
			product.productInfo.categoryId = [];
			product.productInfo.wcsid = [];
			product.productInfo.thumbnailUrl = [];

			if (typeof (addToCartEventData) != "undefined") {
				var cart = {};
				cart.source = "Quick View";
				if (productViewData !== null) {
					var catalogEntryView = productViewData.catalogEntryView["0"];
					product.productInfo.ID.push(addToCartEventData.orderItemPartnumber);
					product.productInfo.parentID.push(catalogEntryView.partNumber);
					product.productInfo.wcsid.push(catalogEntryView.catentry_id_child);
					product.productInfo.name.push(addToCartEventData.orderItemName);
					product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(catalogEntryView));
					product.productInfo.url.push(addToCartEventData.productUrl);
					product.productInfo.currency.push(addToCartEventData.currency);
					if (addToCartEventData.currency == "INR") {
						product.productInfo.price.push(parseFloat(addToCartEventData.orderItemOfferPrice));
						product.productInfo.actualPrice.push(parseFloat(addToCartEventData.orderItemListPrice));
						product.productInfo.totalPrice.push(parseFloat(addToCartEventData.orderItemOfferPrice));
						product.productInfo.discount.push(parseFloat(addToCartEventData.savingsAmt));
						for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
							if (catalogEntryView.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
							}
						}
						for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
							if (catalogEntryView.alternativePrice[j].usage === "Offer") {
								product.productInfo.totalPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
							}
						}
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(addToCartEventData.savingsAmt)));
					} else {
						for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
							if (catalogEntryView.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
							}
						}
						for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
							if (catalogEntryView.alternativePrice[j].usage === "Offer") {
								product.productInfo.price.push(parseFloat(catalogEntryView.alternativePrice[j].value));
								product.productInfo.totalPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
							}
						}
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(addToCartEventData.savingsAmt)));
						product.productInfo.actualPriceUSD.push(parseFloat(addToCartEventData.orderItemListPrice));
						product.productInfo.totalPriceUSD.push(parseFloat(addToCartEventData.orderItemListPrice));
						product.productInfo.discountUSD.push(parseFloat(addToCartEventData.savingsAmt));
					}
					var arr = addToCartEventData.productUrl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());

					if (catalogEntryView.oosFlag == "Available") {
						product.productInfo.stockStatus.push(catalogEntryView.oosFlag);
					} else {
						product.productInfo.stockStatus.push(catalogEntryView.oosFlag);
					}

					if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
						if (typeof (catalogEntryView.sKUs[0].attributes) != "undefined") {
							for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
								if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
									product.productInfo.weight.push(catalogEntryView.sKUs[0].attributes[j].values[0].value);
								}

							}
						}
					}

					if (typeof (catalogEntryView.facetAttributes.Product) != "undefined") {
						product.productInfo.category.push(catalogEntryView.facetAttributes.Product);
					}
					else {
						for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
								catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_CATEGORY" ||
								catalogEntryView.techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
								product.productInfo.category.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
					}

					product.productInfo.quantity.push(addToCartEventData.orderItemQuantity);
					product.productInfo.categoryId.push(adobeDataLayerJS.getSectionForPLP(window.location.href));
					product.productInfo.thumbnailUrl.push(catalogEntryView.thumbnail);
					if (window.location.href.includes('/shop/')) {
						product.productInfo.targetCategoryId = adobeDataLayerJS.getSectionForPLP(window.location.href);
					}
					adobeDataLayerJS.adobeDigitalData.product = {};
					adobeDataLayerJS.adobeDigitalData.product = product;
					adobeDataLayerJS.adobeDigitalData.page = {};
					adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
					//adobeDataLayerJS.adobeDigitalData.page.pageInfo.section = adobeDataLayerJS.getSectionForPLP(window.location.href);
					adobeDataLayerJS.adobeDigitalData.account = {};
					adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
					adobeDataLayerJS.adobeDigitalData.page.cart = {};
					adobeDataLayerJS.adobeDigitalData.page.cart = cart;
					_satellite.track('QW_Add_To_Cart');
				}
				else {
					product.productInfo.ID.push(addToCartEventData.orderItemPartnumber);
					product.productInfo.name.push(addToCartEventData.orderItemName);
					product.productInfo.brand.push(adobeDataLayerJS.brand);
					product.productInfo.url.push(addToCartEventData.productUrl);
					product.productInfo.actualPrice.push(parseFloat(addToCartEventData.orderItemListPrice));
					product.productInfo.price.push(parseFloat(addToCartEventData.orderItemOfferPrice));
					product.productInfo.totalPrice.push(parseFloat(addToCartEventData.orderItemOfferPrice));
					product.productInfo.discount.push(parseFloat(addToCartEventData.savingsAmt));
					var arr = addToCartEventData.productUrl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					product.productInfo.quantity.push(addToCartEventData.orderItemQuantity);
					product.productInfo.categoryId.push(adobeDataLayerJS.getSectionForPLP(window.location.href));
					adobeDataLayerJS.adobeDigitalData.product = {};
					adobeDataLayerJS.adobeDigitalData.product = product;
					adobeDataLayerJS.adobeDigitalData.page = {};
					adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
					//adobeDataLayerJS.adobeDigitalData.page.pageInfo.section = adobeDataLayerJS.getSectionForPLP(window.location.href);
					adobeDataLayerJS.adobeDigitalData.account = {};
					adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
					adobeDataLayerJS.adobeDigitalData.page.cart = {};
					adobeDataLayerJS.adobeDigitalData.page.cart = cart;
					_satellite.track('QW_Add_To_Cart');
				}
			}
		},
		// Product Details Page: Cart Add
		parseAddToCartPDPEventResponse: function (addToCartEventData, productViewData) {
				var product = {};
				product.productInfo = {};
				product.productInfo.ID = []; //
				product.productInfo.parentID = [];
				product.productInfo.quantity = []; //
				product.productInfo.price = [];//
				product.productInfo.actualPrice = [];//
				product.productInfo.totalPrice = [];//
				product.productInfo.category = [];//
				product.productInfo.name = [];//
				product.productInfo.url = [];//
				product.productInfo.currency = [];//
				product.productInfo.brand = [];//
				product.productInfo.stockStatus = [];//
				product.productInfo.weight = [];//
				product.productInfo.platform = [];//
				product.productInfo.domain = [];//
				product.productInfo.actualPriceUSD = []; //
				product.productInfo.totalPriceUSD = []; //
				product.productInfo.discount = [];//
				product.productInfo.discountUSD = [];
				product.productInfo.categoryId = [];
				product.productInfo.wcsid = [];

				if (typeof (addToCartEventData) != "undefined") {
					adobeDataLayerJS.adobeDigitalData.page = {};
					adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
					var cart = {};
					cart.source = "PDP";
					if (productViewData !== null) {
						var catalogEntryView = productViewData.catalogEntryView["0"];
						product.productInfo.ID.push(addToCartEventData.orderItemPartnumber);
						product.productInfo.parentID.push(catalogEntryView.partNumber);
						if (catalogEntryView.sKUs[0].inventory != "undefined") {
							product.productInfo.wcsid.push(catalogEntryView.sKUs[0].inventory.productId);
						} else {
							product.productInfo.wcsid.push(catalogEntryView.catentry_id_child);
						}
						product.productInfo.name.push(addToCartEventData.orderItemName);
						product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(catalogEntryView));
						product.productInfo.url.push(addToCartEventData.productUrl);
						product.productInfo.currency.push(addToCartEventData.currency);
						if (addToCartEventData.currency == "INR") {
							product.productInfo.price.push(parseFloat(addToCartEventData.orderItemOfferPrice));
							product.productInfo.actualPrice.push(parseFloat(addToCartEventData.orderItemListPrice));
							product.productInfo.totalPrice.push(parseFloat(addToCartEventData.orderItemOfferPrice));
							product.productInfo.discount.push(parseFloat(addToCartEventData.savingsAmt));
							for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
								if (catalogEntryView.alternativePrice[j].usage === "Display") {
									product.productInfo.actualPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
								}
							}
							for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
								if (catalogEntryView.alternativePrice[j].usage === "Offer") {
									product.productInfo.totalPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
								}
							}
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(addToCartEventData.savingsAmt)));
						} else {
							for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
								if (catalogEntryView.alternativePrice[j].usage === "Display") {
									product.productInfo.actualPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
								}
							}
							for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
								if (catalogEntryView.alternativePrice[j].usage === "Offer") {
									product.productInfo.price.push(parseFloat(catalogEntryView.alternativePrice[j].value));
									product.productInfo.totalPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
								}
							}
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(addToCartEventData.savingsAmt)));
							product.productInfo.actualPriceUSD.push(parseFloat(addToCartEventData.orderItemListPrice));
							product.productInfo.totalPriceUSD.push(parseFloat(addToCartEventData.orderItemListPrice));
							product.productInfo.discountUSD.push(parseFloat(addToCartEventData.savingsAmt));
						}
						var arr = addToCartEventData.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
						if (catalogEntryView.oosFlag == "Available") {
							product.productInfo.stockStatus.push(catalogEntryView.oosFlag);
						} else {
							product.productInfo.stockStatus.push(catalogEntryView.oosFlag);
						}


						if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
							if (typeof catalogEntryView.sKUs[0].attributes != "undefined") {
								for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
									if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
										product.productInfo.weight.push(catalogEntryView.sKUs[0].attributes[j].values[0].value);
									}

								}
							}
						}


						if (typeof (catalogEntryView.facetAttributes.Product) != "undefined") {
							product.productInfo.category.push(catalogEntryView.facetAttributes.Product);
							adobeDataLayerJS.adobeDigitalData.page.pageInfo.section = catalogEntryView.facetAttributes.Product.toLowerCase();
							product.productInfo.categoryId.push(catalogEntryView.facetAttributes.Product.toLowerCase());
						}
						else {
							for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
								if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
									catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_CATEGORY" ||
									catalogEntryView.techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
									product.productInfo.category.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
									adobeDataLayerJS.adobeDigitalData.page.pageInfo.section = catalogEntryView.techSpecsAttributes[j].values[0].value.toLowerCase();
									product.productInfo.categoryId.push(catalogEntryView.techSpecsAttributes[j].values[0].value.toLowerCase());
								}
							}
						}
						if (document.referrer.includes('/shop/')) {
							product.productInfo.targetCategoryId = adobeDataLayerJS.getSectionForPLP(document.referrer);
						}
						product.productInfo.quantity.push(addToCartEventData.orderItemQuantity);
						adobeDataLayerJS.targetAttributesForSequencing(catalogEntryView,product);
						adobeDataLayerJS.adobeDigitalData.product = {};
						adobeDataLayerJS.adobeDigitalData.product = product;
						adobeDataLayerJS.adobeDigitalData.page.cart = {};
						adobeDataLayerJS.adobeDigitalData.page.cart = cart;
						adobeDataLayerJS.adobeDigitalData.account = {};
						adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
						_satellite.track('PDP_Cart_Addition_Basket');
					}
					else {
						product.productInfo.ID.push(addToCartEventData.orderItemPartnumber);
						product.productInfo.name.push(addToCartEventData.orderItemName);
						product.productInfo.brand.push(adobeDataLayerJS.brand);
						product.productInfo.url.push(addToCartEventData.productUrl);
						product.productInfo.actualPrice.push(parseFloat(addToCartEventData.orderItemListPrice));
						product.productInfo.price.push(parseFloat(addToCartEventData.orderItemOfferPrice));
						product.productInfo.totalPrice.push(parseFloat(addToCartEventData.orderItemOfferPrice));
						product.productInfo.discount.push(parseFloat(addToCartEventData.savingsAmt));
						var arr = addToCartEventData.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
						product.productInfo.quantity.push(addToCartEventData.orderItemQuantity);
						adobeDataLayerJS.adobeDigitalData.product = {};
						adobeDataLayerJS.adobeDigitalData.page.cart = {};
						adobeDataLayerJS.adobeDigitalData.page.cart = cart;
						adobeDataLayerJS.adobeDigitalData.product = product;
						adobeDataLayerJS.adobeDigitalData.account = {};
						adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
						_satellite.track('PDP_Cart_Addition_Basket');
					}
				}
		},
		// Product Details Page: Track load of the PDP page
		parseProductViewByIdData: function (productViewData) {
			if (typeof (productViewData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Page_Load');
			}
		},
		// Product Details Page: Add to Wishlist
		addToWishlistEventResponse: function (addToWishListEventData, productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('PDP_Add_To_Wishlist');
		},
		// Product Details Page: View Cart from overlay button click (Minicart)
		parseMiniCartViewEventResponse: function (cartViewEventData, productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('PDP_Cart_Overlay');
		},
		// Product Details Page: Pin code check
		servicabilityCheckEventResponse: function (servicabilityCheckEventData, productViewData, cartViewEventData) {
			var pageName = "";
			if (adobeDataLayerJS.adobeDigitalData.pageInstanceID != null) {
				var pageInstanceID = adobeDataLayerJS.adobeDigitalData.pageInstanceID;
				pageName = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
			} else {
				var pageInstanceID = window.adobeDigitalData.pageInstanceID;
				pageName = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
			}

			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.page.pin = {};

			if (pageName == adobeDataLayerJS.constants.cartPage) {
				//Cart - Pin Code Check
				if (typeof (cartViewEventData !== "undefined")) {
					var product = adobeDataLayerJS.initializeProductObject();
					for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
						product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
						product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
						product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
						product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
						product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);						
						//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
						product.productInfo.quantity.push(parseFloat(cartViewEventData.orderItem[j].quantity));
						product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
						var price = 0;
						if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
							price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
						} else {
							price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
						}
						if (cartViewEventData.orderItem[j].currency == "INR") {
							product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.actualPrice.push(price);
							product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
							product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
							product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						} else {
							product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
							product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPriceUSD.push(price);
							product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						}
						product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
						product.productInfo.brand.push(cartViewEventData.orderItem[j].productDetail.motherBrand);
						var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					}
					adobeDataLayerJS.adobeDigitalData.product = product;
				}

			} else if (pageName == adobeDataLayerJS.constants.pdpPage) {
				//PDP - Pin Code Check
				if (typeof (servicabilityCheckEventData.selected !== "undefined")) {
					var product = adobeDataLayerJS.initializeProductObject();
					product.productInfo.ID.push(servicabilityCheckEventData.selected.skuPartNumber);
					product.productInfo.wcsid.push(servicabilityCheckEventData.selected.skuCatentryId);
					product.productInfo.stockStatus.push(servicabilityCheckEventData.selected.inventory.inventoryStatus);
					product.productInfo.quantity.push(parseFloat(servicabilityCheckEventData.selected.inventory.availableQuantity));
					product.productInfo.currency.push(servicabilityCheckEventData.selected.price[0].currency);
					if (servicabilityCheckEventData.selected.price[0].currency == "INR") {
						for (var j = 0; j < servicabilityCheckEventData.selected.price.length; j++) {
							if (servicabilityCheckEventData.selected.price[j].usage === "Display") {
								product.productInfo.actualPrice.push(parseFloat(servicabilityCheckEventData.selected.price[j].value));
							}
						}
						for (var j = 0; j < servicabilityCheckEventData.selected.price.length; j++) {
							if (servicabilityCheckEventData.selected.price[j].usage === "Offer") {
								product.productInfo.price.push(parseFloat(servicabilityCheckEventData.selected.price[j].value));
								product.productInfo.totalPrice.push(parseFloat(servicabilityCheckEventData.selected.price[j].value));
							}
						}
						product.productInfo.discount.push(parseFloat(servicabilityCheckEventData.selected.amountSaved.amountSaved));
						for (var j = 0; j < servicabilityCheckEventData.selected.alternativePrice.length; j++) {
							if (servicabilityCheckEventData.selected.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPriceUSD.push(parseFloat(servicabilityCheckEventData.selected.alternativePrice[j].value));
							}
						}
						for (var j = 0; j < servicabilityCheckEventData.selected.alternativePrice.length; j++) {
							if (servicabilityCheckEventData.selected.alternativePrice[j].usage === "Offer") {
								product.productInfo.totalPriceUSD.push(parseFloat(servicabilityCheckEventData.selected.alternativePrice[j].value));
							}
						}
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(servicabilityCheckEventData.selected.amountSaved.amountSaved)));
					} else {
						for (var j = 0; j < servicabilityCheckEventData.selected.price.length; j++) {
							if (servicabilityCheckEventData.selected.price[j].usage === "Display") {
								product.productInfo.actualPriceUSD.push(parseFloat(servicabilityCheckEventData.selected.price[j].value));
							}
						}
						for (var j = 0; j < servicabilityCheckEventData.selected.price.length; j++) {
							if (servicabilityCheckEventData.selected.price[j].usage === "Offer") {
								product.productInfo.totalPriceUSD.push(parseFloat(servicabilityCheckEventData.selected.price[j].value));
							}
						}
						product.productInfo.discountUSD.push(parseFloat(servicabilityCheckEventData.selected.amountSaved.amountSaved));
						for (var j = 0; j < servicabilityCheckEventData.selected.alternativePrice.length; j++) {
							if (servicabilityCheckEventData.selected.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPrice.push(parseFloat(servicabilityCheckEventData.selected.alternativePrice[j].value));

							}
						}
						for (var j = 0; j < servicabilityCheckEventData.selected.alternativePrice.length; j++) {
							if (servicabilityCheckEventData.selected.alternativePrice[j].usage === "Offer") {
								product.productInfo.price.push(parseFloat(servicabilityCheckEventData.selected.alternativePrice[j].value));
								product.productInfo.totalPrice.push(parseFloat(servicabilityCheckEventData.selected.alternativePrice[j].value));

							}
						}
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(servicabilityCheckEventData.selected.amountSaved.amountSaved)));
					}

					product.productInfo.url.push(servicabilityCheckEventData.producturl);
					product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(productViewData.catalogEntryView[0]));
					adobeDataLayerJS.targetAttributesForSequencing(productViewData.catalogEntryView[0],product);
					var arr = servicabilityCheckEventData.producturl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					if (productViewData.catalogEntryView[0].txBrand == "Tanishq" || productViewData.catalogEntryView[0].txBrand == "Mia") {
						if (servicabilityCheckEventData.selected.attributes != "undefined" && servicabilityCheckEventData.selected.attributes[0].name == "Gross Weight") {
							product.productInfo.weight.push(servicabilityCheckEventData.selected.attributes[0].values[0].value);
						}
					}
					product.productInfo.parentID.push(productViewData.catalogEntryView[0].partNumber);
					product.productInfo.name.push(productViewData.catalogEntryView[0].name);
					if (typeof (productViewData.catalogEntryView[0].facetAttributes.Product) != "undefined") {
						product.productInfo.category.push(productViewData.catalogEntryView[0].facetAttributes.Product);
					}
					else {
						for (var j = 0; j < productViewData.catalogEntryView[0].techSpecsAttributes.length; j++) {
							if (productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
								productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "JW_TX_CATEGORY" ||
								productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
								product.productInfo.category.push(productViewData.catalogEntryView[0].techSpecsAttributes[j].values[0].value);
							}
						}
					}
					adobeDataLayerJS.adobeDigitalData.product = product;
				}
			}
			if (typeof (servicabilityCheckEventData) != "undefined") {
				if (typeof (servicabilityCheckEventData.errMsg) == "undefined") {
					if (servicabilityCheckEventData.select.zipCode != null) {
						adobeDataLayerJS.adobeDigitalData.page.pin.code = servicabilityCheckEventData.select.zipCode;
					} else {
						adobeDataLayerJS.adobeDigitalData.page.pin.code = servicabilityCheckEventData.select.pincode;
					}
					adobeDataLayerJS.adobeDigitalData.page.pin.country = servicabilityCheckEventData.select.countryName;
					var deliveryOption = "";
					if (servicabilityCheckEventData.cod == "Available") {
						deliveryOption = "COD";
					}
					for (var j = 0; j < servicabilityCheckEventData.deliveryInfo.length; j++) {
						if (servicabilityCheckEventData.deliveryInfo[j].available) {
							deliveryOption = deliveryOption + "|" + servicabilityCheckEventData.deliveryInfo[0].deliveryMethodName;
						}
					}
					adobeDataLayerJS.adobeDigitalData.page.pin.option = deliveryOption;
					adobeDataLayerJS.adobeDigitalData.page.pin.result = "Success";
				} else {
					adobeDataLayerJS.adobeDigitalData.page.pin.country = servicabilityCheckEventData.select.countryName;
					if (servicabilityCheckEventData.select.zipCode != null) {
						adobeDataLayerJS.adobeDigitalData.page.pin.code = servicabilityCheckEventData.select.zipCode;
					} else {
						adobeDataLayerJS.adobeDigitalData.page.pin.code = servicabilityCheckEventData.select.pincode;
					}

					adobeDataLayerJS.adobeDigitalData.page.pin.result = "Failure";
				}
				if (pageName == adobeDataLayerJS.constants.cartPage) {
					//Cart - Pin Code Check
					_satellite.track('Cart_Pin_Code_Check');

				} else if (pageName == adobeDataLayerJS.constants.pdpPage) {
					//PDP - Pin Code Check
					_satellite.track('PDP_Pin_Code_Check');
				}
			}
		},
		// Product Details Page: Book an Appointment
		bookAppointmentResponse: function (productViewData) {
			if (typeof (productViewData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Book_Appointment');
			}
		},
		// Product Listing Page: See more products
		seeMoreEventResponse: function (seeMoreEvent, productViewData) {
			if (typeof (seeMoreEvent) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PLP_See_More_Products');
			}
		},
		//  Cart Page: Track load of the Cart page
		parseCartViewEventResponse: function (cartViewEventData, productViewData) {
			if (typeof (cartViewEventData !== "undefined")) {
				var product = adobeDataLayerJS.initializeProductObject();
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.page.cart = {};
				adobeDataLayerJS.adobeDigitalData.page.cart.entry = "";
				adobeDataLayerJS.adobeDigitalData.page.cart.merge = "";
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				if (typeof (cartViewEventData.orderItem) !== "undefined") {
					for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
						product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
						product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
						product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
						product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
						product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);
						//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
						product.productInfo.quantity.push(parseFloat(cartViewEventData.orderItem[j].quantity));
						var price = 0;
						if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
							price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
						} else {
							price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
						}
						if (cartViewEventData.orderItem[j].currency == "INR") {
							product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.actualPrice.push(price);
							product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
							product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
							product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						} else {
							product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
							product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPriceUSD.push(price);
							product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						}
						product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
						product.productInfo.brand.push(cartViewEventData.orderItem[j].techSpecsAttributes.WT_TX_DISPLAY_BRAND);
						adobeDataLayerJS.targetAttributesForSequencing(cartViewEventData.orderItem[j],product);
						product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
						var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
						if (adobeDataLayerJS.adobeDigitalData.page.category.websiteIdentifier=="tq-website"||adobeDataLayerJS.adobeDigitalData.page.category.websiteIdentifier=="tq-m-rwd"){
							product.productInfo.category.push(cartViewEventData.orderItem[j].techSpecsAttributes.JW_TX_PRODUCT);
						} else {
							product.productInfo.category.push(cartViewEventData.orderItem[j].techSpecsAttributes.WT_TX_PRODUCT_CATEGORY);
						}
					}
				}
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = product;
				_satellite.track('Cart_Page_Load');
			}
		},
		//"Move To Wishlist" event
		moveToWishlistEventResponse: function (moveToWishlistEventData, productViewData) {
			if (typeof (moveToWishlistEventData) != "undefined") {
				var product = adobeDataLayerJS.initializeProductObject();
				product.productInfo.ID.push(moveToWishlistEventData.orderItem.partNumber);
				product.productInfo.parentID.push(moveToWishlistEventData.orderItem.productDetail.parentPartNumber);
				product.productInfo.wcsid.push(moveToWishlistEventData.orderItem.productId);
				product.productInfo.name.push(moveToWishlistEventData.orderItem.productDetail.itemName);
				product.productInfo.url.push(moveToWishlistEventData.orderItem.productDetail.productUrl);
				product.productInfo.brand.push(moveToWishlistEventData.orderItem.productDetail.motherBrand);				
				//product.productInfo.category.push(adobeDataLayerJS.getCategory(moveToWishlistEventData.orderItem.productDetail.productUrl));
				var arr = moveToWishlistEventData.orderItem.productDetail.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.currency.push(moveToWishlistEventData.orderItem.currency);

				var price = 0;
				if (moveToWishlistEventData.orderItem.productDetail.itemStrikeOffPrice != null) {
					price = parseFloat(moveToWishlistEventData.orderItem.productDetail.itemStrikeOffPrice);
				} else {
					price = parseFloat(moveToWishlistEventData.orderItem.unitPrice);
				}
				if (moveToWishlistEventData.orderItem.currency == "INR") {
					product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem)));
					product.productInfo.actualPrice.push(price);
					product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem)));
					product.productInfo.discount.push(parseFloat(moveToWishlistEventData.orderItem.productDetail.savingsAmt));
					product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
					product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem))));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(moveToWishlistEventData.orderItem.productDetail.savingsAmt)));
				} else {
					product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem))));
					product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
					product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem))));
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(moveToWishlistEventData.orderItem.productDetail.savingsAmt)));
					product.productInfo.actualPriceUSD.push(price);
					product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(moveToWishlistEventData.orderItem)));
					product.productInfo.discountUSD.push(parseFloat(moveToWishlistEventData.orderItem.productDetail.savingsAmt));
				}

				if (moveToWishlistEventData.orderItem.orderItemInventoryStatus == "Available") {
					product.productInfo.stockStatus.push(moveToWishlistEventData.orderItem.orderItemInventoryStatus);
				} else {
					product.productInfo.stockStatus.push(moveToWishlistEventData.orderItem.orderItemInventoryStatus);
				}
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				product.productInfo.quantity.push(parseFloat(moveToWishlistEventData.orderItem.quantity));

				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = product;
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('Cart_Add_To_Wishlist');
			}
		},
		// User Login: Track when user Signs In
		userSignInEventResponse: function (userSignInEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			window.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account.info = {};
			window.adobeDigitalData.account.info = {};
			adobeDataLayerJS.adobeDigitalData.account.signIn = {};
			window.adobeDigitalData.account.signIn = {};
			if (typeof (userSignInEventData !== "undefined")) {
				if (userSignInEventData.result == "success") {
					adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(userSignInEventData.userDetails.email);
					adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(userSignInEventData.userDetails.mobilePhone1);
					adobeDataLayerJS.adobeDigitalData.account.info.encirclePointBalance = userSignInEventData.userDetails.encirclePointBalance;
					adobeDataLayerJS.adobeDigitalData.account.info.loginStatus = "Logged In";
					if (typeof (userSignInEventData.social !== "undefined")) {
						if (userSignInEventData.login_type == "Social") {
							adobeDataLayerJS.adobeDigitalData.account.signIn.type = userSignInEventData.social.source;
						} else {
							adobeDataLayerJS.adobeDigitalData.account.signIn.type = userSignInEventData.login_type;
						}
					}
					if (!userSignInEventData.signUp) {
						_satellite.track('Sign_In');
					} else {
						adobeDataLayerJS.adobeDigitalData.account.signUp = {};
						if (typeof (userSignInEventData.social !== "undefined")) {
							if (userSignInEventData.login_type == "Social") {
								adobeDataLayerJS.adobeDigitalData.account.signUp.type = userSignInEventData.social.source;
							} else {
								adobeDataLayerJS.adobeDigitalData.account.signIn.type = userSignInEventData.login_type;
							}
						}
						_satellite.track('Sign_Up');
						_satellite.track('Sign_In');
					}
				} else {
					if (isNaN(userSignInEventData.userLogId)) {
						adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(userSignInEventData.userLogId);
					} else {
						if (userSignInEventData.userLogId.length == 10) {
							adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(userSignInEventData.userLogId);
						}
					}
					adobeDataLayerJS.adobeDigitalData.account.info.loginStatus = "Anonymous";
					if (typeof (userSignInEventData.social !== "undefined")) {
						if (userSignInEventData.login_type == "Social") {
							adobeDataLayerJS.adobeDigitalData.account.signIn.type = userSignInEventData.social.source;
						} else {
							adobeDataLayerJS.adobeDigitalData.account.signIn.type = userSignInEventData.login_type;
						}
					}
					if (!userSignInEventData.signUp) {
					} else {
						adobeDataLayerJS.adobeDigitalData.account.signUp = {};
						if (typeof (userSignInEventData.social !== "undefined")) {
							if (userSignInEventData.login_type == "Social") {
								adobeDataLayerJS.adobeDigitalData.account.signUp.type = userSignInEventData.social.source;
							} else {
								adobeDataLayerJS.adobeDigitalData.account.signUp.type = userSignInEventData.login_type;
							}
						}
					}
					_satellite.track('custid_test');
				}
			}
		},
		// User Login: Track when user Signs Up
		userRegistrationEventResponse: function (userRegistrationEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			if (adobeDataLayerJS.adobeDigitalData.page.pageInfo.pageName.includes('checkout')) {
				adobeDataLayerJS.adobeDigitalData.page.pageInfo.pageName = adobeDataLayerJS.brandCode + "-" + "confirmation-page";
				adobeDataLayerJS.adobeDigitalData.page.pageInfo.pageType = "confirmation-page";
				adobeDataLayerJS.adobeDigitalData.page.pageInfo.section = adobeDataLayerJS.adobeDigitalData.page.pageInfo.pageType;
			}
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account.info = {};
			if (userRegistrationEventData.result == "success") {
				adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(userRegistrationEventData.address.email1);
				adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(userRegistrationEventData.address.mobilePhone1);
				adobeDataLayerJS.adobeDigitalData.account.info.encirclePointBalance = userRegistrationEventData.encirclePointBalance;
				adobeDataLayerJS.adobeDigitalData.account.info.loginStatus = "Logged In";
				adobeDataLayerJS.adobeDigitalData.account.signIn = {};
				adobeDataLayerJS.adobeDigitalData.account.signUp = {};
				adobeDataLayerJS.adobeDigitalData.account.signIn.type = "emailpassword";
				adobeDataLayerJS.adobeDigitalData.account.signUp.type = "emailpassword";
				_satellite.track('Sign_Up');
				_satellite.track('Sign_In');
			} else {
				if (isNaN(userRegistrationEventData.userLogId)) {
					adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(userRegistrationEventData.userLogId);
				} else {
					if (userRegistrationEventData.userLogId.length == 10) {
						adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(userRegistrationEventData.userLogId);
					}
				}
				adobeDataLayerJS.adobeDigitalData.account.info.loginStatus = "Anonymous";
				adobeDataLayerJS.adobeDigitalData.account.signIn = {};
				adobeDataLayerJS.adobeDigitalData.account.signUp = {};
				adobeDataLayerJS.adobeDigitalData.account.signIn.type = "emailpassword";
				adobeDataLayerJS.adobeDigitalData.account.signUp.type = "emailpassword";
			}
		},
		//Filter Tracking: Track Filters on Search, Product Listing Page
		filterByEventResponse: function (filterByEvent) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.page.filter = {};
			var filter = {};
			if (typeof (filterByEvent) != "undefined") {
				var list = "";
				var typeList = Object.keys(filterByEvent.type);
				var typeArray = typeList.toString().split(",");
				list = typeArray[typeArray.length - 1];
				if (filterByEvent.value == "Price" || filterByEvent.value == "OfferPrice_INR" || filterByEvent.value == "OfferPrice_USD") {
					var res = list.substring(list.lastIndexOf("{") + 1, list.indexOf("}"));
					var array = res.split(' ');
					var filterSel = "";
					if (array[0] == "*") {
						filterSel = "Less than " + array[1];
					} else if (array[1] == "*") {
						filterSel = "More than " + array[0];
					} else {
						filterSel = "Between " + array[0] + " and " + array[1];
					}
					if (filterByEvent.value == "OfferPrice_USD") {
						filterSel = filterSel.replace("", "$");
					}
					filter.list = filterSel;
					filter.value = "Price";
				} else {
					filter.list = list;
					filter.value = filterByEvent.value;
				}

				filter.category = adobeDataLayerJS.getSectionForPLP(window.location.href);
				adobeDataLayerJS.filterApplied = true;
				adobeDataLayerJS.adobeDigitalData.page.filter = filter;
				var filterDeselected = false;
				for (var j = 0; j < typeArray.length; j++) {
					if (!filterByEvent.type[typeArray[j]]) {
						filterDeselected = true;
					}
				}
				if (!filterDeselected) {
					_satellite.track('Filter_Selection');
				}
			}
		},
		// Thank you page: Track Thank you page load
		orderPlacedEventResponse: function (orderSummaryEventData) {
			var order = {};
			order.payment = {};
			order.revenue = {};
			order.info = {};

			if (typeof (orderSummaryEventData) != "undefined" && typeof (orderSummaryEventData.orderSummary) != "undefined" && orderSummaryEventData.orderSummary.length > 0) {
				order.info.ID = orderSummaryEventData.orderId;
				var paymentInfo = JSON.parse(localStorage.getItem('usablePaymentInfoEvent'));
				order.payment.method = paymentInfo.paymentOption;
				if (parseFloat(paymentInfo.redeemAmount) > 0) {
					order.payment.giftPay = parseFloat(paymentInfo.redeemAmount)
				} else {
					order.payment.giftPay = 0;
				}
				if (paymentInfo.product.productInfo.currency[0] == "USD") {
					order.revenue.total = adobeDataLayerJS.convertorINR(parseFloat(orderSummaryEventData.totalBreakDown.total));
					order.revenue.totalUSD = parseFloat(orderSummaryEventData.totalBreakDown.total);
					order.revenue.shippingCharge = adobeDataLayerJS.convertorINR(parseFloat(paymentInfo.shipping));
					//order.revenue.shippingChargeUSD = paymentInfo.shipping;
				} else {
					order.revenue.total = parseFloat(orderSummaryEventData.totalBreakDown.total);
					order.revenue.totalUSD = adobeDataLayerJS.convertorUSD(parseFloat(orderSummaryEventData.totalBreakDown.total));
					order.revenue.shippingCharge = parseFloat(paymentInfo.shipping);
					//order.revenue.shippingChargeUSD = adobeDataLayerJS.convertorUSD(parseFloat(paymentInfo.shipping));
				}
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = paymentInfo.product;
			}
			adobeDataLayerJS.adobeDigitalData.order = {};
			adobeDataLayerJS.adobeDigitalData.order = order;
			adobeDataLayerJS.adobeDigitalData.page = {};
			var page = {};
			page.pageInfo = {};
			page.category = {};
			page.pageInfo.section = "";
			page.pageInfo.pageType = "";
			page.pageInfo.prodCatL0 = "";

			page.pageInfo.pageName = adobeDataLayerJS.brandCode + "-" + "confirmation-page";
			page.pageInfo.pageType = page.pageInfo.pageName.slice(page.pageInfo.pageName.indexOf("-") + 1, page.pageInfo.pageName.length);
			page.pageInfo.section = page.pageInfo.pageType;
			page.category.primaryCategory = page.pageInfo.pageType;
			page.category.websiteIdentifier = adobeDataLayerJS.brandCode + "-" + adobeDataLayerJS.deviceCheck().toLowerCase();
			page.category.platform = adobeDataLayerJS.deviceCheck().toLowerCase();
			adobeDataLayerJS.adobeDigitalData.page = page;
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			
				if (adobeDataLayerJS.adobeDigitalData.account.info.loginStatus == "Anonymous") {
					adobeDataLayerJS.adobeDigitalData.order.ordertype="Guest Order";
				}
				else{
					adobeDataLayerJS.adobeDigitalData.order.ordertype="Logged-In Order";
				}
			_satellite.track('Thank_You_Page_Load');

		},
		//Cart Page: Enter Promo Code
		applyPromoCodeEventResponse: function (applyPromoCodeEventData, cartViewEventData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			var page = adobeDataLayerJS.setPageObject();
			page.promo = {};
			page.cart = {};
			if (typeof (applyPromoCodeEventData.promoParams) !== "undefined") {
				if (applyPromoCodeEventData.promoParams.result == "Success") {
					page.promo.result = "Success";
				} else {
					page.promo.result = "Failure";
				}
				page.cart.promo = applyPromoCodeEventData.promoParams.code;
			}
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = page;
			if (typeof (cartViewEventData !== "undefined")) {
				var product = adobeDataLayerJS.initializeProductObject();
				product.productInfo.quantity.push(parseFloat(cartViewEventData.totalQtyCount));
				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
					product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
					product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
					product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
					product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);
					//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
					var price = 0;
					if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
						price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
					} else {
						price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
					}
					if (cartViewEventData.orderItem[j].currency == "INR") {
						product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.actualPrice.push(price);
						product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
						product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
					} else {
						product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
						product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						product.productInfo.actualPriceUSD.push(price);
						product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
					}
					product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
					product.productInfo.brand.push(cartViewEventData.orderItem[j].productDetail.motherBrand);
					product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
					product.productInfo.weight.push(cartViewEventData.orderItem[j]['Gross Weight']);
					var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
			}
			_satellite.track('Cart_Promo_Code_Apply');
		},
		//Cart Page: Proceed to Checkout
		proceedToCheckoutEventResponse: function (proceedToCheckoutEventData, cartViewEventData) {

			if (typeof (cartViewEventData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				var product = adobeDataLayerJS.initializeProductObject();
				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
					product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
					product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
					product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
					product.productInfo.quantity.push(parseFloat(cartViewEventData.orderItem[j].quantity));
					product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);

					if (adobeDataLayerJS.adobeDigitalData.page.category.websiteIdentifier=="tq-website"||adobeDataLayerJS.adobeDigitalData.page.category.websiteIdentifier=="tq-m-rwd"){
						product.productInfo.category.push(cartViewEventData.orderItem[j].techSpecsAttributes.JW_TX_PRODUCT);
					} else {
						product.productInfo.category.push(cartViewEventData.orderItem[j].techSpecsAttributes.WT_TX_PRODUCT_CATEGORY);
					}
					var price = 0;
					if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
						price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
					} else {
						price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
					}
					if (cartViewEventData.orderItem[j].currency == "INR") {
						product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.actualPrice.push(price);
						product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
						product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
					} else {
						product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
						product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						product.productInfo.actualPriceUSD.push(price);
						product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
						product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
					}
					product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
					product.productInfo.brand.push(cartViewEventData.orderItem[j].techSpecsAttributes.WT_TX_DISPLAY_BRAND);
					adobeDataLayerJS.targetAttributesForSequencing(cartViewEventData.orderItem[j],product);
					product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
					var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
				localStorage.setItem("cartProduct", JSON.stringify(product));
			}
			_satellite.track('Cart_Proceed_To_Checkout');
		},
		initializeProductObject: function () {
			var product = {};
			product.productInfo = {};
			product.productInfo.ID = [];
			product.productInfo.parentID = [];
			product.productInfo.quantity = [];
			product.productInfo.price = [];
			product.productInfo.actualPrice = [];
			product.productInfo.totalPrice = [];
			product.productInfo.category = [];
			product.productInfo.name = [];
			product.productInfo.url = [];
			product.productInfo.currency = [];
			product.productInfo.brand = [];
			product.productInfo.stockStatus = [];
			product.productInfo.weight = [];
			product.productInfo.emi = [];
			product.productInfo.platform = [];
			product.productInfo.domain = [];
			product.productInfo.actualPriceUSD = [];
			product.productInfo.totalPriceUSD = [];
			product.productInfo.discount = [];
			product.productInfo.discountUSD = [];
			product.productInfo.wcsid = [];

			return product;
		},
		//Common method to construct Product Object
		constructProductObject: function (productViewData) {
			var product = {};
			product.productInfo = {};
			product.productInfo.ID = [];
			product.productInfo.parentID = [];
			product.productInfo.quantity = [];
			product.productInfo.price = [];
			product.productInfo.actualPrice = [];
			product.productInfo.totalPrice = [];
			product.productInfo.category = [];
			product.productInfo.name = [];
			product.productInfo.url = [];
			product.productInfo.currency = [];
			product.productInfo.brand = [];
			product.productInfo.stockStatus = [];
			product.productInfo.emi = [];
			product.productInfo.platform = [];
			product.productInfo.domain = [];
			product.productInfo.actualPriceUSD = [];
			product.productInfo.totalPriceUSD = [];
			product.productInfo.discount = [];
			product.productInfo.discountUSD = [];
			product.productInfo.wcsid = [];					
			if (typeof (productViewData) != "undefined") {
				var catalogEntryView = productViewData.catalogEntryView["0"];
				product.productInfo.url.push(catalogEntryView.productUrl.split('?')[0]);
				product.productInfo.name.push(catalogEntryView.name);
				if (typeof (productViewData.selected) != "undefined") {
					product.selectedSKU = productViewData.selected;
				}
				product.productInfo.parentID.push(catalogEntryView.partNumber);
				product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(catalogEntryView));
				if (document.referrer.includes('/shop/')) {
					product.productInfo.targetCategoryId = adobeDataLayerJS.getSectionForPLP(document.referrer)
				}
				if (catalogEntryView.oosFlag == "Available") {
					product.productInfo.quantity.push(1);
				} else {
					product.productInfo.quantity.push(0);
				}
				//If currency is INR/USD
				var currencyVal = "";
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					currencyVal = catalogEntryView.price[j].currency;
				}
				product.productInfo.currency.push(currencyVal);
				if (typeof catalogEntryView.alternativePrice != 'undefined' && typeof catalogEntryView.price != 'undefined') {
					adobeDataLayerJS.setCurrencyRate(catalogEntryView, currencyVal);
				}
				adobeDataLayerJS.getProductPriceDetails(catalogEntryView, currencyVal, product);
				/*if (currencyVal == "INR") {
					//When currency selector is INR
					for (var j = 0; j < catalogEntryView.price.length; j++) {
						if (catalogEntryView.price[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(catalogEntryView.price[j].value));
						}
					}
					for (var j = 0; j < catalogEntryView.price.length; j++) {
						if (catalogEntryView.price[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(catalogEntryView.price[j].value));
							product.productInfo.totalPrice.push(parseFloat(catalogEntryView.price[j].value));
						}
					}
					for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
						if (catalogEntryView.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
						}
					}
					for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
						if (catalogEntryView.alternativePrice[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(catalogEntryView.alternativePrice[j].value));
						}
					}

					product.productInfo.discount.push(parseFloat(catalogEntryView.amountSaved.amountSaved));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(catalogEntryView.amountSaved.amountSaved)));
				} else {
					//When currency selector is USD
					for (var j = 0; j < catalogEntryView.price.length; j++) {
						if (catalogEntryView.price[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(catalogEntryView.price[j].value));
						}
					}
					for (var j = 0; j < catalogEntryView.price.length; j++) {
						if (catalogEntryView.price[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(catalogEntryView.price[j].value));
						}
					}


					for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
						if (catalogEntryView.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
						}
					}
					for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
						if (catalogEntryView.alternativePrice[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(catalogEntryView.alternativePrice[j].value));
							product.productInfo.totalPrice.push(parseFloat(catalogEntryView.alternativePrice[j].value));
						}
					}
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(catalogEntryView.amountSaved.amountSaved)));
					product.productInfo.discountUSD.push(parseFloat(catalogEntryView.amountSaved.amountSaved));
				}*/
				if (typeof catalogEntryView.sKUs != "undefined") {
					if (typeof (productViewData.selected) != "undefined") {
						product.productInfo.ID.push(productViewData.selected);
					}
					if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
						product.productInfo.weight = [];
						if (typeof catalogEntryView.sKUs[0].attributes != "undefined") {
							for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
								if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
									product.productInfo.weight.push(catalogEntryView.sKUs[0].attributes[j].values[0].value);
								}
							}
						}

					}
					if (catalogEntryView.sKUs[0].inventory != "undefined") {
						product.productInfo.wcsid.push(catalogEntryView.sKUs[0].inventory.productId);
					} else {
						product.productInfo.wcsid.push(catalogEntryView.catentry_id_child);
					}
				} else {
					product.productInfo.ID.push(catalogEntryView.child_partNumber_ntk);
					product.productInfo.wcsid.push(catalogEntryView.catentry_id_child);
				}
				if (typeof (catalogEntryView.facetAttributes.Product) != "undefined") {
					product.productInfo.category.push(catalogEntryView.facetAttributes.Product);
				}
				else {
					for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
						if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
							catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_CATEGORY"||
							catalogEntryView.techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
							product.productInfo.category.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
						}
					}
				}
				for (var j = 0; j < catalogEntryView.descriptiveAttributes.length; j++) {
					if (catalogEntryView.descriptiveAttributes[j].identifier === "TX_EMI") {
						if (catalogEntryView.descriptiveAttributes[j].values[0].value) {
							product.productInfo.emi.push("Yes");
						} else {
							product.productInfo.emi.push("No");
						}
					}
				}
				product.productInfo.stockStatus.push(catalogEntryView.oosFlag);
				var arr = catalogEntryView.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				var pageUrl = window.location.href;
				if (pageUrl.includes("yfret")) {
					product.productInfo.findingMethod = "Recommendations";
					product.productInfo.recoType = "";
				} else if (pageUrl.includes("cm_mmc")) {
					pageUrl = pageUrl.substring(pageUrl.indexOf("cm_mmc"), pageUrl.length);
					product.productInfo.findingMethod = "Recommendations";
					if (pageUrl.includes("mailer")) {
						product.productInfo.findingMethod = "mailer";
					}
					if (pageUrl.includes("trending-products")) {
						product.productInfo.recoType = "trending-products";
					}
					if (pageUrl.includes("similar-products")) {
						product.productInfo.recoType = "similar-products";
					}
					if (pageUrl.includes("top-sellers")) {
						product.productInfo.recoType = "top-sellers";
					}
					if (pageUrl.includes("customers-who-bought-this-also-bought")) {
						product.productInfo.recoType = "customers-who-bought-this-also-bought";
					}
					if (pageUrl.includes("customers-who-viewed-this-also-viewed")) {
						product.productInfo.recoType = "customers-who-viewed-this-also-viewed";
					}
					if (pageUrl.includes("handpicked")) {
						product.productInfo.recoType = "handpicked";
					}
					if (pageUrl.includes("recently-viewed")) {
						product.productInfo.recoType = "recently-viewed";
					}
					if (pageUrl.includes("plp-personalisation")) {
						product.productInfo.recoType = "plp-personalisation";
					}
					if (pageUrl.includes("virtual-tryon")) {
						product.productInfo.findingMethod = "Virtual Try On";
					}
					//Position
					// if (pageUrl.includes("position")) {
					// 	product.productInfo.position = "plp-personalisation";
					// }
				} else {
					var fromPageUrl = document.referrer;
					if (fromPageUrl.includes("cart")) {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.cartPage;
					} else if (fromPageUrl.includes("wishlist")) {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.wishlistPage;
					} else if (fromPageUrl.includes("/shop/")) {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.plpPage;
					} else if (fromPageUrl.includes("/product/")) {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.pdpPage;
					} else if (fromPageUrl.includes("/order-history/")) {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.orderhistoryPage;
					} else if (fromPageUrl.includes("/guestOrder")) {
						product.productInfo.findingMethod = "guest-order-page";
					} else if (fromPageUrl.includes("/confirmation/")) {
						product.productInfo.findingMethod = "order-confirmation-page";
					} else {
						product.productInfo.findingMethod = adobeDataLayerJS.constants.plpPage;
					}
				}
				for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
					//Jewellery
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_KARATAGE") {
						product.productInfo.goldKaratage = [];
						product.productInfo.goldKaratage.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_GENDER") {
						product.productInfo.gender = [];
						product.productInfo.gender.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_COLLECTION") {
						product.productInfo.collection = [];
						product.productInfo.collection.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_METAL") {
						product.productInfo.metal = [];
						product.productInfo.metal.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_JEWELLERY_TYPE") {
						product.productInfo.jewelleryType = [];
						product.productInfo.jewelleryType.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_METAL_COLOR") {
						product.productInfo.metalColor = [];
						product.productInfo.metalColor.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_TYPE") {
						product.productInfo.type = [];
						product.productInfo.type.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_OCCASION") {
						product.productInfo.occasion = [];
						product.productInfo.occasion.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_PRODUCT") {
						product.productInfo.productType = [];
						product.productInfo.productType.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					} else {
						if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
							catalogEntryView.techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
							product.productInfo.productType = [];
							product.productInfo.productType.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
						}
					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_NO OF DIAMONDS") {
						product.productInfo.no_of_diamonds = [];
						product.productInfo.no_of_diamonds.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_DIAMOND_CLARITY") {
						product.productInfo.diamond_clarity = [];
						product.productInfo.diamond_clarity.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_DIAMOND_COLOR") {
						product.productInfo.diamond_color = [];
						product.productInfo.diamond_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_DIAMOND_SHAPE") {
						product.productInfo.diamond_shape = [];
						product.productInfo.diamond_shape.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_DIAMOND_SETTING") {
						//TODO
					}
					if (catalogEntryView.techSpecsAttributes[j].identifier === "JW_TX_NO OF DIAMONDS") {
						product.productInfo.no_of_diamonds = [];
						product.productInfo.no_of_diamonds.push(catalogEntryView.techSpecsAttributes[j].values[0].value);

					}
					if (typeof product.productInfo.category[0] != 'undefined') {
						//Watches
						if (product.productInfo.category[0].toLowerCase() == "watches" || product.productInfo.category[0].toLowerCase() == "clocks") {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_MOVEMENT") {
								product.productInfo.movement = [];
								product.productInfo.movement.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_FUNCTION") {
								product.productInfo.function = [];
								product.productInfo.function.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_CASE_SHAPE") {
								product.productInfo.case_shape = [];
								product.productInfo.case_shape.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_STRAP_MATERIAL") {
								product.productInfo.strap_material = [];
								product.productInfo.strap_material.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_DIAL_COLOR") {
								product.productInfo.dial_color = [];
								product.productInfo.dial_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_STRAP_COLOR") {
								product.productInfo.strap_color = [];
								product.productInfo.strap_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
						//Bags
						if (product.productInfo.category[0].toLowerCase() == "bags") {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_VOLUME(LITRES)") {
								product.productInfo.bag_volume = [];
								product.productInfo.bag_volume.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_MATERIAL_(BODY)") {
								product.productInfo.bag_material_body = [];
								product.productInfo.bag_material_body.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_WATER_RESISTANT") {
								product.productInfo.bag_water_reisitant = [];
								product.productInfo.bag_water_reisitant.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_TYPE") {
								product.productInfo.bag_type = [];
								product.productInfo.bag_type.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_COLOR") {
								product.productInfo.bag_color = [];
								product.productInfo.bag_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
						//Belts
						if (product.productInfo.category[0].toLowerCase() == "belts") {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_MATERIAL") {
								product.productInfo.belt_material = [];
								product.productInfo.belt_material.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_DRESS_CODE") {
								product.productInfo.belt_dresscode = [];
								product.productInfo.belt_dresscode.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
						//Wallets
						if (product.productInfo.category[0].toLowerCase() == "wallets") {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_SMART_FEATURES") {
								product.productInfo.smart_features = [];
								product.productInfo.smart_features.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_TYPE") {
								product.productInfo.wallet_type = [];
								product.productInfo.wallet_type.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_COLOR") {
								product.productInfo.wallet_color = [];
								product.productInfo.wallet_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_DRESS_CODE") {
								product.productInfo.wallet_dresscode = [];
								product.productInfo.wallet_dresscode.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
						//Eyewear
						if (product.productInfo.category[0].toLowerCase() == "frames" || product.productInfo.category[0].toLowerCase() == "sunglasses"
							|| product.productInfo.category[0].toLowerCase().includes('sunglasses') || product.productInfo.category[0].toLowerCase().includes('frames')) {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_FRAME_COLOR") {
								product.productInfo.frame_color = [];
								product.productInfo.frame_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_FRAME_MATERIAL") {
								product.productInfo.frame_material = [];
								product.productInfo.frame_material.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_FRAME_TYPE") {
								product.productInfo.frame_type = [];
								product.productInfo.frame_type.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_LENS_COLOR") {
								product.productInfo.lens_color = [];
								product.productInfo.lens_color.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_LENS_MATERIAL") {
								product.productInfo.lens_material = [];
								product.productInfo.lens_material.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
						//Fragrances
						if (product.productInfo.category[0].toLowerCase() == "fragrances") {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_FRAGRANCE_TYPE") {
								product.productInfo.fragrance_type = [];
								product.productInfo.fragrance_type.push(catalogEntryView.techSpecsAttributes[j].values[0].value);
							}
						}
					}
					/*product.productInfo.frame_design - WT_TX_FRAME_FRONT_SHAPE
					product.productInfo.frame_shape -WT_TX_FRAME_FACE_SHAPE
					product.productInfo.bridge_type*/

				}

				if (typeof catalogEntryView.ribbons != "undefined") {
					for (var j = 0; j < catalogEntryView.ribbons.length; j++) {
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_Collection_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_Collection_Ribbon") {
							product.productInfo.collection_ribbon = [];
							product.productInfo.collection_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_Discounts_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_Discounts_Ribbon") {
							product.productInfo.discounts_ribbon = [];
							product.productInfo.discounts_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_On_Sale" || catalogEntryView.ribbons[j].identifier === "WT_TX_On_Sale") {
							product.productInfo.on_sale_ribbon = [];
							product.productInfo.on_sale_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_Online_Exclusive" || catalogEntryView.ribbons[j].identifier === "WT_TX_Online_Exclusive") {
							product.productInfo.online_exclusive_ribbon = [];
							product.productInfo.online_exclusive_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_New_Arrival" || catalogEntryView.ribbons[j].identifier === "WT_TX_New_Arrival") {
							product.productInfo.new_arrival_ribbon = [];
							product.productInfo.new_arrival_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_Occasion_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_Occasion_Ribbon") {
							product.productInfo.occasion_ribbon = [];
							product.productInfo.occasion_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_EMI_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_EMI_Ribbon") {
							product.productInfo.emi_ribbon = [];
							product.productInfo.emi_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_Express_Delivery_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_Express_Delivery_Ribbon") {
							product.productInfo.express_delivery_ribbon = [];
							product.productInfo.express_delivery_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_BOPIS_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_BOPIS_Ribbon") {
							product.productInfo.bopis_ribbon = [];
							product.productInfo.bopis_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_ROPIS_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_ROPIS_Ribbon") {
							product.productInfo.ropis_ribbon = [];
							product.productInfo.ropis_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "JW_TX_PreOrder_Ribbon" || catalogEntryView.ribbons[j].identifier === "WT_TX_PreOrder_Ribbon") {
							product.productInfo.preorder_ribbon = [];
							product.productInfo.preorder_ribbon.push(catalogEntryView.ribbons[j].values[0].value);
						}
						if (catalogEntryView.ribbons[j].identifier === "TX_RIBBON_TAG") {
							product.productInfo.ribbon_tag = [];
							product.productInfo.ribbon_tag.push(catalogEntryView.ribbons[j].values[0].value);
						}					
					}
				}
				for (var j = 0; j < catalogEntryView.descriptiveAttributes.length; j++) {
					if (catalogEntryView.descriptiveAttributes[j].identifier === "JW_TX_isFOC" || catalogEntryView.descriptiveAttributes[j].identifier == "WT_TX_isFOC") {
						product.productInfo.isfoc = [];
						product.productInfo.isfoc.push(catalogEntryView.descriptiveAttributes[j].values[0].value);
					}
				}
				product.productInfo.thumbnailUrl = [];
				product.productInfo.thumbnailUrl.push(catalogEntryView.thumbnail);

				return product;

			}
		},
		getProductPriceDetails: function (catalogEntryView, currencyVal, product) {
			var priceDetails={};
			if (typeof catalogEntryView.sKUs != "undefined") {
				for (var k = 0; k < catalogEntryView.sKUs.length; k++) {
					if (typeof (product.selectedSKU) !== "undefined" && product.selectedSKU == catalogEntryView.sKUs[k].skuPartNumber) {
						priceDetails = catalogEntryView.sKUs[k];
					}
				}
			} else {
				priceDetails.price = {};
				priceDetails.price = catalogEntryView.price;
			}
			if (currencyVal == "INR") {
				//When currency selector is INR
				for (var j = 0; j < priceDetails.price.length; j++) {
					if (priceDetails.price[j].usage === "Display") {
						product.productInfo.actualPrice.push(parseFloat(priceDetails.price[j].value));
					}
				}
				for (var j = 0; j < priceDetails.price.length; j++) {
					if (priceDetails.price[j].usage === "Offer") {
						product.productInfo.price.push(parseFloat(priceDetails.price[j].value));
						product.productInfo.totalPrice.push(parseFloat(priceDetails.price[j].value));
					}
				}
				for (var j = 0; j < priceDetails.alternativePrice.length; j++) {
					if (priceDetails.alternativePrice[j].usage === "Display") {
						product.productInfo.actualPriceUSD.push(parseFloat(priceDetails.alternativePrice[j].value));
					}
				}
				for (var j = 0; j < priceDetails.alternativePrice.length; j++) {
					if (priceDetails.alternativePrice[j].usage === "Offer") {
						product.productInfo.totalPriceUSD.push(parseFloat(priceDetails.alternativePrice[j].value));
					}
				}

				product.productInfo.discount.push(parseFloat(priceDetails.amountSaved.amountSaved));
				product.productInfo.discountUSD.push(parseFloat(priceDetails.altPriceAmountSaved.amountSaved));
			} else {
				//When currency selector is USD
				for (var j = 0; j < priceDetails.price.length; j++) {
					if (priceDetails.price[j].usage === "Display") {
						product.productInfo.actualPriceUSD.push(parseFloat(priceDetails.price[j].value));
					}
				}
				for (var j = 0; j < priceDetails.price.length; j++) {
					if (priceDetails.price[j].usage === "Offer") {
						product.productInfo.totalPriceUSD.push(parseFloat(priceDetails.price[j].value));
					}
				}


				for (var j = 0; j < priceDetails.alternativePrice.length; j++) {
					if (priceDetails.alternativePrice[j].usage === "Display") {
						product.productInfo.actualPrice.push(parseFloat(priceDetails.alternativePrice[j].value));
					}
				}
				for (var j = 0; j < priceDetails.alternativePrice.length; j++) {
					if (priceDetails.alternativePrice[j].usage === "Offer") {
						product.productInfo.price.push(parseFloat(priceDetails.alternativePrice[j].value));
						product.productInfo.totalPrice.push(parseFloat(priceDetails.alternativePrice[j].value));
					}
				}
				product.productInfo.discount.push(parseFloat(priceDetails.altPriceAmountSaved.amountSaved));
				product.productInfo.discountUSD.push(parseFloat(priceDetails.amountSaved.amountSaved));
			}
		},
		getSectionForPLP: function (url) {
			var res = url.substring(url.indexOf("shop/") + 5, url.length);
			if (res.indexOf("/") != -1) {
				res = res.substring(0, res.indexOf("/"));
			} else if (res.indexOf("?") != -1) {
				res = res.substring(0, res.indexOf("?"));
			}
			return res;
		},
		setPageObject: function () {
			var page = {};
			page.pageInfo = {};
			page.category = {};
			page.pageInfo.section = "";
			page.pageInfo.pageType = "";
			page.pageInfo.prodCatL0 = "";
			if (typeof (adobeDataLayerJS.adobeDigitalData) != "undefined") {
				page.pageInfo.pageName = adobeDataLayerJS.adobeDigitalData.pageInstanceID;
				var pageInstanceID = adobeDataLayerJS.adobeDigitalData.pageInstanceID;
				page.pageInfo.pageType = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
				if (page.pageInfo.pageType == adobeDataLayerJS.constants.plpPage) {
					page.pageInfo.section = adobeDataLayerJS.getSectionForPLP(window.location.href);
				} else if (page.pageInfo.pageType == adobeDataLayerJS.constants.pdpPage) {
					page.pageInfo.section = adobeDataLayerJS.constants.pdpPage;
				} else {
					page.pageInfo.section = page.pageInfo.pageType;
				}
				var brand = adobeDataLayerJS.adobeDigitalData.pageInstanceID.slice(0, adobeDataLayerJS.adobeDigitalData.pageInstanceID.indexOf("-"));
				page.category.primaryCategory = adobeDataLayerJS.adobeDigitalData.pageInstanceID;
				page.category.websiteIdentifier = brand + "-" + adobeDataLayerJS.deviceCheck().toLowerCase();
				page.category.platform = adobeDataLayerJS.deviceCheck().toLowerCase();
			} else {
				if (typeof (window.adobeDigitalData) != "undefined") {
					if (typeof (window.adobeDigitalData.page) == "undefined") {
						page.pageInfo.pageName = window.adobeDigitalData.pageInstanceID;
						var pageInstanceID = window.adobeDigitalData.pageInstanceID;
						page.pageInfo.pageType = pageInstanceID.slice(pageInstanceID.indexOf("-") + 1, pageInstanceID.length);
						if (page.pageInfo.pageType == adobeDataLayerJS.constants.plpPage) {
							page.pageInfo.section = adobeDataLayerJS.getSectionForPLP(window.location.href);
						} else if (page.pageInfo.pageType == adobeDataLayerJS.constants.pdpPage) {
							page.pageInfo.section = adobeDataLayerJS.constants.pdpPage;
						} else {
							page.pageInfo.section = page.pageInfo.pageType;
						}
						var brand = window.adobeDigitalData.pageInstanceID.slice(0, window.adobeDigitalData.pageInstanceID.indexOf("-"));
						page.category.primaryCategory = window.adobeDigitalData.pageInstanceID;
						page.category.websiteIdentifier = brand + "-" + adobeDataLayerJS.deviceCheck().toLowerCase();
						page.category.platform = adobeDataLayerJS.deviceCheck().toLowerCase();
					}
				}
			}
			return page;
		},
		setAccountObject: function () {
			var account = {};
			account.info = {};
			var cookieUserType = "userType";
			var cookieCustID = adobeDataLayerJS.fetchUserInfoFromCookie();
			var adobeCustId = "";
			var custMobileNum = "";
			var userType = "";
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(cookieUserType) === 0) {
					userType = c.substring(
						cookieUserType.length + 1, c.length);
				}
			}			
			var cookieUserInfo = "UserInfo";
			var userInfo = "";
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(cookieUserInfo) === 0) {
					userInfo = c.substring(
						cookieUserInfo.length + 1, c.length);
				}
			}
			if (userInfo != "" && userInfo.length > 0) {
				var dec = decodeURIComponent(userInfo);
				var email = JSON.parse(dec).email;
				adobeCustId = adobeDataLayerJS.piEncryption(email);
				var mobileNum = JSON.parse(dec).mobilePhone1;
				custMobileNum = adobeDataLayerJS.piEncryption(mobileNum);
				account.info.encirclePointBalance = JSON.parse(dec).encirclePointBalance;
				
			}

			if (userType != "" && userType == "R") {
				account.info.custID = adobeCustId;
				account.info.loginStatus = "Logged In";
				account.info.phone = custMobileNum;
			} else {
				account.info.loginStatus = "Anonymous";
			}
			return account;
		},
		fetchUserInfoFromCookie: function () {
			var ca = document.cookie.split(';');
			var cookieName = "CustID";
			var userInfoFromCookie = "";
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(cookieName) === 0) {
					userInfoFromCookie = c.substring(
						cookieName.length + 1, c.length);
				}
			}
			if (userInfoFromCookie != "" && userInfoFromCookie.length > 0) {
				return userInfoFromCookie;
			}
			return userInfoFromCookie;
		},
		getPageName: function () {
			var pageName = "";
			if (typeof (adobeDataLayerJS.adobeDigitalData) != "undefined") {
				pageName = adobeDataLayerJS.adobeDigitalData.pageInstanceID;

			}
			if (typeof (window.adobeDigitalData) != "undefined") {
				if (typeof (window.adobeDigitalData.page) == "undefined") {
					pageName = window.adobeDigitalData.pageInstanceID;
				}
			}
			return pageName;
		},
		//Device Check
		deviceCheck: function () {
			var device;
			if (/Mobi/.test(navigator.userAgent) || /iPad/.test(navigator.userAgent)) {
				device = "M-RWD";
			} else {
				device = "WEBSITE";
			}
			return device
		},
		setCurrencyRate: function (catalogEntryView, currencyVal) {
			var inr = 0;
			var usd = 0;
			if (currencyVal == "INR") {
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					if (catalogEntryView.price[j].usage === "Display") {
						inr = parseFloat(catalogEntryView.price[j].value);
					}
				}
				for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
					if (catalogEntryView.alternativePrice[j].usage === "Display") {
						usd = parseFloat(catalogEntryView.alternativePrice[j].value);
					}
				}
			} else {
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					if (catalogEntryView.price[j].usage === "Display") {
						usd = parseFloat(catalogEntryView.price[j].value);
					}
				}
				for (var j = 0; j < catalogEntryView.alternativePrice.length; j++) {
					if (catalogEntryView.alternativePrice[j].usage === "Display") {
						inr = parseFloat(catalogEntryView.alternativePrice[j].value);
					}
				}
			}
			var rate = inr / usd;
			//Setting Cookie
			var date = new Date();
			date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
			var expires = "; expires=" + date.toGMTString();
			document.cookie = 'currencyRate=' + rate + expires + "; path=/";
		},
		getCurrencyRate: function () {
			//Displaying Cookie
			var ca = document.cookie.split(';');
			var cookieName = "currencyRate";
			var curRate = "";
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(cookieName) === 0) {
					curRate = c.substring(
						cookieName.length + 1, c.length);
				}
			}
			var rate = 0;
			if (curRate != "" && curRate.length > 0) {
				rate = parseFloat(curRate);
			}
			return rate;
		},
		convertorINR: function (usdValue) {
			var inrVal = 0;
			var rate = adobeDataLayerJS.getCurrencyRate();
			inrVal = usdValue * rate;
			var convertedVal = parseFloat(inrVal).toFixed(0);
			return parseFloat(convertedVal);
		},
		convertorUSD: function (inrValue) {
			var usdVal = 0;
			var rate = adobeDataLayerJS.getCurrencyRate();
			if (rate != 0) {
				usdVal = inrValue / rate;
				var convertedVal = parseFloat(usdVal).toFixed(2);
				return parseFloat(convertedVal);
			} else {
				return 0;
			}
		},
		//Cart Page: Remove product from cart & Quantity change
		updateCartEventResponse: function (cartViewEventData, updateCartEventData) {
			if (typeof (updateCartEventData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				var product = adobeDataLayerJS.initializeProductObject();
				if (typeof (cartViewEventData) !== "undefined") {
					for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
						if (cartViewEventData.orderItem[j].orderItemId == updateCartEventData.orderItemId) {
							product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
							product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
							product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
							product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
							product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);
							//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
							product.productInfo.quantity.push(parseFloat(cartViewEventData.orderItem[j].quantity));
							var price = 0;
							if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
								price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
							} else {
								price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
							}
							if (cartViewEventData.orderItem[j].currency == "INR") {
								product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
								product.productInfo.actualPrice.push(price);
								product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
								product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
								product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
								product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
								product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
							} else {
								product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
								product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
								product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
								product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
								product.productInfo.actualPriceUSD.push(price);
								product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
								product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
							}
							product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
							product.productInfo.brand.push(cartViewEventData.orderItem[j].productDetail.motherBrand);
							product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
							var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
							var domain = arr[0] + "//" + arr[2];
							product.productInfo.domain.push(domain);
							product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
						}
						//remove.products.push(product);
					}
					adobeDataLayerJS.adobeDigitalData.product = product;
					if (typeof (updateCartEventData.operation) != "undefined" && updateCartEventData.operation == "remove") {
						_satellite.track('Cart_Remove_Product');
					} else {
						_satellite.track('Cart_Quantity_Change');
					}
				}
			}
		},
		//1. Checkout -Sign In page: Guest checkout
		loginOrCheckoutAsGuestEventResponse: function (loginOrCheckoutAsGuestEventData, cartViewEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			var url = window.location.href;
			var res = url.substring(url.indexOf("checkout/") + 9, url.length);
			if (res == "login") {
				if (adobeDataLayerJS.adobeDigitalData.account.info.loginStatus == "Anonymous") {
					_satellite.track('Checkout_SignIn_Guest_Checkout');
				}
			} else {
				if (adobeDataLayerJS.adobeDigitalData.account.info.loginStatus == "Anonymous") {
					_satellite.track('Checkout_SignIn_Page_Load');
				}
			}
		},
		//2. Checkout -Delivery Info page: Track Load of the Checkout -Delivery Info page
		userDeliveryInfoEventResponse: function (userDeliveryInfoEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			//Fired on load and on click of event in step 2
			_satellite.track('Checkout_Delivery_Page_Load');
		},
		//3. Checkout - Order Review page:Track load of the Checkout - Order Review page
		parseCartReviewEventResponse: function (orderReviewEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			if (adobeDataLayerJS.adobeDigitalData.account.info.loginStatus == "Anonymous") {
				adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(orderReviewEventData.orderItem[0].email1);
				adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(orderReviewEventData.orderItem[0].mobilePhone1);
			}
			adobeDataLayerJS.adobeDigitalData.product = {};
			var product = adobeDataLayerJS.initializeProductObject();
			for (var j = 0; j < orderReviewEventData.orderItem.length; j++) {
				product.productInfo.ID.push(orderReviewEventData.orderItem[j].partNumber);
				product.productInfo.parentID.push(orderReviewEventData.orderItem[j].productDetail.parentPartNumber);
				product.productInfo.wcsid.push(orderReviewEventData.orderItem[j].productId);
				product.productInfo.name.push(orderReviewEventData.orderItem[j].productDetail.itemName);
				product.productInfo.quantity.push(parseFloat(orderReviewEventData.orderItem[j].quantity));
				product.productInfo.currency.push(orderReviewEventData.orderItem[j].currency);	
				//product.productInfo.category.push(adobeDataLayerJS.getCategory(orderReviewEventData.orderItem[j].productDetail.productUrl));
				var price = 0;
				if (orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
					price = parseFloat(orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
				} else {
					price = parseFloat(orderReviewEventData.orderItem[j].unitPrice);
				}
				if (orderReviewEventData.orderItem[j].currency == "INR") {
					product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPrice.push(price);
					product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
					product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
				} else {
					product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
					product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPriceUSD.push(price);
					product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
				}
				product.productInfo.url.push(orderReviewEventData.orderItem[j].productDetail.productUrl);
				product.productInfo.brand.push(orderReviewEventData.orderItem[j].productDetail.motherBrand);
				product.productInfo.stockStatus.push(orderReviewEventData.orderItem[j].orderItemInventoryStatus);
				var arr = orderReviewEventData.orderItem[j].productDetail.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
			}
			//adobeDataLayerJS.adobeDigitalData.product = product;
			/**Adobe Analytics | Product Details - Cart --> Checkout --> Thankyou | 25268 */
			adobeDataLayerJS.adobeDigitalData.product = JSON.parse(localStorage.getItem('cartProduct'));

			_satellite.track('Checkout_Delivery_Continue_To_Review');
			_satellite.track('Checkout_Order_Page_Load');
		},
		//3.a. Checkout -Order Review page: Continue to Pay for your order
		usablePaymentInfoEventResponse: function (orderReviewEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.product = {};
			var product = adobeDataLayerJS.initializeProductObject();
			for (var j = 0; j < orderReviewEventData.orderItem.length; j++) {
				product.productInfo.ID.push(orderReviewEventData.orderItem[j].partNumber);
				product.productInfo.parentID.push(orderReviewEventData.orderItem[j].productDetail.parentPartNumber);
				product.productInfo.wcsid.push(orderReviewEventData.orderItem[j].productId);
				product.productInfo.name.push(orderReviewEventData.orderItem[j].productDetail.itemName);
				product.productInfo.quantity.push(parseFloat(orderReviewEventData.orderItem[j].quantity));
				product.productInfo.currency.push(orderReviewEventData.orderItem[j].currency);				
				//product.productInfo.category.push(adobeDataLayerJS.getCategory(orderReviewEventData.orderItem[j].productDetail.productUrl));
				var price = 0;
				if (orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
					price = parseFloat(orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
				} else {
					price = parseFloat(orderReviewEventData.orderItem[j].unitPrice);
				}
				if (orderReviewEventData.orderItem[j].currency == "INR") {
					product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPrice.push(price);
					product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
					product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
				} else {
					product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
					product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPriceUSD.push(price);
					product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
				}
				product.productInfo.url.push(orderReviewEventData.orderItem[j].productDetail.productUrl);
				product.productInfo.brand.push(orderReviewEventData.orderItem[j].productDetail.motherBrand);
				product.productInfo.stockStatus.push(orderReviewEventData.orderItem[j].orderItemInventoryStatus);
				var arr = orderReviewEventData.orderItem[j].productDetail.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
			}
			//adobeDataLayerJS.adobeDigitalData.product = product;
			/**Adobe Analytics | Product Details - Cart --> Checkout --> Thankyou | 25268 */
			adobeDataLayerJS.adobeDigitalData.product = JSON.parse(localStorage.getItem('cartProduct'));


			adobeDataLayerJS.adobeDigitalData.order = {};
			adobeDataLayerJS.adobeDigitalData.order.payment = {};
			if (typeof (orderReviewEventData.redeemAmount) != "undefined" && orderReviewEventData.redeemAmount != '') {
				adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = parseFloat(orderReviewEventData.redeemAmount);
			} else {
				adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = 0;
			}
			_satellite.track('Checkout_Order_Continue_To_Payment');
			_satellite.track('Checkout_Payment_Info_Page_Load');
			
		},
		//4-a. Checkout - Payment Info page: Pay for your order	
		paymentInformationEventResponse: function (paymentInformationEventData, orderReviewEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.order = {};
			adobeDataLayerJS.adobeDigitalData.order.payment = {};
			var paymentMethod = "";
			if (typeof (paymentInformationEventData.paymentOption) != "undefined") {
				paymentMethod = paymentInformationEventData.paymentMode + "-" + paymentInformationEventData.paymentOption;
			} else {
				paymentMethod = paymentInformationEventData.paymentMode;
			}
			if (typeof paymentInformationEventData.gitfcardSubmit != "undefined") {
				if (typeof (paymentInformationEventData.redeemAmount) != "undefined" && paymentInformationEventData.redeemAmount != '') {
					adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = parseFloat(paymentInformationEventData.redeemAmount);
					paymentMethod = paymentMethod + "|GiftCard";
				} else {
					adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = 0;
				}
			} else {
				adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = 0;
			}
			if (parseInt(paymentInformationEventData.encircle)>0) {
				paymentMethod = paymentMethod + "|Encircle";
			}
			
			if(paymentInformationEventData.digiGold){
				if(paymentInformationEventData.digiGoldRedeem){
				paymentMethod = paymentMethod + "|DigiGold";
				}
				if(paymentInformationEventData.otherDigiGold){
					paymentMethod = paymentMethod + "|Other DigiGold";
				}
				

			}
			adobeDataLayerJS.adobeDigitalData.order.payment.method = paymentMethod;
			adobeDataLayerJS.adobeDigitalData.order.revenue = {};

			if (parseFloat(paymentInformationEventData.extradetails.shippingCharge) >= 0 && paymentInformationEventData.extradetails.currency == "INR") {
				adobeDataLayerJS.adobeDigitalData.order.revenue.shippingCharge = parseFloat(paymentInformationEventData.extradetails.shippingCharge);
				//adobeDataLayerJS.adobeDigitalData.order.revenue.shippingChargeUSD = adobeDataLayerJS.convertorUSD(parseFloat(paymentInformationEventData.extradetails.shippingCharge));
			} else {
				adobeDataLayerJS.adobeDigitalData.order.revenue.shippingCharge = adobeDataLayerJS.convertorINR(parseFloat(paymentInformationEventData.extradetails.shippingCharge));
				//adobeDataLayerJS.adobeDigitalData.order.revenue.shippingChargeUSD = paymentInformationEventData.extradetails.shippingCharge;
			}

			adobeDataLayerJS.adobeDigitalData.product = {};
			var product = adobeDataLayerJS.initializeProductObject();
			product.productInfo.delMethod = [];
			for (var j = 0; j < orderReviewEventData.orderItem.length; j++) {
				product.productInfo.ID.push(orderReviewEventData.orderItem[j].partNumber);
				product.productInfo.parentID.push(orderReviewEventData.orderItem[j].productDetail.parentPartNumber);
				product.productInfo.wcsid.push(orderReviewEventData.orderItem[j].productId);
				product.productInfo.name.push(orderReviewEventData.orderItem[j].productDetail.itemName);
				product.productInfo.quantity.push(parseFloat(orderReviewEventData.orderItem[j].quantity));
				product.productInfo.currency.push(orderReviewEventData.orderItem[j].currency);
				//product.productInfo.category.push(adobeDataLayerJS.getCategory(orderReviewEventData.orderItem[j].productDetail.productUrl));
				var price = 0;
				if (orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
					price = parseFloat(orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
				} else {
					price = parseFloat(orderReviewEventData.orderItem[j].unitPrice);
				}
				if (orderReviewEventData.orderItem[j].currency == "INR") {
					product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPrice.push(price*parseFloat(orderReviewEventData.orderItem[j].quantity));
					product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscountWithQty(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price*parseFloat(orderReviewEventData.orderItem[j].quantity)));
					product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscountWithQty(orderReviewEventData.orderItem[j]))));
				} else {
					product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price*parseFloat(orderReviewEventData.orderItem[j].quantity)));
					product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscountWithQty(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPriceUSD.push(price*parseFloat(orderReviewEventData.orderItem[j].quantity));
					product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscountWithQty(orderReviewEventData.orderItem[j])));
				}
				product.productInfo.url.push(orderReviewEventData.orderItem[j].productDetail.productUrl);
				product.productInfo.brand.push(orderReviewEventData.orderItem[j].productDetail.motherBrand);
				product.productInfo.stockStatus.push(orderReviewEventData.orderItem[j].orderItemInventoryStatus);
				product.productInfo.delMethod.push(orderReviewEventData.orderItem[j].selectedDeliveryMethodName);
				var arr = orderReviewEventData.orderItem[j].productDetail.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
			}
			//adobeDataLayerJS.adobeDigitalData.product = product;
			/**Adobe Analytics | Product Details - Cart --> Checkout --> Thankyou | 25268 */
			adobeDataLayerJS.adobeDigitalData.product = JSON.parse(localStorage.getItem('cartProduct'));
			var productFromCart = JSON.parse(localStorage.getItem('cartProduct'));

			localStorage.setItem("usablePaymentInfoEvent", 
				JSON.stringify(
					{
						paymentOption: paymentMethod,
						product: productFromCart, revenue: parseFloat(paymentInformationEventData.total),
						redeemAmount: paymentInformationEventData.redeemAmount,
						shipping: parseFloat(paymentInformationEventData.extradetails.shippingCharge)
					}));
			_satellite.track('Checkout_Payment_Pay_For_Order');
		},
		//5. Checkout - Payment Info page: Pay for your order
		paymentSuccessEventResponse: function (paymentSuccessEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.order = {};
			adobeDataLayerJS.adobeDigitalData.order.payment = {};
			adobeDataLayerJS.adobeDigitalData.order.payment.method = paymentSuccessEventData.paymentMode + "-" + paymentInformationEventData.paymentOption;
		},
		//Thank you page: Track payment failure
		paymentFailureEventResponse: function (paymentFailureEventData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.order = {};
			adobeDataLayerJS.adobeDigitalData.order.payment = {};
			adobeDataLayerJS.adobeDigitalData.order.failure = {};
			adobeDataLayerJS.adobeDigitalData.order.revenue = {};
			var paymentInfo = JSON.parse(localStorage.getItem('usablePaymentInfoEvent'));
			adobeDataLayerJS.adobeDigitalData.order.payment.method = paymentInfo.paymentOption;
			if (typeof (paymentInfo.redeemAmount) != "undefined" && parseFloat(paymentInfo.redeemAmount) > 0) {
				adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = parseFloat(paymentInfo.redeemAmount)
			} else {
				adobeDataLayerJS.adobeDigitalData.order.payment.giftPay = 0;
			}
			adobeDataLayerJS.adobeDigitalData.order.failure.reason = paymentFailureEventData.errMsg;
			if (paymentInfo.product.productInfo.currency[0] == "USD") {
				adobeDataLayerJS.adobeDigitalData.order.revenue.oppMissed = adobeDataLayerJS.convertorINR(parseFloat(paymentInfo.revenue));
				adobeDataLayerJS.adobeDigitalData.order.revenue.oppMissedUSD = paymentInfo.revenue;
			} else {
				adobeDataLayerJS.adobeDigitalData.order.revenue.oppMissed = paymentInfo.revenue;
				adobeDataLayerJS.adobeDigitalData.order.revenue.oppMissedUSD = adobeDataLayerJS.convertorUSD(parseFloat(paymentInfo.revenue));
			}
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = paymentInfo.product;
			localStorage.removeItem("usablePaymentInfoEvent");
			_satellite.track('Order_Failure');
		},
		//Header Tracking:Track Currency selection on top of the page
		currencySelectionHeader: function (headerCurrencyChangeEventRes) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.page.pageInfo.currency = headerCurrencyChangeEventRes.currencyValue;
			_satellite.track('Currency_Selection');
		},
		//Home Page: Find Store Near You
		storeLocatorSearchEventResponse: function (storeLocatorSearchEvent) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.page.pin = {};
			if (typeof (storeLocatorSearchEvent) != "undefined") {
				if (typeof (storeLocatorSearchEvent.errMsg) == "undefined") {
					adobeDataLayerJS.adobeDigitalData.page.pin.code = storeLocatorSearchEvent.cityOrZipcode;
					var country = "";
					if (storeLocatorSearchEvent.PhysicalStore.length > 0) {
						country = storeLocatorSearchEvent.PhysicalStore[0].country.toLowerCase() == "india" ? "IN" : storeLocatorSearchEvent.PhysicalStore[0].country;
					}
					adobeDataLayerJS.adobeDigitalData.page.pin.country = country;
				} else {
					adobeDataLayerJS.adobeDigitalData.page.pin.code = storeLocatorSearchEvent.cityOrZipcode;
				}
			}
			_satellite.track('Find_Store');
		},
		//Sorting on Search Page: Best Seller sort, New Arrivals, Popularity, Discount, Price
		sortingFilterEventResponse: function (sortingFilterEvent) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();

			if (typeof (sortingFilterEvent) != "undefined") {
				if (sortingFilterEvent.type == "Best sellers") {
					_satellite.track('Best_Seller_Sort');
				}
				else if (sortingFilterEvent.type == "New arrivals") {
					_satellite.track('New_Arrivals_Sort');
				}
				else if (sortingFilterEvent.type == "Popularity") {
					_satellite.track('Popularity_Sort');
				}
				else if (sortingFilterEvent.type == "Discount" || sortingFilterEvent.type.toLowerCase().includes("discount")) {
					_satellite.track('Discount_Sort');
				}
				else if (sortingFilterEvent.type == "Price" || sortingFilterEvent.type.toLowerCase().includes("price")) {
					_satellite.track('Price_Sort');
				}
			}
		},
		//Cart Page: Add a gift message
		addEditGiftMessageEventResponse: function (addEditGiftMessageEventData, cartViewEventData) {
			if (typeof (cartViewEventData != "undefined")) {
				var product = adobeDataLayerJS.initializeProductObject();
				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					if (addEditGiftMessageEventData.orderItem[0].orderItemId == cartViewEventData.orderItem[j].orderItemId) {
						product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
						product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
						product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
						product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
						product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
						product.productInfo.brand.push(cartViewEventData.orderItem[j].productDetail.motherBrand);						
						//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
						product.productInfo.quantity.push(parseInt(cartViewEventData.orderItem[j].unitQuantity));
						product.productInfo.url.push(cartViewEventData.orderItem[j].productUrl);

						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
						var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.currency.push(parseFloat(cartViewEventData.currency));
						var price = 0;
						if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
							price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
						} else {
							price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
						}
						if (cartViewEventData.orderItem[j].currency == "INR") {
							product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.actualPrice.push(price);
							product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
							product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
							product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						} else {
							product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
							product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPriceUSD.push(price);
							product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						}
					}
				}
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = product;
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('Cart_Gift_Message');
			}
		},
		//User Login:Forgot Password
		forgotPasswordEventResponse: function (forgotPasswordEventResponseData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('Forgot_Password');
		},
		// Product Details Page:Click 'Zoom Image' under images
		clickZoomEventResponse: function (productViewData) {
			if (typeof (productViewData != "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Zoom_Image');

			}
		},
		//Product Details Page: Notify Me
		notifyClickedEventResponse: function (productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('PDP_Notify_Me');
		},
		//User Login: Track logouts
		userlogoutEventResponse: function () {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('Logout')
		},
		//Home Page: Use My current location
		useCurrentLocationEvent: function () {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('Current_Location');
		},
		//Product Details Page: Click on share button
		clickShareEventResponse: function (productViewData) {
			if (typeof (productViewData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Share_Button');
			}
		},
		// Product Details Page: Share the Product details
		shareResponse: function (shareEventData, productViewData) {
			if (typeof (shareEventData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				if (shareEventData === 'Facebook' || shareEventData === 'Twitter' || shareEventData === 'Gplus' || shareEventData === 'Pinterest' || shareEventData === 'Whatsapp') {
					adobeDataLayerJS.adobeDigitalData.product.productInfo.sharingChannel = shareEventData;
					_satellite.track('PDP_Share_Product_Details');
				}
				else {
					adobeDataLayerJS.adobeDigitalData.product.productInfo.sharingChannel = 'Email';
					_satellite.track('PDP_Share_Product_Details');
				}
			}
		},
		//Product Details Page: Try On
		tryOnEventHandler: function (productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('virtual_try_on');
		},
		//Error Tracking: Track Errors encoutered on website	
		errorEventHandler: function (errorHandlerResponse) {
			if (errorHandlerResponse.type == "danger") {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.page.error = {};
				adobeDataLayerJS.adobeDigitalData.page.error.name = errorHandlerResponse.msg;
				_satellite.track('Error');
			}
		
		},
		//Cart Page: Continue Shopping		
		continueShoppingEventHandler: function (cartViewEventData) {
			if (typeof (cartViewEventData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				if (typeof (cartViewEventData.orderItem) !== "undefined") {
					var product = adobeDataLayerJS.initializeProductObject();
					for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
						product.productInfo.ID.push(cartViewEventData.orderItem[j].partNumber);
						product.productInfo.parentID.push(cartViewEventData.orderItem[j].productDetail.parentPartNumber);
						product.productInfo.wcsid.push(cartViewEventData.orderItem[j].productId);
						product.productInfo.name.push(cartViewEventData.orderItem[j].productDetail.itemName);
						product.productInfo.currency.push(cartViewEventData.orderItem[j].currency);						
						//product.productInfo.category.push(adobeDataLayerJS.getCategory(cartViewEventData.orderItem[j].productDetail.productUrl));
						product.productInfo.quantity.push(parseFloat(cartViewEventData.orderItem[j].quantity));
						var price = 0;
						if (cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
							price = parseFloat(cartViewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
						} else {
							price = parseFloat(cartViewEventData.orderItem[j].unitPrice);
						}
						if (cartViewEventData.orderItem[j].currency == "INR") {
							product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.actualPrice.push(price);
							product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
							product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
							product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
						} else {
							product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
							product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j]))));
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j]))));
							product.productInfo.actualPriceUSD.push(price);
							product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(cartViewEventData.orderItem[j])));
							product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(cartViewEventData.orderItem[j])));
						}
						product.productInfo.url.push(cartViewEventData.orderItem[j].productDetail.productUrl);
						product.productInfo.brand.push(cartViewEventData.orderItem[j].productDetail.motherBrand);
						product.productInfo.stockStatus.push(cartViewEventData.orderItem[j].orderItemInventoryStatus);
						var arr = cartViewEventData.orderItem[j].productDetail.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					}
					adobeDataLayerJS.adobeDigitalData.product = product;
				}
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('Cart_Continue_Shopping');
			}
		},
		//Product Details Page: Product Image clicks
		clickImageEventResponse: function (clickImageEventData, productViewData) {
			if (typeof (clickImageEventData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				if (typeof (clickImageEventData.selected !== "undefined")) {
					var product = adobeDataLayerJS.initializeProductObject();
					product.productInfo.ID.push(clickImageEventData.selected.skuPartNumber);
					product.productInfo.wcsid.push(clickImageEventData.selected.skuCatentryId);
					product.productInfo.stockStatus.push(clickImageEventData.selected.inventory.inventoryStatus);
					product.productInfo.quantity.push(parseFloat(clickImageEventData.selected.inventory.availableQuantity));
					product.productInfo.currency.push(clickImageEventData.selected.price[0].currency);
					if (clickImageEventData.selected.price[0].currency == "INR") {
						for (var j = 0; j < clickImageEventData.selected.price.length; j++) {
							if (clickImageEventData.selected.price[j].usage === "Display") {
								product.productInfo.actualPrice.push(parseFloat(clickImageEventData.selected.price[j].value));
							}
						}
						for (var j = 0; j < clickImageEventData.selected.price.length; j++) {
							if (clickImageEventData.selected.price[j].usage === "Offer") {
								product.productInfo.price.push(parseFloat(clickImageEventData.selected.price[j].value));
								product.productInfo.totalPrice.push(parseFloat(clickImageEventData.selected.price[j].value));
							}
						}
						product.productInfo.discount.push(parseFloat(clickImageEventData.selected.amountSaved.amountSaved));
						for (var j = 0; j < clickImageEventData.selected.alternativePrice.length; j++) {
							if (clickImageEventData.selected.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPriceUSD.push(parseFloat(clickImageEventData.selected.alternativePrice[j].value));
							}
						}
						for (var j = 0; j < clickImageEventData.selected.alternativePrice.length; j++) {
							if (clickImageEventData.selected.alternativePrice[j].usage === "Offer") {
								product.productInfo.totalPriceUSD.push(parseFloat(clickImageEventData.selected.alternativePrice[j].value));
							}
						}
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(clickImageEventData.selected.amountSaved.amountSaved)));
					} else {
						for (var j = 0; j < clickImageEventData.selected.price.length; j++) {
							if (clickImageEventData.selected.price[j].usage === "Display") {
								product.productInfo.actualPriceUSD.push(parseFloat(clickImageEventData.selected.price[j].value));
							}
						}
						for (var j = 0; j < clickImageEventData.selected.price.length; j++) {
							if (clickImageEventData.selected.price[j].usage === "Offer") {
								product.productInfo.totalPriceUSD.push(parseFloat(clickImageEventData.selected.price[j].value));
							}
						}
						product.productInfo.discountUSD.push(parseFloat(clickImageEventData.selected.amountSaved.amountSaved));
						for (var j = 0; j < clickImageEventData.selected.alternativePrice.length; j++) {
							if (clickImageEventData.selected.alternativePrice[j].usage === "Display") {
								product.productInfo.actualPrice.push(parseFloat(clickImageEventData.selected.alternativePrice[j].value));

							}
						}
						for (var j = 0; j < clickImageEventData.selected.alternativePrice.length; j++) {
							if (clickImageEventData.selected.alternativePrice[j].usage === "Offer") {
								product.productInfo.price.push(parseFloat(clickImageEventData.selected.alternativePrice[j].value));
								product.productInfo.totalPrice.push(parseFloat(clickImageEventData.selected.alternativePrice[j].value));

							}
						}
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(clickImageEventData.selected.amountSaved.amountSaved)));
					}

					product.productInfo.url.push(clickImageEventData.producturl);
					product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(productViewData.catalogEntryView[0]));
					var arr = clickImageEventData.producturl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					if (productViewData.catalogEntryView[0].txBrand == "Tanishq" || productViewData.catalogEntryView[0].txBrand == "Mia") {
						if (clickImageEventData.selected.attributes!="undefined" && clickImageEventData.selected.attributes[0].name == "Gross Weight") {
							product.productInfo.weight.push(clickImageEventData.selected.attributes[0].values[0].value);
						}
					}
					product.productInfo.parentID.push(productViewData.catalogEntryView[0].partNumber);
					product.productInfo.name.push(productViewData.catalogEntryView[0].name);	
					if (typeof (productViewData.catalogEntryView[0].facetAttributes.Product) != "undefined") {
						product.productInfo.category.push(productViewData.catalogEntryView[0].facetAttributes.Product);
					}
					else {
						for (var j = 0; j < productViewData.catalogEntryView[0].techSpecsAttributes.length; j++) {
							if (productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
								productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "JW_TX_PRODUCT" ||
								productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
								product.productInfo.category.push(productViewData.catalogEntryView[0].techSpecsAttributes[j].values[0].value);
							}
						}
					}				
					adobeDataLayerJS.adobeDigitalData.product = product;
				}
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.page.image = {};
				if (clickImageEventData.imageCatagory == "ANGLEIMAGES_THUMBNAIL") {
					adobeDataLayerJS.adobeDigitalData.page.image.type = "Image";
				} else if (clickImageEventData.imageCatagory == "MEDIA_THUMBNAIL") {
					adobeDataLayerJS.adobeDigitalData.page.image.type = "Video";
				} else {
					adobeDataLayerJS.adobeDigitalData.page.image.type = "Tangiblee";
				}
				adobeDataLayerJS.adobeDigitalData.page.image.position = clickImageEventData.position;
				_satellite.track('PDP_Product_Image');
			}
		},
		//Product Details Page: Size guide clicks
		clickSizeGuideEventResponse: function (clickImageData, productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			if (typeof (clickImageData.selected !== "undefined")) {
				var product = adobeDataLayerJS.initializeProductObject();
				product.productInfo.ID.push(clickImageData.selected.skuPartNumber);
				product.productInfo.wcsid.push(clickImageData.selected.skuCatentryId);
				product.productInfo.stockStatus.push(clickImageData.selected.inventory.inventoryStatus);
				product.productInfo.quantity.push(parseFloat(clickImageData.selected.inventory.availableQuantity));
				product.productInfo.currency.push(clickImageData.selected.price[0].currency);
				if (clickImageData.selected.price[0].currency == "INR") {
					for (var j = 0; j < clickImageData.selected.price.length; j++) {
						if (clickImageData.selected.price[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(clickImageData.selected.price[j].value));
						}
					}
					for (var j = 0; j < clickImageData.selected.price.length; j++) {
						if (clickImageData.selected.price[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(clickImageData.selected.price[j].value));
							product.productInfo.totalPrice.push(parseFloat(clickImageData.selected.price[j].value));
						}
					}
					product.productInfo.discount.push(parseFloat(clickImageData.selected.amountSaved.amountSaved));
					for (var j = 0; j < clickImageData.selected.alternativePrice.length; j++) {
						if (clickImageData.selected.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(clickImageData.selected.alternativePrice[j].value));
						}
					}
					for (var j = 0; j < clickImageData.selected.alternativePrice.length; j++) {
						if (clickImageData.selected.alternativePrice[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(clickImageData.selected.alternativePrice[j].value));
						}
					}
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(clickImageData.selected.amountSaved.amountSaved)));
				} else {
					for (var j = 0; j < clickImageData.selected.price.length; j++) {
						if (clickImageData.selected.price[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(clickImageData.selected.price[j].value));
						}
					}
					for (var j = 0; j < clickImageData.selected.price.length; j++) {
						if (clickImageData.selected.price[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(clickImageData.selected.price[j].value));
						}
					}
					product.productInfo.discountUSD.push(parseFloat(clickImageData.selected.amountSaved.amountSaved));
					for (var j = 0; j < clickImageData.selected.alternativePrice.length; j++) {
						if (clickImageData.selected.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(clickImageData.selected.alternativePrice[j].value));

						}
					}
					for (var j = 0; j < clickImageData.selected.alternativePrice.length; j++) {
						if (clickImageData.selected.alternativePrice[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(clickImageData.selected.alternativePrice[j].value));
							product.productInfo.totalPrice.push(parseFloat(clickImageData.selected.alternativePrice[j].value));

						}
					}
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(clickImageData.selected.amountSaved.amountSaved)));
				}

				product.productInfo.url.push(clickImageData.producturl);
				product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(productViewData.catalogEntryView[0]));
				var arr = clickImageData.producturl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				if (productViewData.catalogEntryView[0].txBrand == "Tanishq" || productViewData.catalogEntryView[0].txBrand == "Mia") {
					if (clickImageData.selected.attributes!="undefined" && clickImageData.selected.attributes[0].name == "Gross Weight") {
						product.productInfo.weight.push(clickImageData.selected.attributes[0].values[0].value);
					}
				}
				product.productInfo.parentID.push(productViewData.catalogEntryView[0].partNumber);
				product.productInfo.name.push(productViewData.catalogEntryView[0].name);				
				if (typeof (productViewData.catalogEntryView[0].facetAttributes.Product) != "undefined") {
					product.productInfo.category.push(productViewData.catalogEntryView[0].facetAttributes.Product);
				}
				else {
					for (var j = 0; j < productViewData.catalogEntryView[0].techSpecsAttributes.length; j++) {
						if (productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY"||
						productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "JW_TX_CATEGORY"||
						productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
							product.productInfo.category.push(productViewData.catalogEntryView[0].techSpecsAttributes[j].values[0].value);
						}
					}
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
			}
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('PDP_Size_Guide');
		},
		//Product Details Page: Product Video clicks
		parseVideoAnalyticsData: function (videoData, productViewData) {
			if (typeof (videoData !== "undefined")) {
				if (videoData.isPlay) {
					adobeDataLayerJS.adobeDigitalData.product = {};
					adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
					adobeDataLayerJS.adobeDigitalData.product.productInfo.url = [];
					adobeDataLayerJS.adobeDigitalData.product.productInfo.url.push(window.location.href);
					adobeDataLayerJS.adobeDigitalData.page = {};
					adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
					adobeDataLayerJS.adobeDigitalData.account = {};
					adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
					_satellite.track('PDP_Product_Video_Play');
				}
			}
		},
		//Product Details Page: Add to Cart from 'Compare products' [Tangiblee]
		tangibleeAddToCartEventHandler: function (eventData, productViewData) {
			if (typeof (productViewData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.page.cart = {};
				adobeDataLayerJS.adobeDigitalData.page.cart.source = "Tangiblee";
				_satellite.track('PDP_Cart_Add_Compare');
				localStorage.setItem("atcTangiblee", true);
			}
		},
		// Checkout -Order Review page: Delivery method selection
		deliveryMethodChangeEventHandler: function (orderReviewEventData) {
			if (typeof (orderReviewEventData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.product = {};
				var product = adobeDataLayerJS.initializeProductObject();
				product.productInfo.delMethod = [];
				for (var j = 0; j < orderReviewEventData.orderItem.length; j++) {
					product.productInfo.ID.push(orderReviewEventData.orderItem[j].partNumber);
					product.productInfo.parentID.push(orderReviewEventData.orderItem[j].productDetail.parentPartNumber);
					product.productInfo.wcsid.push(orderReviewEventData.orderItem[j].productId);
					product.productInfo.name.push(orderReviewEventData.orderItem[j].productDetail.itemName);
					product.productInfo.quantity.push(parseFloat(orderReviewEventData.orderItem[j].quantity));
					product.productInfo.currency.push(orderReviewEventData.orderItem[j].currency);					
					//product.productInfo.category.push(adobeDataLayerJS.getCategory(orderReviewEventData.orderItem[j].productDetail.productUrl));
					var price = 0;
					if (orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
						price = parseFloat(orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
					} else {
						price = parseFloat(orderReviewEventData.orderItem[j].unitPrice);
					}
					if (orderReviewEventData.orderItem[j].currency == "INR") {
						product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
						product.productInfo.actualPrice.push(price);
						product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
						product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
						product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
						product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
						product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
					} else {
						product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
						product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
						product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
						product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
						product.productInfo.actualPriceUSD.push(price);
						product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
						product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
					}
					product.productInfo.url.push(orderReviewEventData.orderItem[j].productDetail.productUrl);
					product.productInfo.brand.push(orderReviewEventData.orderItem[j].productDetail.motherBrand);
					product.productInfo.stockStatus.push(orderReviewEventData.orderItem[j].orderItemInventoryStatus);
					product.productInfo.delMethod.push(orderReviewEventData.orderItem[j].selectedDeliveryMethodName);
					var arr = orderReviewEventData.orderItem[j].productDetail.productUrl.split("/");
					var domain = arr[0] + "//" + arr[2];
					product.productInfo.domain.push(domain);
					product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
				_satellite.track('Checkout_Order_Delivery Method_Selction');
			}
		},
		//My Account- Order History: Order Details
		userOrderDetailsViewEventResponse: function (userOrderDetailsViewEventData) {

		},
		//Internal Search: Clickthroughs
