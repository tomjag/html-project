		//Product Details Page: Reviews
		clickOnReviewLinkEventResponse: function (clickOnReviewLinkData, productViewData) {
			if (typeof (clickOnReviewLinkData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				_satellite.track('PDP_Review_Link');
			}
		},
		//Order Cancellation Event
		orderCancellationEventResponse: function (orderCancellationEventResponseData, userOrderDetailsViewEventData) {
			if (typeof (orderCancellationEventResponseData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.product = {};
				var product = adobeDataLayerJS.initializeProductObject();
				//Check Shipped Items
				if (orderCancellationEventResponseData.cancel.cancelItem) {
					for (var key in userOrderDetailsViewEventData.shipment) {
						var shipmentItemArray = userOrderDetailsViewEventData.shipment[key].Items;
						for (var key in shipmentItemArray) {
							var shipmentItem = shipmentItemArray[key];
							if (shipmentItem.orderItemId == orderCancellationEventResponseData.cancel.orderItemId) {
								product.productInfo.ID.push(shipmentItem.partNumber);
								var tUrl = shipmentItem.thumbnail;
								var res = tUrl.substring(tUrl.lastIndexOf("/") + 1, tUrl.indexOf(".jpg"));
								var parentId = res.replace("_1", "_P");
								product.productInfo.parentID.push(parentId);
								product.productInfo.wcsid.push(shipmentItem.parentProductId);
								product.productInfo.name.push(shipmentItem.name);
								product.productInfo.currency.push(shipmentItem.currency);
								if (shipmentItem.currency == "INR") {
									product.productInfo.price.push(parseFloat(shipmentItem.orderItemPrice));
									product.productInfo.actualPrice.push(parseFloat(shipmentItem.unitPrice));
									product.productInfo.totalPrice.push(parseFloat(shipmentItem.orderItemPrice));
									product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(shipmentItem.unitPrice)));
									product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(shipmentItem.orderItemPrice)));
								} else {
									product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.orderItemPrice)));
									product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.unitPrice)));
									product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.orderItemPrice)));
									product.productInfo.actualPriceUSD.push(parseFloat(shipmentItem.unitPrice));
									product.productInfo.totalPriceUSD.push(parseFloat(shipmentItem.orderItemPrice));
								}

								product.productInfo.quantity.push(parseInt(shipmentItem.quantity));
								product.productInfo.url.push(shipmentItem.productUrl);
								product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
								var arr = shipmentItem.productUrl.split("/");
								var domain = arr[0] + "//" + arr[2];
								product.productInfo.domain.push(domain);
							}
						}
					}
				} else {
					for (var key in userOrderDetailsViewEventData.shipment) {
						var shipmentItemArray = userOrderDetailsViewEventData.shipment[key].Items;
						for (var key in shipmentItemArray) {
							var shipmentItem = shipmentItemArray[key];
							product.productInfo.ID.push(shipmentItem.partNumber);
							var tUrl = shipmentItem.thumbnail;
							var res = tUrl.substring(tUrl.lastIndexOf("/") + 1, tUrl.indexOf(".jpg"));
							var parentId = res.replace("_1", "_P");
							product.productInfo.parentID.push(parentId);
							product.productInfo.wcsid.push(shipmentItem.parentProductId);
							product.productInfo.name.push(shipmentItem.name);
							product.productInfo.currency.push(shipmentItem.currency);
							if (shipmentItem.currency == "INR") {
								product.productInfo.price.push(parseFloat(shipmentItem.orderItemPrice));
								product.productInfo.actualPrice.push(parseFloat(shipmentItem.unitPrice));
								product.productInfo.totalPrice.push(parseFloat(shipmentItem.orderItemPrice));
								product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(shipmentItem.unitPrice)));
								product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(shipmentItem.orderItemPrice)));
							} else {
								product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.orderItemPrice)));
								product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.unitPrice)));
								product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(shipmentItem.orderItemPrice)));
								product.productInfo.actualPriceUSD.push(parseFloat(shipmentItem.unitPrice));
								product.productInfo.totalPriceUSD.push(parseFloat(shipmentItem.orderItemPrice));
							}

							product.productInfo.quantity.push(parseInt(shipmentItem.quantity));
							product.productInfo.url.push(shipmentItem.productUrl);
							product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
							var arr = shipmentItem.productUrl.split("/");
							var domain = arr[0] + "//" + arr[2];
							product.productInfo.domain.push(domain);
						}
					}
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.order = {};
				adobeDataLayerJS.adobeDigitalData.order.info = {};
				adobeDataLayerJS.adobeDigitalData.order.info.ID = orderCancellationEventResponseData.orderId;
				adobeDataLayerJS.adobeDigitalData.order.info.status = userOrderDetailsViewEventData.orderStatusDescription;
				adobeDataLayerJS.adobeDigitalData.order.cancel = {};
				adobeDataLayerJS.adobeDigitalData.order.cancel.reason = orderCancellationEventResponseData.cancelReason;
				_satellite.track('Order_History_Review');
			}
		},
		//Product wise discount caluculation (Product discount + Promotional discount)
		calculateProductDiscount: function (productData) {
			var productDiscount = 0;
			var promotionDiscount = 0;
			if (typeof productData.productDetail.savingsAmt != "undefined") {
				productDiscount = Math.abs(parseFloat(productData.productDetail.savingsAmt));
			}
			if (typeof productData.totalAdjustment.value != "undefined") {
				promotionDiscount = Math.abs(parseFloat(productData.totalAdjustment.value));
			}
			var totalDiscount = 0;
			totalDiscount = productDiscount + promotionDiscount;
			return totalDiscount;
		},
		calculateProductDiscountWithQty: function (productData) {
			var productDiscount = 0;
			var promotionDiscount = 0;
			if (typeof productData.productDetail.savingsAmt != "undefined") {
				productDiscount = Math.abs(parseFloat(productData.productDetail.savingsAmt));
			}
			if (typeof productData.totalAdjustment.value != "undefined") {
				promotionDiscount = Math.abs(parseFloat(productData.totalAdjustment.value));
			}
			var totalDiscount = 0;
			totalDiscount = (productDiscount * parseFloat(productData.quantity)) + promotionDiscount;
			return totalDiscount;
		},
		//Total price calculation - Actual price - (Product discount + Promotional Discount)
		calculateFinalPrice: function (productData) {
			var discountedPrice = 0;
			var promotionDiscount = 0;
			if (typeof productData.orderItemPrice != "undefined") {
				discountedPrice = Math.abs(parseFloat(productData.orderItemPrice));
			}
			if (typeof productData.totalAdjustment.value != "undefined") {
				promotionDiscount = Math.abs(parseFloat(productData.totalAdjustment.value));
			}
			var finalPayablePrice = 0;
			finalPayablePrice = discountedPrice - promotionDiscount;
			return finalPayablePrice;
		},
		//Common method for selectedProductobject
		constructSelectedProductObject: function (objectData, productViewData) {
			if (typeof (objectData.selected) !== "undefined") {
				var product = adobeDataLayerJS.initializeProductObject();
				product.productInfo.ID.push(objectData.selected.skuPartNumber);
				product.productInfo.wcsid.push(objectData.selected.skuCatentryId);
				product.productInfo.stockStatus.push(objectData.selected.inventory.inventoryStatus);
				product.productInfo.quantity.push(parseFloat(objectData.selected.inventory.availableQuantity));
				product.productInfo.currency.push(objectData.selected.price[0].currency);
				if (objectData.selected.price[0].currency == "INR") {
					for (var j = 0; j < objectData.selected.price.length; j++) {
						if (objectData.selected.price[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(objectData.selected.price[j].value));
						}
					}
					for (var j = 0; j < objectData.selected.price.length; j++) {
						if (objectData.selected.price[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(objectData.selected.price[j].value));
							product.productInfo.totalPrice.push(parseFloat(objectData.selected.price[j].value));
						}
					}
					product.productInfo.discount.push(parseFloat(objectData.selected.amountSaved.amountSaved));
					for (var j = 0; j < objectData.selected.alternativePrice.length; j++) {
						if (objectData.selected.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(objectData.selected.alternativePrice[j].value));
						}
					}
					for (var j = 0; j < objectData.selected.alternativePrice.length; j++) {
						if (objectData.selected.alternativePrice[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(objectData.selected.alternativePrice[j].value));
						}
					}
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(objectData.selected.amountSaved.amountSaved)));
				} else {
					for (var j = 0; j < objectData.selected.price.length; j++) {
						if (objectData.selected.price[j].usage === "Display") {
							product.productInfo.actualPriceUSD.push(parseFloat(objectData.selected.price[j].value));
						}
					}
					for (var j = 0; j < objectData.selected.price.length; j++) {
						if (objectData.selected.price[j].usage === "Offer") {
							product.productInfo.totalPriceUSD.push(parseFloat(objectData.selected.price[j].value));
						}
					}
					product.productInfo.discountUSD.push(parseFloat(objectData.selected.amountSaved.amountSaved));
					for (var j = 0; j < objectData.selected.alternativePrice.length; j++) {
						if (objectData.selected.alternativePrice[j].usage === "Display") {
							product.productInfo.actualPrice.push(parseFloat(objectData.selected.alternativePrice[j].value));

						}
					}
					for (var j = 0; j < objectData.selected.alternativePrice.length; j++) {
						if (objectData.selected.alternativePrice[j].usage === "Offer") {
							product.productInfo.price.push(parseFloat(objectData.selected.alternativePrice[j].value));
							product.productInfo.totalPrice.push(parseFloat(objectData.selected.alternativePrice[j].value));

						}
					}
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(objectData.selected.amountSaved.amountSaved)));
				}

				product.productInfo.url.push(objectData.producturl);
				product.productInfo.brand.push(adobeDataLayerJS.getMotherBrand(productViewData.catalogEntryView[0]));
				var arr = objectData.producturl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
				if ((productViewData.catalogEntryView[0].txBrand == "Tanishq" || productViewData.catalogEntryView[0].txBrand == "Mia") && objectData.selected.attributes != "undefined") {
					if (objectData.selected.attributes[0].name == "Gross Weight") {
						product.productInfo.weight.push(objectData.selected.attributes[0].values[0].value);
					}
				}
				product.productInfo.parentID.push(productViewData.catalogEntryView[0].partNumber);
				product.productInfo.name.push(productViewData.catalogEntryView[0].name);
				if (typeof (productViewData.catalogEntryView[0].facetAttributes.Product) != "undefined") {
					product.productInfo.category.push(productViewData.catalogEntryView[0].facetAttributes.Product);
				}
				else {
					for (var j = 0; j < productViewData.catalogEntryView[0].techSpecsAttributes.length; j++) {
						if (productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY" ||
							productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "JW_TX_CATEGORY" ||
							productViewData.catalogEntryView[0].techSpecsAttributes[j].identifier === "TA_TX_PRODUCT_CATEGORY") {
							product.productInfo.category.push(productViewData.catalogEntryView[0].techSpecsAttributes[j].values[0].value);
						}
					}
				}
				return product;
			}
		},
		//Quick View: Add to Compare
		addToCompareResponse: function (response, productViewData) {
			if (typeof (productViewData) != "undefined") {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.product = {};
				var product = adobeDataLayerJS.initializeProductObject();
				for (var i = 0; i < productViewData.catalogEntryView.length; i++) {
					if (productViewData.catalogEntryView[i].uniqueID == response.uniqueId) {
						var catalogEntryView = productViewData.catalogEntryView[i];
						product.productInfo.url.push(catalogEntryView.productUrl.split('?')[0]);
						product.productInfo.name.push(catalogEntryView.name);
						product.productInfo.parentID.push(catalogEntryView.partNumber);
						var arr = catalogEntryView.thumbnail.split("/");
						var prdBrand = arr[3];
						product.productInfo.brand.push(prdBrand);
						if (catalogEntryView.oosFlag == "Available") {
							product.productInfo.quantity.push(1);
						} else {
							product.productInfo.quantity.push(0);
						}
						//If currency is INR/USD
						var currencyVal = "";
						for (var j = 0; j < catalogEntryView.price.length; j++) {
							currencyVal = catalogEntryView.price[j].currency;
						}
						product.productInfo.currency.push(currencyVal);
						if (currencyVal == "INR") {
							//When currency selector is INR
							for (var j = 0; j < catalogEntryView.price.length; j++) {
								if (catalogEntryView.price[j].usage === "Display") {
									product.productInfo.actualPrice.push(parseFloat(catalogEntryView.price[j].value));
									product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(catalogEntryView.price[j].value)));
								}
							}
							for (var j = 0; j < catalogEntryView.price.length; j++) {
								if (catalogEntryView.price[j].usage === "Offer") {
									product.productInfo.price.push(parseFloat(catalogEntryView.price[j].value));
									product.productInfo.totalPrice.push(parseFloat(catalogEntryView.price[j].value));
									product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(catalogEntryView.price[j].value)));
								}
							}
							product.productInfo.discount.push(parseFloat(catalogEntryView.amountSaved.amountSaved));
							product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(catalogEntryView.amountSaved.amountSaved)));
						} else {
							//When currency selector is USD
							for (var j = 0; j < catalogEntryView.price.length; j++) {
								if (catalogEntryView.price[j].usage === "Display") {
									product.productInfo.actualPriceUSD.push(parseFloat(catalogEntryView.price[j].value));
									product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(parseFloat(catalogEntryView.price[j].value)));
								}
							}
							for (var j = 0; j < catalogEntryView.price.length; j++) {
								if (catalogEntryView.price[j].usage === "Offer") {
									product.productInfo.totalPriceUSD.push(parseFloat(catalogEntryView.price[j].value));
									product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(catalogEntryView.price[j].value)));
									product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(catalogEntryView.price[j].value)));
								}
							}
							product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(catalogEntryView.amountSaved.amountSaved)));
							product.productInfo.discountUSD.push(parseFloat(catalogEntryView.amountSaved.amountSaved));
						}
						product.productInfo.ID.push(catalogEntryView.child_partNumber_ntk);
						product.productInfo.wcsid.push(catalogEntryView.catentry_id_child);

						product.productInfo.stockStatus.push(catalogEntryView.available);
						var arr = catalogEntryView.productUrl.split("/");
						var domain = arr[0] + "//" + arr[2];
						product.productInfo.domain.push(domain);
						product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
					}
				}
				adobeDataLayerJS.adobeDigitalData.product = product;
				if (response.page == 'qv') {
					_satellite.track('QW_Add_To_Compare');
				} else {
					_satellite.track('Compare_Products');
				}
			}
		},
		//Product Details Page: Track currency selection
		pdpCurrencySelectorHandler: function (pdpCurrencyChangeEventRes, productViewData) {
			adobeDataLayerJS.adobeDigitalData.product = {};
			adobeDataLayerJS.adobeDigitalData.product = adobeDataLayerJS.constructProductObject(productViewData);
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.page.pageInfo.currency = pdpCurrencyChangeEventRes.priceCurrency;
			_satellite.track('PDP_Currency_Selection');
		},
