				adobeDataLayerJS.paymentSuccessEventResponse(analyticsJS.paymentSuccessEventData);
			}
			//2. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.paymentSuccessEventResponse(analyticsJS.paymentSuccessEventData);
			}
		},
		//Checkout - Payment failure section
		paymentFailureHandler: function(paymentFailureEventResponseData){
			analyticsJS.paymentFailureEventData = paymentFailureEventResponseData;
			//1. ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.paymentFailureEventResponse(analyticsJS.paymentFailureEventData);
			}
			//2. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.paymentFailureEventResponse(analyticsJS.paymentFailureEventData);
			}
		},
		//Checkout - Thank you page
		orderSummaryHandler: function (orderSummaryEventResponseData) {
			analyticsJS.orderSummaryEventData = orderSummaryEventResponseData;

			// 1.ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.orderPlacedEventResponse(analyticsJS.orderSummaryEventData);
			}
			//1. IBM DDX Handler invocation
			// if (analyticsJS.isCoremetricsEnabled && typeof (ddxDataLayerJS) != "undefined") {
			// 	ddxDataLayerJS.orderSummaryEventResponse(analyticsJS.orderSummaryEventData);
			// }

			//GA handler invocation
			if (analyticsJS.isGAEnabled && typeof (gaDataLayerJS) != "undefined") {
				gaDataLayerJS.orderSummaryEventResponse(analyticsJS.orderSummaryEventData);
				// checkout_product event 
				gaDataLayerJS.orderSummaryItemEventResponse(analyticsJS.orderSummaryEventData);
				// checkout_ order placed
				gaDataLayerJS.orderPlacedEventResponse(analyticsJS.orderSummaryEventData);
			}

			//TODO:2. YFRET Handler invocation
			if (analyticsJS.isYFRETEnabled && typeof (yfretDataLayerJS) != "undefined") {
				yfretDataLayerJS.orderSummaryEventResponse(analyticsJS.orderSummaryEventData);
			}

			// Yotpo Handler invocation
			if (analyticsJS.isYotpoEnabled && typeof (yotpoDataLayerJS) != "undefined") {
				yotpoDataLayerJS.orderSummaryEventResponse(analyticsJS.orderSummaryEventData);
			}

		},

		//"User Registration" event handler
		userRegistrationHandler: function(userRegistrationEventResponseData){
			analyticsJS.userRegistrationEventData = userRegistrationEventResponseData;

			//1. ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.userRegistrationEventResponse(analyticsJS.userRegistrationEventData);
			}
			//TODO:2. YFRET Handler invocation
