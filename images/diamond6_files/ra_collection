				adobeDataLayerJS.userDeliveryInfoEventResponse(analyticsJS.userDeliveryInfoEventData);
			}
			//2. IBM DDX Handler invocation
			if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
				//No DA handler required as of now
			}

			//3. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.userDeliveryInfoEventResponse(analyticsJS.userDeliveryInfoEventData);
			}

		},
		//Checkout - Order review section
		orderReviewHandler: function(orderReviewHandlerResponseData){
			analyticsJS.orderReviewEventData = orderReviewHandlerResponseData;

			//1. ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.parseCartReviewEventResponse(analyticsJS.orderReviewEventData);
			}

			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseCartReviewEventResponse(analyticsJS.orderReviewEventData);
			// }

			//2. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseCartReviewEventResponse(analyticsJS.orderReviewEventData);
			}
		},
		//Checkout - Load Payment info section
		continueToPaymentHandler: function(checkoutPaymentInfoResData){
			//1. ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.usablePaymentInfoEventResponse(analyticsJS.orderReviewEventData);
			}	
		},		
		//Checkout - Payment info section
		usablePaymentInfoHandler: function(usablePaymentInfoEventResponseData){
			analyticsJS.usablePaymentInfoEventData = usablePaymentInfoEventResponseData;

			//1. ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.paymentInformationEventResponse(analyticsJS.usablePaymentInfoEventData,analyticsJS.orderReviewEventData);
			}	

			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.usablePaymentInfoEventResponse(analyticsJS.usablePaymentInfoEventData);
			// }

			//2. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.usablePaymentInfoEventResponse(analyticsJS.usablePaymentInfoEventData);
				gaDataLayerJS.paymentInformationEventResponse(analyticsJS.usablePaymentInfoEventData);
			}		
		},
		//Checkout - Payment success section
		paymentSuccessEventHandler: function(paymentSuccessEventResponseData){
