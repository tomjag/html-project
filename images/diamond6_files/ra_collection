		},
		digiGoldSellInEventResponse: function (responseSellValueData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObjectForDigiGold(adobeDataLayerJS.adobeDigitalData.page);
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.digigold={};
			if(responseSellValueData.sellType=="inr"){
			adobeDataLayerJS.adobeDigitalData.digigold.sellin ="Sell in Rupees";
			_satellite.track('Digi_Gold_Sell_in_Rupees');
			}
			else{
				adobeDataLayerJS.adobeDigitalData.digigold.sellin="Sell in gram";
				_satellite.track('Digi_Gold_Sell_in_Grams');

			}
			
			
		},
		digiGoldProceedToSellEventResponse: function (responseSellData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObjectForDigiGold(adobeDataLayerJS.adobeDigitalData.page);
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.digigold = {};
			if(typeof(responseSellData)!="undefined"){
				if(responseSellData.sellType=='inr'){
					adobeDataLayerJS.adobeDigitalData.digigold.sellValuerupees = responseSellData.sell_price;
				}else{
					adobeDataLayerJS.adobeDigitalData.digigold.sellValuegrams = responseSellData.sell_gold_amount;
				}
			}
		
			_satellite.track('Digi_Gold_Proceed_to_Sell');
		},
		digiGoldConfirmSellEventResponse: function (catalogEntryView) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObjectForDigiGold(adobeDataLayerJS.adobeDigitalData.page);
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('Digi_Gold_Confirm_Sell');
			
		},
		digiGoldConfirmBuyEventResponse: function (catalogEntryView) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObjectForDigiGold(adobeDataLayerJS.adobeDigitalData.page);
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('Digi_Gold_Confirm_Payment');
			
		},
		setPageObjectForDigiGold: function (pageObject) {
			 if (window.location.href.includes("/digigold/buy")) {
				pageObject.pageInfo.pageName = "tq-digigold-buy-page";
			} else if (window.location.href.includes("/digigold/sell")) {
				pageObject.pageInfo.pageName = "tq-digigold-sell-page";
			} else if (window.location.href.includes("/digigold-account")) {
				pageObject.pageInfo.pageName = "tq-digigold-account-page";
			} else if (window.location.href.includes("/user-kyc")) {
				pageObject.pageInfo.pageName = "tq-digigold-kyc-page";
			}
						
			pageObject.pageInfo.pageType = pageObject.pageInfo.pageName.slice(pageObject.pageInfo.pageName.indexOf("-") + 1, pageObject.pageInfo.pageName.length);
			pageObject.pageInfo.section = pageObject.pageInfo.pageType;
			return pageObject;
		},
		mirrarJewllerySelectedEventResponse: function (res) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.virtualtryon = {};
			adobeDataLayerJS.adobeDigitalData.virtualtryon.products = res.data.productCode;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.categories = res.data.category;
			_satellite.track('virtual_tryon_products');
		},
		mirrarImageCaptureEventResponse: function (res) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.virtualtryon = {};
			adobeDataLayerJS.adobeDigitalData.virtualtryon.products = res.data.productCode;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.categories = res.data.category;
			_satellite.track('virtual_tryon_image_capture');
		},
		mirrarUIElementClickedEventResponse: function (res) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.virtualtryon = {};
			adobeDataLayerJS.adobeDigitalData.virtualtryon.products = res.data.productCode;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.categories = res.data.category;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.element = res.data.element;
			_satellite.track('virtual_tryon_elements');
		},
		mirrarPopupCloseEventResponse: function () {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			_satellite.track('virtual_tryon_closed');
		},
		mirrarAddToCartEventResponse: function (res) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			adobeDataLayerJS.adobeDigitalData.virtualtryon = {};
			adobeDataLayerJS.adobeDigitalData.virtualtryon.products = res.data.productCode;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.categories = res.data.category;
			adobeDataLayerJS.adobeDigitalData.virtualtryon.element = res.data.element;
			adobeDataLayerJS.adobeDigitalData.page.cart = {};
			adobeDataLayerJS.adobeDigitalData.page.cart.source = "MirrAR";
			_satellite.track('virtual_tryon_add_to_cart');
		},
		init: function (initProperties) {
			if (typeof (initProperties) != "undefined") {
				for (var i = 0; i < initProperties.length; i++) {
					adobeDataLayerJS[initProperties[i].name] = initProperties[i].value;
				}
			}
			adobeDataLayerJS.createAdobeDataLayer();
		}
	}
}
}catch(e){console.log("Module 'st_titan': ",e);}
try{if(typeof(analyticsJS) == "undefined" || !analyticsJS ){
	analyticsJS={
		isCoremetricsEnabled: false,
		isYFRETEnabled: false,
		isGAEnabled : false,
		isAdobeAnalyticsEnabled: false,
		productViewData: null,
		cartData: null,
		internalSearch:false,
		flattenedCategoryList: {},
		categoryViewHandler: function(){

		},
		homePageHandler: function(){
			//1. ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.homePageHandler();
			}
			//GA handler invocation
			if (analyticsJS.isGAEnabled && typeof (gaDataLayerJS) != "undefined") {
				gaDataLayerJS.homePageHandler();
			}
		},
		//PLP page
		productviewByTXCategoryHandler: function(productViewResponse){
			analyticsJS.productViewData = productViewResponse;
			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseProductViewByCategory(analyticsJS.productViewData);
			// }
			//2. schema.org handler invocation
			schemaJS.parseProductViewByCategory(analyticsJS.productViewData);

			//3. YFRET Handler invocation
			if(analyticsJS.isYFRETEnabled && typeof(yfretDataLayerJS) != "undefined"){
				yfretDataLayerJS.parseProductViewByCategory(analyticsJS.productViewData);
			}

			//GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseProductViewByCategory(analyticsJS.productViewData);
			}
		},
		//Search page
		productviewByTXSearchTermHandler: function(productViewResponse){
			analyticsJS.productViewData = productViewResponse;

			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseProductViewBySearchTerm(analyticsJS.productViewData);
			// }
			//GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseProductViewBySearchTerm(analyticsJS.productViewData);
			}
		},
		//Search Event Handler 
		searchHandler: function(productViewResponse){
			analyticsJS.productViewData = productViewResponse;
			//1. ADOBE Handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				analyticsJS.internalSearch = true;
				adobeDataLayerJS.parseProductViewBySearchTerm(analyticsJS.productViewData);
			}
		},
		//PDP Page
		productDetailsbyIdHandler: function(productViewResponse){
			analyticsJS.productViewData = productViewResponse;

			//1. ADOBE Handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.parseProductViewByIdData(analyticsJS.productViewData,analyticsJS.internalSearch);
			}
			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseProductViewByIdData(analyticsJS.productViewData);
			// }

			//schema.org handler invocation
			schemaJS.parseProductViewByIdData(analyticsJS.productViewData);

			//TODO:2. YFRET Handler invocation
			if(analyticsJS.isYFRETEnabled && typeof(yfretDataLayerJS) != "undefined"){
				yfretDataLayerJS.parseProductViewByIdData(analyticsJS.productViewData);
			}

			//GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseProductViewByIdData(analyticsJS.productViewData);
			}

		},
		//Size Change Handler
		sizeChangeHandler: function(selectedSize){
			analyticsJS.productViewData.selected = selectedSize;
		},
		//videoCapture
		videoAnalyticsHandler: function(videoResponse){
			analyticsJS.videoData = videoResponse;
			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 		ddxDataLayerJS.parseVideoAnalyticsData(analyticsJS.videoData);
			// }
		},
		//video Play
		videoAnalyticsPlayHandler: function(videoResponse){
			analyticsJS.videoData = videoResponse;
			// 1. ADOBE Handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.parseVideoAnalyticsData(analyticsJS.videoData,analyticsJS.productViewData);
			}
		},
		//Quick View - on PLP, PDP, Search
		quickViewHandler: function(productViewResponse){
			analyticsJS.quickViewData = productViewResponse;

			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseQuickViewData(analyticsJS.quickViewData);
			// }

			//1. ADOBE Handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.parseQuickViewData(analyticsJS.quickViewData,analyticsJS.productViewData);
			}
			
			//GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseQuickViewData(analyticsJS.quickViewData);
			}
		},
		//Quick View - Site-wide
		addToCartHandler: function(addToCartEventResponseData){
			analyticsJS.addToCartEventData = addToCartEventResponseData;

			//1. ADOBE Handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				var productViewResponse  = analyticsJS.quickViewData || analyticsJS.productViewData;
				adobeDataLayerJS.parseAddToCartEventResponse(analyticsJS.addToCartEventData, productViewResponse);
			}
			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	var productViewResponse  = analyticsJS.quickViewData || analyticsJS.productViewData;
			// 	ddxDataLayerJS.parseAddToCartEventResponse(analyticsJS.addToCartEventData, productViewResponse);
			// }
			
			//2. YFRET Handler invocation
			if(analyticsJS.isYFRETEnabled && typeof(yfretDataLayerJS) != "undefined"){
				var productViewResponse  = analyticsJS.quickViewData || analyticsJS.productViewData;
				yfretDataLayerJS.parseAddToCartEventResponse(analyticsJS.addToCartEventData, productViewResponse);
			}

			//3. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				var productViewResponse  = analyticsJS.quickViewData || analyticsJS.productViewData;
				gaDataLayerJS.parseAddToCartEventResponse(analyticsJS.addToCartEventData, productViewResponse);
			}

		},
		//Add to Cart - Tangiblee
		tangibleeAddToCartHandler: function(eventResponse){
			//1. ADOBE Handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				var productViewResponse = analyticsJS.quickViewData || analyticsJS.productViewData;
				adobeDataLayerJS.tangibleeAddToCartEventHandler(eventResponse, productViewResponse);
			}	
		},
		//Mini cart View - Site-wide
		miniCartViewHandler: function(miniCartViewEventResponseData){

			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseMiniCartViewEventResponse(miniCartViewEventResponseData);
			// }

			//1. ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				var productViewResponse  = analyticsJS.quickViewData || analyticsJS.productViewData;
				adobeDataLayerJS.parseMiniCartViewEventResponse(miniCartViewEventResponseData,productViewResponse);
			}

		},
		//Cart page
		cartViewHandler: function(cartViewEventResponseData){
			analyticsJS.cartViewEventData = cartViewEventResponseData;
			//1. IBM DDX Handler invocation
			// if(analyticsJS.isCoremetricsEnabled && typeof(ddxDataLayerJS) != "undefined"){
			// 	ddxDataLayerJS.parseCartViewEventResponse(cartViewEventResponseData);
			// }
			//1. ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.parseCartViewEventResponse(analyticsJS.cartViewEventData , analyticsJS.productViewData);
			}
			//3. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.parseCartViewEventResponse(analyticsJS.cartViewEventData , analyticsJS.productViewData);
			}
		},

		// On Increase / Decrease quantity and Remove items from cart
		updateCartHandler: function(updateCartEventResponseData){
			analyticsJS.updateCartEventData = updateCartEventResponseData;
			//1. ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.updateCartEventResponse(analyticsJS.cartViewEventData, analyticsJS.updateCartEventData);
			}
			//2. YFRET Handler invocation
			if(analyticsJS.isYFRETEnabled && typeof(yfretDataLayerJS) != "undefined"){
				yfretDataLayerJS.updateCartEventResponse(analyticsJS.cartViewEventData, analyticsJS.updateCartEventData);
			}

			//3. GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.updateCartEventResponse(analyticsJS.cartViewEventData, analyticsJS.updateCartEventData);
				//gaDataLayerJS.cartEventResponse(analyticsJS.cartViewEventData, analyticsJS.updateCartEventData);
			}
			
		},
		// Sign In or Checkout as guest section
		loginOrCheckoutAsGuestHandler: function(loginOrCheckoutAsGuestEventData){
			analyticsJS.loginOrCheckoutAsGuestEventData = loginOrCheckoutAsGuestEventData;
			// 1. ADOBE handler invocation
			if (analyticsJS.isAdobeAnalyticsEnabled && typeof (adobeDataLayerJS) != "undefined") {
				adobeDataLayerJS.loginOrCheckoutAsGuestEventResponse(analyticsJS.loginOrCheckoutAsGuestEventData, analyticsJS.cartViewEventData);
			}
			// GA handler invocation
			if (analyticsJS.isGAEnabled && typeof (gaDataLayerJS) != "undefined") {
				gaDataLayerJS.loginOrCheckoutAsGuestEventResponse(analyticsJS.loginOrCheckoutAsGuestEventData);
			}
		},

