        bookAnAppointmentEventResponse: function(bookAppointmentEventData,productViewData){
			var id = "book-an-appointment";
//			var component = {};
//			component.componentInfo = {};
//			component.componentInfo.componentID = ddxDataLayerJS.brandCode + "-" + id;
//			component.category = {};
//			component.category.primaryCategory = "";
            var catalogEntryView;
			var exploreAttributes = {};

//			exploreAttributes['PAGEID'] = ddxDataLayerJS.digitalData.page.pageInfo.pageID;
			exploreAttributes['NAME'] = bookAppointmentEventData.name;
			exploreAttributes['EMAIL'] = bookAppointmentEventData.emailId;
			exploreAttributes['PHONE'] = bookAppointmentEventData.phone;
			exploreAttributes['PINCODE'] = bookAppointmentEventData.pin;
			exploreAttributes['CITY'] = bookAppointmentEventData.city;
			exploreAttributes['STORE'] = bookAppointmentEventData.storeName;
			exploreAttributes['PREFFERED_DATE'] = bookAppointmentEventData.appointmentDate;            
            
			if(typeof(productViewData) != "undefined" && productViewData != null && productViewData.catalogEntryView.constructor == Array && productViewData.catalogEntryView.length > 0){
				 catalogEntryView = productViewData.catalogEntryView["0"];

			}

			if(typeof(catalogEntryView) != "undefined"){
				exploreAttributes['BRAND'] = catalogEntryView.txBrand;
				
				if(typeof(catalogEntryView.facetAttributes.Product) != "undefined"){
				exploreAttributes['PRODUCT_CATEGORY'] = catalogEntryView.facetAttributes.Product;
				}else{
				exploreAttributes['PRODUCT_CATEGORY'] = "Watches";
				}
				
				if(typeof(analyticsJS.productViewData.catalogEntryView[0].child_partNumber_ntk) != "undefined"){
					exploreAttributes['SKU'] = catalogEntryView.child_partNumber_ntk;
				}else if(catalogEntryView.sKUs !== null){
					exploreAttributes['SKU'] = catalogEntryView.sKUs[0].skuPartNumber;
				}
				else{
					exploreAttributes['SKU'] = catalogEntryView.sKUs[0].partNumber;
				}
//				exploreAttributes['PRODUCT_NAME'] = catalogEntryView.name;
			}
		
//			component.attributes = {};
//			component.attributes.exploreAttributes = ddxDataLayerJS.generateExploreAttributesForComponents(exploreAttributes);
//			ddxDataLayerJS.addToComponentObject(component);
            
            var event = ddxDataLayerJS.createEventObject(ddxDataLayerJS.brandCode + "-" + id,"2","500", ddxDataLayerJS.digitalData.page.pageInfo.pageID);
            event.attributes.exploreAttributes = ddxDataLayerJS.generateExploreAttributesForConversionTag(exploreAttributes);
			ddxDataLayerJS.addToEventObject(event);
		},
