				if (typeof (catalogEntryView.parentCatalogGroupID) != "undefined" && typeof (window.flattenedCategoryList) != "undefined") {
					for (var i = 0; i < catalogEntryView.parentCatalogGroupID.length; i++) {
						var parentCategoryID = catalogEntryView.parentCatalogGroupID[i].split("_")[1];
						if (typeof (window.flattenedCategoryList[parentCategoryID]) != "undefined" && window.flattenedCategoryList[parentCategoryID].leafCategory == "true") {
							detail.actionField.list = window.flattenedCategoryList[parentCategoryID].identifier;
						}
					}
				}

				detail.products = [];
				var product = {};
				product.id = catalogEntryView.child_partNumber_ntk;
				product.name = catalogEntryView.name;
				product.brand = gaDataLayerJS.brand;
				product.category = typeof (detail.actionField.list) !== "undefined" && detail.actionField.list !== "" ? detail.actionField.list : "";
				product.list = typeof (gaDataLayerJS.list) !== "undefined" && gaDataLayerJS.list !== "" ? gaDataLayerJS.list : "";
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					if (catalogEntryView.price[j].usage === "Offer") {
						product.price = catalogEntryView.price[j].value;
					}
				}
				detail.products.push(product);
				dataLayer.ecommerce.detail = detail;
				gaDataLayerJS.dataLayer.push(dataLayer);
			}

			//Gamooga
			if (typeof (quickViewData) != "undefined") {
				var dataLayer = {};
				dataLayer.event = "quick_view";
				var catalogEntryView = quickViewData.catalogEntryView["0"];
				dataLayer.name = catalogEntryView.name;
				//dataLayer.category = typeof (detail.actionField.list) !== "undefined" && detail.actionField.list !== "" ? detail.actionField.list : "";
				dataLayer.sku = catalogEntryView.sKUs[0].skuPartNumber
				dataLayer.brand = gaDataLayerJS.brand;
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					if (catalogEntryView.price[j].usage === "Display") {
						dataLayer.mrp = parseFloat(catalogEntryView.price[j].value);
					}
				}
				for (var j = 0; j < catalogEntryView.price.length; j++) {
					if (catalogEntryView.price[j].usage === "Offer") {
						dataLayer.price = parseFloat(catalogEntryView.price[j].value)	;
					}
				}

				dataLayer.produrl = catalogEntryView.productUrl.split('?')[0];
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();

				if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
					for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
						if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
							dataLayer.weight_in_gm = catalogEntryView.sKUs[0].attributes[j].values[0].value;
						}
					}
				}

				if(typeof(catalogEntryView.facetAttributes.Product) != "undefined"){
				dataLayer.category = catalogEntryView.facetAttributes.Product;
				}
				else{
					for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
						if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY") {
							dataLayer.category = catalogEntryView.techSpecsAttributes[j].values[0].value;
						}
					}
				}

				dataLayer.tags_ribbons = [];
				if (typeof catalogEntryView.ribbons != "undefined") {
					for (var j = 0; j < catalogEntryView.ribbons.length; j++) {
						dataLayer.tags_ribbons[j] = catalogEntryView.ribbons[j].values[0].value;
					}
				}
				dataLayer.discount = parseFloat(catalogEntryView.amountSaved.amountSaved);
				dataLayer.image_url = catalogEntryView.thumbnail;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
								if (catalogEntryView.oosFlag == "Available") {
					dataLayer.stock_status = catalogEntryView.oosFlag;
				} else {
					dataLayer.stock_status = catalogEntryView.oosFlag;
				}
				dataLayer.description = catalogEntryView.shortDescription;
				dataLayer.gender = catalogEntryView.facetAttributes.Gender;
				gaDataLayerJS.dataLayer.push(dataLayer);
			}
		},
		//Gamooga book apointment
		bookAppointmentCompletionEventResponse: function (appointmentResponse, productViewData) {
			if (typeof (appointmentResponse) != "undefined") {
				var dataLayer = {};
				dataLayer.event = "book_appointment_submit";
				dataLayer.name = appointmentResponse.userDetails.name;
				dataLayer.email = appointmentResponse.email;
				dataLayer.countryCode = appointmentResponse.userDetails.country;
				dataLayer.mobile = appointmentResponse.phone;
				dataLayer.pinocde = appointmentResponse.pincode;
				dataLayer.city = appointmentResponse.city;
				dataLayer.store = appointmentResponse.storeName;
				dataLayer.preferred_date = appointmentResponse.appointmentDate;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.url = window.location.href;
				gaDataLayerJS.dataLayer.push(dataLayer);
			}
		},
		parseAddToCartEventResponse: function (addToCartEventData, productViewData) {
			var dataLayer = {}, add = {};
			dataLayer.ecommerce = {};
			add = {};
			add.products = [];
			var product = {};

			if (typeof (addToCartEventData) != "undefined") {
				dataLayer.event = "addToCart";
				if (typeof (addToCartEventData.fromPage) != "undefined" && addToCartEventData.fromPage === "cart") {
					product.id = addToCartEventData.partNumber;
					product.name = addToCartEventData.productDetail.itemName;
					product.brand = gaDataLayerJS.brand;
					product.quantity = addToCartEventData.updatedQuantity;
					product.price = addToCartEventData.unitPrice;
					product.list = typeof (gaDataLayerJS.list) !== "undefined" && gaDataLayerJS.list !== "" ? gaDataLayerJS.list : "";
					add.products.push(product);
				} else {
					product.id = addToCartEventData.orderItemPartnumber;
					product.name = addToCartEventData.orderItemName;
					product.brand = gaDataLayerJS.brand;
					product.quantity = addToCartEventData.orderItemQuantity;
					product.price = addToCartEventData.orderItemOfferPrice;
					product.list = typeof (gaDataLayerJS.list) !== "undefined" && gaDataLayerJS.list !== "" ? gaDataLayerJS.list : "";
					add.products.push(product);
				}
				dataLayer.ecommerce.add = add;
				gaDataLayerJS.dataLayer.push(dataLayer);
			}

			//Gamooga
			if (typeof (addToCartEventData) != "undefined") {
				dataLayer.event = "cart_add";
				if (productViewData !== null) {
					var catalogEntryView = productViewData.catalogEntryView["0"];
					dataLayer.produrl = addToCartEventData.productUrl;
					dataLayer.name = addToCartEventData.orderItemName;
					dataLayer.sku = addToCartEventData.orderItemPartnumber;
					dataLayer.brand = gaDataLayerJS.brand.toLowerCase();
					dataLayer.mrp = parseFloat(addToCartEventData.orderItemListPrice);
					dataLayer.price = parseFloat(addToCartEventData.orderItemOfferPrice);
					dataLayer.discount = parseFloat(addToCartEventData.savingsAmt);

					if (catalogEntryView.oosFlag == "Available") {
						dataLayer.stock_status = catalogEntryView.oosFlag;
					} else {
						dataLayer.stock_status = catalogEntryView.oosFlag;
					}
					for (var j = 0; j < catalogEntryView.price.length; j++) {
						if (catalogEntryView.price[j].usage === "Offer") {
							dataLayer.currency = catalogEntryView.price[j].currency;
						}
					}


					if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
						for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
							if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
								dataLayer.weight_in_gm = catalogEntryView.sKUs[0].attributes[j].values[0].value;
							}
						}
					}

					
				if(typeof(catalogEntryView.facetAttributes.Product) != "undefined"){
					dataLayer.category = catalogEntryView.facetAttributes.Product;
					}
					else{
						for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
							if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY") {
								dataLayer.category = catalogEntryView.techSpecsAttributes[j].values[0].value;
							}
						}
					}
	

					dataLayer.description = catalogEntryView.shortDescription;
					dataLayer.gender = catalogEntryView.facetAttributes.Gender;
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					dataLayer.from_page = document.referrer;
					dataLayer.added_from = pageProperties["analytics.coremetrics.digitaldata.pageInstanceID"];
					dataLayer.website =  gaDataLayerJS.brand.toLowerCase();
					/*dataLayer.added_from = */

					dataLayer.image_url = catalogEntryView.thumbnail;

					/*	if(catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia"){
							dataLayer.category = catalogEntryView.facetAttributes.Product;
						}else{
							dataLayer.category = catalogEntryView.facetAttributes['Product Category']
						}*/

					dataLayer.tags_ribbons = [];
					if (typeof catalogEntryView.ribbons != "undefined") {
						for (var j = 0; j < catalogEntryView.ribbons.length; j++) {
							dataLayer.tags_ribbons[j] = catalogEntryView.ribbons[j].values[0].value;
						}
					}
					dataLayer.quantity = addToCartEventData.orderItemQuantity;
					dataLayer.list = typeof (gaDataLayerJS.list) !== "undefined" && gaDataLayerJS.list !== "" ? gaDataLayerJS.list : "";
					gaDataLayerJS.dataLayer.push(dataLayer);
				}
				else {
					dataLayer.produrl = addToCartEventData.productUrl;
					dataLayer.name = addToCartEventData.orderItemName;
					dataLayer.sku = addToCartEventData.orderItemPartnumber;
					dataLayer.brand = gaDataLayerJS.brand;
					dataLayer.mrp = parseFloat(addToCartEventData.orderItemPrice);
					dataLayer.price = parseFloat(addToCartEventData.orderItemOfferPrice);
					dataLayer.discount = parseFloat(addToCartEventData.savingsPercent);
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					dataLayer.from_page = document.referrer;
					dataLayer.added_from = pageProperties["analytics.coremetrics.digitaldata.pageInstanceID"];
					dataLayer.website =  gaDataLayerJS.brand.toLowerCase();
					dataLayer.image_url = addToCartEventData.orderItemImage;
					dataLayer.quantity = parseFloat(addToCartEventData.orderItemQuantity);
					gaDataLayerJS.dataLayer.push(dataLayer);
				}
			}
		},
		updateCartEventResponse: function (cartViewEventData, updateCartEventData) {
			if (typeof (updateCartEventData) != "undefined" && typeof (updateCartEventData.operation) != "undefined") {
				var dataLayer = {}, remove = {};
				dataLayer.event = "removeFromCart";
				dataLayer.ecommerce = {};
				remove = {};
				remove.products = [];
				var product = {};
				if (typeof (cartViewEventData) !== "undefined") {
					for (var i = 0; i < cartViewEventData.orderSummary.length; i++) {
						if (cartViewEventData.orderSummary[i].orderItemId === updateCartEventData.orderItemId) {
							product.id = cartViewEventData.orderSummary[i].partNumber;
							product.name = cartViewEventData.orderSummary[i].productDetail.itemName;
							product.brand = gaDataLayerJS.brand;
							product.quantity = cartViewEventData.orderSummary[i].quantity - addToCartEventData.updatedQuantity;
							product.price = cartViewEventData.orderSummary[i].unitPrice;
							product.list = typeof (gaDataLayerJS.list) !== "undefined" && gaDataLayerJS.list !== "" ? gaDataLayerJS.list : "";
							remove.products.push(product);
						}
					}
					dataLayer.ecommerce.remove = remove;
					gaDataLayerJS.dataLayer.push(dataLayer);
				}

				//Gamooga
				if (typeof (updateCartEventData) != "undefined" && typeof (updateCartEventData.operation) != "undefined") {
					dataLayer.event = "cart";
					if (typeof (cartViewEventData !== "undefined")) {
						dataLayer.discount = cartViewEventData.orderSummary.discount;
						dataLayer.items = cartViewEventData.recordSetTotal;
						dataLayer.quantity = cartViewEventData.totalQtyCount;
						dataLayer.product_count = cartViewEventData.recordSetCount;
						dataLayer.subtotal = cartViewEventData.orderSummary.subtotal;
						dataLayer.total = cartViewEventData.orderSummary.total;
						dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					}
					gaDataLayerJS.dataLayer.push(dataLayer);
				}
			}
		},
		loginOrCheckoutAsGuestEventResponse: function () {
			var dataLayer = {};
			dataLayer.event = "checkout";
			dataLayer.ecommerce = {};
			dataLayer.ecommerce.checkout = {};
			dataLayer.ecommerce.checkout.actionField = {};
			dataLayer.ecommerce.checkout.actionField.step = 1;
			dataLayer.ecommerce.checkout.actionField.option = "SignIn/Register/Guest";
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		userDeliveryInfoEventResponse: function (userDeliveryInfoEventData) {
			var dataLayer = {};
			dataLayer.event = "checkout";
			dataLayer.ecommerce = {};
			dataLayer.ecommerce.checkout = {};
			dataLayer.ecommerce.checkout.actionField = {};
			dataLayer.ecommerce.checkout.actionField.step = 2;
			dataLayer.ecommerce.checkout.actionField.option = "Delivery Information";
			gaDataLayerJS.dataLayer.push(dataLayer);

			//Gamooga
			if (typeof (userDeliveryInfoEventData) != "undefined") {
				// for (var key in userDeliveryInfoEventData.length) { }
				var userDeliveryInfoData = userDeliveryInfoEventData
				var dataLayer = {};
				dataLayer.event = "delivery_info";
				dataLayer.title = userDeliveryInfoData.personTitle;
				dataLayer.first_name = userDeliveryInfoData.firstName;
				dataLayer.last_name = userDeliveryInfoData.lastName
				dataLayer.email = userDeliveryInfoData.email1;
				dataLayer.mobile = userDeliveryInfoData.mobilePhone1;
				dataLayer.country = userDeliveryInfoData.country
				dataLayer.pincode = userDeliveryInfoData.zipCode;
				dataLayer.state = userDeliveryInfoData.state;
				dataLayer.city = userDeliveryInfoData.city;
				dataLayer.address = userDeliveryInfoData.addressLine;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				gaDataLayerJS.dataLayer.push(dataLayer);
			}

		},
		parseCartReviewEventResponse: function (cartReviewEventData) {
			var dataLayer = {}, checkout = {};
			dataLayer.event = "checkout";
			dataLayer.ecommerce = {};
			checkout = {};
			checkout.actionField = {};
			checkout.actionField.step = 3;
			checkout.actionField.option = "Order Review";

			checkout.products = [];
			var product = {};
			if (typeof (cartReviewEventData) !== "undefined") {
				for (var i = 0; i < cartReviewEventData.orderItem.length; i++) {
					product.id = cartReviewEventData.orderItem[i].partNumber;
					product.name = cartReviewEventData.orderItem[i].productDetail.itemName;
					product.brand = gaDataLayerJS.brand;
					product.quantity = cartReviewEventData.orderItem[i].quantity;
					product.price = cartReviewEventData.orderItem[i].orderItemPrice;
					checkout.products.push(product);
				}
			}

			dataLayer.ecommerce.checkout = checkout;
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//Gamooga orderReview
		parseCartReviewEventResponse: function (cartReviewEventData) {
			var dataLayer = {};
			dataLayer.event = "order_review";
			if (typeof (cartReviewEventData) !== "undefined") {
				for (var i = 0; i < cartReviewEventData.orderItem.length; i++) {
					dataLayer.id = cartReviewEventData.orderItem[i].partNumber;
					dataLayer.name = cartReviewEventData.orderItem[i].productDetail.itemName;
					dataLayer.brand = gaDataLayerJS.brand;
					dataLayer.quantity = parseFloat(cartReviewEventData.orderItem[i].quantity);
					dataLayer.price = parseFloat(cartReviewEventData.orderItem[i].orderItemPrice);
				}
				dataLayer.products_count = cartReviewEventData.orderItem.length;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.discount = parseFloat(cartReviewEventData.orderSummary.discount);
				dataLayer.shipping = cartReviewEventData.orderSummary.shippingCharge;
				dataLayer.subtotal = parseFloat(cartReviewEventData.orderSummary.subtotal);
				dataLayer.total = parseFloat(cartReviewEventData.orderSummary.total);
				dataLayer.cart_items = [];

				for (var j = 0; j < cartReviewEventData.orderItem.length; j++) {
					var cartItemDetails = new Object();
					cartItemDetails.name = cartReviewEventData.orderItem[j].productDetail.itemName;
					cartItemDetails.prodUrl = cartReviewEventData.orderItem[j].productDetail.productUrl;
					cartItemDetails.imageUrl = cartReviewEventData.orderItem[j].productDetail.thumbnailImg;
					cartItemDetails.brand = cartReviewEventData.orderItem[j].productDetail.motherBrand;
					cartItemDetails.price = parseFloat(cartReviewEventData.orderItem[j].orderItemPrice);
					cartItemDetails.currency = cartReviewEventData.orderItem[j].totalAdjustment.currency;
					cartItemDetails.sku = cartReviewEventData.orderItem[j].partNumber;
					cartItemDetails.discount = parseFloat(cartReviewEventData.orderItem[j].totalAdjustment.value);
					cartItemDetails.mrp = parseFloat(cartReviewEventData.orderItem[j].unitPrice);
					dataLayer.cart_items.push(cartItemDetails);
				}

			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		usablePaymentInfoEventResponse: function (usablePaymentInfoEventData) {
			var dataLayer = {};
			dataLayer.event = "checkout";
			dataLayer.ecommerce = {};
			dataLayer.ecommerce.checkout = {};
			dataLayer.ecommerce.checkout.actionField = {};
			dataLayer.ecommerce.checkout.actionField.step = 4;
			dataLayer.ecommerce.checkout.actionField.option = "Payment Information";
			gaDataLayerJS.dataLayer.push(dataLayer);
		},

		orderSummaryEventResponse: function (orderSummaryEventData) {
			var dataLayer = {};
			dataLayer.event = "Purchase Tracking";
			dataLayer.ecommerce = {};
			dataLayer.ecommerce.purchase = {};

			if (typeof (orderSummaryEventData) != "undefined" && typeof (orderSummaryEventData.orderSummary) != "undefined" && orderSummaryEventData.orderSummary.length > 0) {
				var purchase = {}, actionField = {};

				actionField.id = orderSummaryEventData.orderId; // Transaction ID
				actionField.affiliation = gaDataLayerJS.brand;
				actionField.revenue = orderSummaryEventData.totalBreakDown.total;
				actionField.shipping = orderSummaryEventData.totalBreakDown.shippingCharge;
				actionField.tax = '';
				purchase.actionField = actionField;

				purchase.products = [];
				for (var i = 0; i < orderSummaryEventData.orderSummary.length; i++) {
					var product = {};
					product.id = orderSummaryEventData.orderSummary[i].partNumber;
					product.name = orderSummaryEventData.orderSummary[i].name;
					product.price = orderSummaryEventData.orderSummary[i].unitPrice;
					product.quantity = orderSummaryEventData.orderSummary[i].quantity;
					for (var j = 0; j < orderSummaryEventData.orderSummary[i].orderItemExtendAttribute.length; j++) {
						if (orderSummaryEventData.orderSummary[i].orderItemExtendAttribute[j].attributeName == "MotherBrand") {
							product.brand = orderSummaryEventData.orderSummary[i].orderItemExtendAttribute[j].attributeValue
						}
					}
					// product.category = '';
					// product.coupon = '';
					// product.variant = '';
					purchase.products.push(product);
				}
				dataLayer.ecommerce.purchase = purchase;
				gaDataLayerJS.dataLayer.push(dataLayer);
			}

			//Gamooga
			if (typeof (orderSummaryEventData) != "undefined" && typeof (orderSummaryEventData.orderSummary) != "undefined" && orderSummaryEventData.orderSummary.length > 0) {
				var dataLayer = {};
				dataLayer.event = "checkout_success";
				dataLayer.order_id = orderSummaryEventData.orderId;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.total = parseFloat(orderSummaryEventData.totalBreakDown.total);
				dataLayer.payment_mode = orderSummaryEventData.billingInfo[0].payMethodId;
				dataLayer.checkout_type = orderSummaryEventData.billingInfo[0].paymentTab;
				dataLayer.subtotal = parseFloat(orderSummaryEventData.totalBreakDown.subtotal);
				dataLayer.products_count = parseFloat(orderSummaryEventData.orderSummary.length);
				dataLayer.shipping = orderSummaryEventData.totalBreakDown.shippingCharge;
				dataLayer.discount = parseFloat(orderSummaryEventData.totalBreakDown.discount);
				dataLayer.email = orderSummaryEventData.deliveryDetails["contactDetails"].email1;
				dataLayer.mobile = orderSummaryEventData.deliveryDetails["contactDetails"].mobilePhone1;
				dataLayer.website = window.location.hostname;
				dataLayer.url = window.location.href;
				dataLayer.coupon = [];

				for (var j = 0; j < orderSummaryEventData.orderSummary.length; j++) {
					if (typeof orderSummaryEventData.orderSummary[j].adjustment != "undefined") {
						dataLayer.coupon[j] = orderSummaryEventData.orderSummary[j].adjustment[0].code;
					}
				}

				dataLayer.quantity = [];
				for (var j = 0; j < orderSummaryEventData.orderSummary.length; j++) {
					if (typeof orderSummaryEventData.orderSummary[j].quantity != "undefined") {
						dataLayer.quantity[j] = parseFloat(orderSummaryEventData.orderSummary[j].quantity);
					}
				}

				dataLayer.cart_items = [];

				for (var j = 0; j < orderSummaryEventData.orderSummary.length; j++) {
					var cartItemDetails = new Object();
					cartItemDetails.name = orderSummaryEventData.orderSummary[j].name;
					cartItemDetails.prodUrl = orderSummaryEventData.orderSummary[j].productUrl;
					cartItemDetails.imageUrl = orderSummaryEventData.orderSummary[j].thumbnail;
					cartItemDetails.price = parseFloat(orderSummaryEventData.orderSummary[j].orderItemPrice);
					cartItemDetails.currency = orderSummaryEventData.orderSummary[j].totalAdjustment.currency;
					cartItemDetails.sku = orderSummaryEventData.orderSummary[j].partNumber;
					cartItemDetails.discount = parseFloat(orderSummaryEventData.orderSummary[j].totalAdjustment.value);
					cartItemDetails.mrp = parseFloat(orderSummaryEventData.orderSummary[j].unitPrice);
					dataLayer.cart_items.push(cartItemDetails);
				}


				gaDataLayerJS.dataLayer.push(dataLayer);
			}

			var crtoDataLayer = {};
			crtoDataLayer.event = "crto_transactionpage";
			crtoDataLayer.crto = {};
			var userInfo = gaDataLayerJS.fetchUserInfoFromCookie();
			crtoDataLayer.crto.email = typeof userInfo != "undefined" && typeof userInfo.email != "undefined" ? userInfo.email : "";
			crtoDataLayer.crto.products = [];
			if (typeof orderSummaryEventData !== "undefined" && typeof orderSummaryEventData.orderSummary !== "undefined") {
				for (var j = 0; j < orderSummaryEventData.orderSummary.length; j++) {
					crtoDataLayer.crto.products.push({
						id: orderSummaryEventData.orderSummary[j].partNumber,
						price: orderSummaryEventData.orderSummary[j].unitPrice,
						quantity: orderSummaryEventData.orderSummary[j].quantity
					});
				}
			}
			if(gaDataLayerJS.brand == "skinn" || gaDataLayerJS.brand == "mia" || gaDataLayerJS.brand == "sonata" || gaDataLayerJS.brand == "helios" || gaDataLayerJS.brand == "traq") {
				gaDataLayerJS.dataLayer.push(crtoDataLayer);
			}
		},

		//Device Check
		deviceCheck: function () {
			var device;
			if (/Mobi/.test(navigator.userAgent) || /iPad/.test(navigator.userAgent)) {
				device = "M-RWD";
			} else {
				device = "WEB";
			}
			return device
		},
		//SignIn Event
		userSignInEventResponse: function (userSignInEventData) {
			var dataLayer = {};
			if (window.location.href.indexOf("checkout") >= 0){
				dataLayer.event = "checkout_login";
			}
			else{
				dataLayer.event = "signin";
			}
			if (typeof (userSignInEventData !== "undefined")) {
				if (!isNaN(userSignInEventData.username)) {
					dataLayer.mobile = userSignInEventData.username;
				}
				else {
					dataLayer.email = userSignInEventData.username;
				}
				dataLayer.device = gaDataLayerJS.deviceCheck();
				dataLayer.login_type = userSignInEventData.login_type;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
			}
			gaDataLayerJS.dataLayer.push(dataLayer);			
			setTimeout(
				function () {
					if (typeof userSignInEventData.callback != "undefined") {						
						 userSignInEventData.callback() ;
					}
				}, 3000);
		},

		//SignUp Event
		userRegistrationEventResponse: function (userRegistrationEventData) {
			var dataLayer = {};
			dataLayer.event = "signup";
			if (typeof (userRegistrationEventData !== "undefined")) {
				dataLayer.email = userRegistrationEventData.address.email1;
				dataLayer.mobile = userRegistrationEventData.address.mobilePhone1;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.firstName = userRegistrationEventData.address.firstName;
				dataLayer.lastName = userRegistrationEventData.address.lastName;
				dataLayer.title = userRegistrationEventData.personTitle;
				dataLayer.dob = userRegistrationEventData.dateOfBirth;
				dataLayer.anniversary = userRegistrationEventData.anniversaryDate;
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
			setTimeout(
				function () {
					if (typeof userRegistrationEventData.callback != "undefined") {
						typeof userRegistrationEventData.callbackParams != "undefined" ? userRegistrationEventData.callback(userRegistrationEventData.callbackParams) : userRegistrationEventData.callback();
					}
				}, 3000);
		},
		//"Add To Wishlist" event
		addToWishlistEventResponse: function (addToWishListEventData, productViewData) {
			var dataLayer = {};
			dataLayer.event = "wishlist_add";
			var catalogEntryView = productViewData.catalogEntryView["0"];
			dataLayer.produrl = catalogEntryView.productUrl.split('?')[0];
			dataLayer.name = catalogEntryView.name;
			dataLayer.sku = catalogEntryView.sKUs[0].skuPartNumber;
			dataLayer.brand = catalogEntryView.txBrand.toLowerCase();
			dataLayer.discount = parseFloat(catalogEntryView.amountSaved.amountSaved);
			dataLayer.website = catalogEntryView.txBrand.toLowerCase();
			dataLayer.from_page = document.referrer;
			dataLayer.image_url = catalogEntryView.thumbnail;

			if (catalogEntryView.oosFlag == "Available") {
				dataLayer.stock_status = catalogEntryView.oosFlag;
			} else {
				dataLayer.stock_status = catalogEntryView.oosFlag;
			}
			dataLayer.description = catalogEntryView.shortDescription;
			dataLayer.gender = catalogEntryView.facetAttributes.Gender;
			dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
			dataLayer.quantity = parseFloat(catalogEntryView.sKUs[0].inventory.availableQuantity);

			for (var j = 0; j < catalogEntryView.price.length; j++) {
				if (catalogEntryView.price[j].usage === "Display") {
					dataLayer.price = parseFloat(catalogEntryView.price[j].value);
				}
			}

			for (var j = 0; j < catalogEntryView.price.length; j++) {
				if (catalogEntryView.price[j].usage === "Offer") {
					dataLayer.mrp = parseFloat(catalogEntryView.price[j].value);
					dataLayer.currency = catalogEntryView.price[j].currency;
				}
			}

			if (catalogEntryView.txBrand == "Tanishq" || catalogEntryView.txBrand == "Mia") {
				for (var j = 0; j < catalogEntryView.sKUs[0].attributes.length; j++) {
					if (catalogEntryView.sKUs[0].attributes[j].name === "Gross Weight") {
						dataLayer.weight_in_gm = catalogEntryView.sKUs[0].attributes[j].values[0].value;
					}
				}
			}

			dataLayer.tags_ribbons = [];
			if (typeof catalogEntryView.ribbons != "undefined") {
				for (var j = 0; j < catalogEntryView.ribbons.length; j++) {
					dataLayer.tags_ribbons[j] = catalogEntryView.ribbons[j].values[0].value;
				}
			}

			
			if(typeof(catalogEntryView.facetAttributes.Product) != "undefined"){
				dataLayer.category = catalogEntryView.facetAttributes.Product;
			}
				else{
					for (var j = 0; j < catalogEntryView.techSpecsAttributes.length; j++) {
						if (catalogEntryView.techSpecsAttributes[j].identifier === "WT_TX_PRODUCT_CATEGORY") {
							dataLayer.category = catalogEntryView.techSpecsAttributes[j].values[0].value;
						}
					}
				}


			dataLayer.added_from = pageProperties["analytics.coremetrics.digitaldata.pageInstanceID"];

			gaDataLayerJS.dataLayer.push(dataLayer);
		},

		//"Servicability Check" event
		servicabilityCheckEventResponse: function (servicabilityCheckEventData) {
			var dataLayer = {};
			if (typeof (servicabilityCheckEventData) != "undefined") {
				if (servicabilityCheckEventData.page == "cart") {
					dataLayer.event = "pincode_check_cart";
				}
				else {
					dataLayer.event = "pincode_check_product";
					dataLayer.produrl = servicabilityCheckEventData.producturl;
				}
				if (typeof (servicabilityCheckEventData.errMsg) == "undefined") {
					if (servicabilityCheckEventData.select.zipCode != null) {
						dataLayer.pincode = servicabilityCheckEventData.select.zipCode;
					} else {
						dataLayer.pincode = servicabilityCheckEventData.select.pincode;
					}
					dataLayer.sku = servicabilityCheckEventData.sku;
					dataLayer.country = servicabilityCheckEventData.select.countryName;
					dataLayer.cod_delivery = servicabilityCheckEventData.cod;
					dataLayer.website = gaDataLayerJS.brand.toLowerCase();
					if (servicabilityCheckEventData.deliveryInfo[0].deliveryMethodName === "Express Delivery") {
						dataLayer.exp_delivery = servicabilityCheckEventData.deliveryInfo[0].available;
					} else if (servicabilityCheckEventData.deliveryInfo[0].deliveryMethodName === "Standard Delivery") {
						dataLayer.std_delivery = servicabilityCheckEventData.deliveryInfo[0].available;
					}
					if (typeof servicabilityCheckEventData.deliveryInfo[1] != "undefined" && servicabilityCheckEventData.deliveryInfo[1].deliveryMethodName === "Standard Delivery") {
						dataLayer.std_delivery = servicabilityCheckEventData.deliveryInfo[1].available;
					} else if (typeof servicabilityCheckEventData.deliveryInfo[1] != "undefined" && servicabilityCheckEventData.deliveryInfo[1].deliveryMethodName === "Express Delivery") {
						dataLayer.exp_delivery = servicabilityCheckEventData.deliveryInfo[1].available;
					}
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					
					gaDataLayerJS.dataLayer.push(dataLayer);
				} else {
					dataLayer.country = servicabilityCheckEventData.select.countryName;
					dataLayer.sku = servicabilityCheckEventData.sku;
					dataLayer.device = gaDataLayerJS.deviceCheck();
					dataLayer.website = window.location.hostname;
					if (servicabilityCheckEventData.select.zipCode != null) {
						dataLayer.pincode = servicabilityCheckEventData.select.zipCode;
					} else {
						dataLayer.pincode = servicabilityCheckEventData.select.pincode;
					}
					dataLayer.errMsg = servicabilityCheckEventData.errMsg[0].errorMessage;
					gaDataLayerJS.dataLayer.push(dataLayer);
				}
			}
		},
		//"Cart" page
		parseCartViewEventResponse: function (cartViewEventData, productViewData) {
			var dataLayer = {};
			dataLayer.event = "cart_page";
			if (typeof (cartViewEventData !== "undefined")) {
				dataLayer.discount = parseFloat(cartViewEventData.orderSummary.discount);
				dataLayer.quantity = parseFloat(cartViewEventData.totalQtyCount);
				dataLayer.product_count = parseFloat(cartViewEventData.recordSetCount);
				dataLayer.subtotal = parseFloat(cartViewEventData.orderSummary.subtotal);
				dataLayer.total = parseFloat(cartViewEventData.orderSummary.total);
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.cart_items = [];

				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					var cartItemDetails = new Object();
					cartItemDetails.name = cartViewEventData.orderItem[j].productDetail.itemName;
					cartItemDetails.prodUrl = cartViewEventData.orderItem[j].productDetail.productUrl;
					cartItemDetails.brand = cartViewEventData.orderItem[j].productDetail.motherBrand;
					cartItemDetails.imageUrl = cartViewEventData.orderItem[j].productDetail.thumbnailImg;
					cartItemDetails.price = parseFloat(cartViewEventData.orderItem[j].orderItemPrice);
					cartItemDetails.currency = cartViewEventData.orderItem[j].totalAdjustment.currency;
					cartItemDetails.sku = cartViewEventData.orderItem[j].partNumber;
					cartItemDetails.discount = parseFloat(cartViewEventData.orderItem[j].totalAdjustment.value);
					cartItemDetails.mrp = parseFloat(cartViewEventData.orderItem[j].unitPrice);
					dataLayer.cart_items.push(cartItemDetails);
				}
			}
			gaDataLayerJS.dataLayer.push(dataLayer);

			var crtoDataLayer = {};
			crtoDataLayer.event = "crto_basketpage";
			crtoDataLayer.crto = {};
			var userInfo = gaDataLayerJS.fetchUserInfoFromCookie();
			crtoDataLayer.crto.email = typeof userInfo != "undefined" && typeof userInfo.email != "undefined" ? userInfo.email : "";
			crtoDataLayer.crto.products = [];
			if (typeof cartViewEventData !== "undefined") {
				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					crtoDataLayer.crto.products.push({
						id: cartViewEventData.orderItem[j].partNumber,
						price: cartViewEventData.orderItem[j].unitPrice,
						quantity: cartViewEventData.orderItem[j].quantity
					});
				}
			}
			if(gaDataLayerJS.brand == "skinn" || gaDataLayerJS.brand == "mia" || gaDataLayerJS.brand == "sonata" || gaDataLayerJS.brand == "helios" || gaDataLayerJS.brand == "traq"){
				gaDataLayerJS.dataLayer.push(crtoDataLayer);
			}
		},
		//"add_gift_msg" Event
		addEditGiftMessageEventResponse: function (addEditGiftMessageEventData) {
			var dataLayer = {};
			dataLayer.event = "add_gift_msg";
			if (typeof (addEditGiftMessageEventData !== "undefined")) {
				dataLayer.message = addEditGiftMessageEventData.message;
				dataLayer.sku = addEditGiftMessageEventData.sku;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.sender = addEditGiftMessageEventData.from;
				dataLayer.receiver = addEditGiftMessageEventData.receiver;
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//viewPromoCode Event
		viewPromoCodeEventResponse: function (viewPromoCodeEventData) {
			var dataLayer = {};
			dataLayer.event = "view_promo";
			if (typeof (viewPromoCodeEventData !== "undefined")) {
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.status = viewPromoCodeEventData;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.promocodes = []
				for (var j = 0; j < viewPromoCodeEventData.Promotion.length; j++) {
					dataLayer.promocodes[j] = viewPromoCodeEventData.Promotion[j].code;
				}
				gaDataLayerJS.dataLayer.push(dataLayer);
			}
		},
		//applyPromoCode Event
		applyPromoCodeEventResponse: function (applyPromoCodeEventData, cartViewEventData) {
			var dataLayer = {};
			dataLayer.event = "enter_promo";
			if (typeof (applyPromoCodeEventData.promoParams.errMsg) == "undefined") {
				if (typeof (applyPromoCodeEventData !== "undefined")) {
					dataLayer.promocode = applyPromoCodeEventData.promoParams;
					if (applyPromoCodeEventData.promoParams.status === 1) {
						dataLayer.status = "Promotion applied successfully";
					} else if (applyPromoCodeEventData.promoParams.status === 2) {
						dataLayer.status = "Coupon applied successfully";
					}
				}
			}
			else {
				dataLayer.promocode = applyPromoCodeEventData.promoParams;
				dataLayer.status = applyPromoCodeEventData.promoParams.errMsg;
			}
			if (typeof (cartViewEventData !== "undefined")) {
				dataLayer.discount = parseFloat(cartViewEventData.orderSummary.discount);
				dataLayer.subtotal = parseFloat(cartViewEventData.orderSummary.subtotal);
				dataLayer.total = parseFloat(cartViewEventData.orderSummary.total);
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();

			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//proceed_to_checkout Event
		proceedToCheckoutEventResponse: function (proceedToCheckoutEventData, cartViewEventData) {
			var dataLayer = {};
			dataLayer.event = "proceed_to_checkout";
			if (typeof (cartViewEventData !== "undefined")) {
				dataLayer.discount = parseFloat(cartViewEventData.orderSummary.discount);
				dataLayer.quantity = parseFloat(cartViewEventData.totalQtyCount);
				dataLayer.product_count = parseFloat(cartViewEventData.recordSetCount);
				dataLayer.subtotal = parseFloat(cartViewEventData.orderSummary.subtotal);
				dataLayer.total = parseFloat(cartViewEventData.orderSummary.total);
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();

				if (typeof (cartViewEventData.promotionCode !== "undefined")) {
					dataLayer.promocodes = cartViewEventData.promotionCode;

				}

				dataLayer.cart_items = [];

				for (var j = 0; j < cartViewEventData.orderItem.length; j++) {
					var cartItemDetails = new Object();
					cartItemDetails.name = cartViewEventData.orderItem[j].productDetail.itemName;
					cartItemDetails.prodUrl = cartViewEventData.orderItem[j].productDetail.productUrl;
					cartItemDetails.imageUrl = cartViewEventData.orderItem[j].productDetail.thumbnailImg;
					cartItemDetails.brand = cartViewEventData.orderItem[j].productDetail.motherBrand;
					cartItemDetails.price = parseFloat(cartViewEventData.orderItem[j].orderItemPrice);
					cartItemDetails.currency = cartViewEventData.orderItem[j].totalAdjustment.currency;
					cartItemDetails.sku = cartViewEventData.orderItem[j].partNumber;
					cartItemDetails.discount = parseFloat(cartViewEventData.orderItem[j].totalAdjustment.value);
					cartItemDetails.mrp = parseFloat(cartViewEventData.orderItem[j].unitPrice);
					dataLayer.cart_items.push(cartItemDetails);
					
				}

			}
			gaDataLayerJS.dataLayer.push(dataLayer);
			setTimeout(
				function () {
					if (typeof proceedToCheckoutEventData.callback != "undefined") {
						typeof proceedToCheckoutEventData.callbackParams != "undefined" ? proceedToCheckoutEventData.callback(proceedToCheckoutEventData.callbackParams) : proceedToCheckoutEventData.callback();
					}
				}, 3000);
		},
		//"Move To Wishlist" event
		moveToWishlistEventResponse: function (moveToWishlistEventData, productViewData) {
			var dataLayer = {};
			dataLayer.event = "wishlist_move";
			if (typeof (moveToWishlistEventData) != "undefined") {
				dataLayer.produrl = moveToWishlistEventData.orderItem.productDetail.productUrl;
				dataLayer.name = moveToWishlistEventData.orderItem.productDetail.itemName;
				dataLayer.sku = moveToWishlistEventData.orderItem.partNumber;
				dataLayer.brand = gaDataLayerJS.brand;
				dataLayer.price = parseFloat(moveToWishlistEventData.orderItem.unitPrice);

				if (moveToWishlistEventData.orderItem.productDetail.itemStrikeOffPrice != null) {
					dataLayer.mrp = parseFloat(moveToWishlistEventData.orderItem.productDetail.itemStrikeOffPrice);
				} 

				
				dataLayer.image_url = moveToWishlistEventData.orderItem.productDetail.thumbnailImg;
				dataLayer.discount = parseFloat(moveToWishlistEventData.orderItem.productDetail.savingsAmt);
				if (moveToWishlistEventData.orderItem.orderItemInventoryStatus == "Available") {
					dataLayer.stock_status = moveToWishlistEventData.orderItem.orderItemInventoryStatus;
				} else {
					dataLayer.stock_status = moveToWishlistEventData.orderItem.orderItemInventoryStatus;
				}

				dataLayer.description = moveToWishlistEventData.orderItem.productDetail.itemShortDesc;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.from_page = document.referrer;
				dataLayer.gender = moveToWishlistEventData.orderItem.productDetail.gender;
				dataLayer.quantity = parseFloat(moveToWishlistEventData.orderItem.quantity);
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//Store locator Event
		storeLocatorSearchEventResponse: function (storeLocatorSearchEvent) {
			var dataLayer = {};
			dataLayer.event = "store_locator";
			if (typeof (storeLocatorSearchEvent) != "undefined") {
				if (typeof (storeLocatorSearchEvent.errMsg) == "undefined") {
					dataLayer.location = storeLocatorSearchEvent.cityOrZipcode;
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					//dataLayer.type = storeLocatorSearchEvent;
					dataLayer.results = storeLocatorSearchEvent.PhysicalStore;
					dataLayer.website = gaDataLayerJS.brand.toLowerCase();
					if (storeLocatorSearchEvent.store_Type != 0) {
						dataLayer.type = "Service-Center";
					}
					else {
						dataLayer.type = "Store";
					}
					gaDataLayerJS.dataLayer.push(dataLayer);
				} else {
					dataLayer.location = storeLocatorSearchEvent.cityOrZipcode;
					dataLayer.device = gaDataLayerJS.deviceCheck();
					dataLayer.website = window.location.hostname;
					dataLayer.errMsg = storeLocatorSearchEvent.errMsg;
					gaDataLayerJS.dataLayer.push(dataLayer);
				}
			}
		},
		//filterByEvent Event
		filterByEventResponse: function (filterByEvent) {
			var dataLayer = {};
			dataLayer.event = "filter_by";
			if (typeof (filterByEvent) != "undefined") {
				dataLayer.value = filterByEvent.type;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.type = filterByEvent.value;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//sortingFilterEvent Event
		sortingFilterEventResponse: function (sortingFilterEvent) {
			var dataLayer = {};
			dataLayer.event = "sort_by";
			if (typeof (sortingFilterEvent) != "undefined") {
				dataLayer.type = sortingFilterEvent.type;
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				dataLayer.value = sortingFilterEvent.value;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//seeMoreEvent Event
		seeMoreEventResponse: function (seeMoreEvent, productViewData) {
			var dataLayer = {};
			dataLayer.event = "see_more";
			
			if (typeof (seeMoreEvent) != "undefined") {
				dataLayer.url = window.location.href;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
				if (typeof productViewData.breadCrumbTrailEntryView != "undefined" && productViewData.breadCrumbTrailEntryView["1"] != "undefined") {
					dataLayer.category = productViewData.breadCrumbTrailEntryView["1"].label;
				}
			}	
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//order_placed Event
		orderPlacedEventResponse: function (orderSummaryEventData) {
			var dataLayer = {};
			dataLayer.event = "order_placed";
			if (typeof (orderSummaryEventData) != "undefined" && typeof (orderSummaryEventData.orderSummary) != "undefined" && orderSummaryEventData.orderSummary.length > 0) {
				dataLayer.order_id = orderSummaryEventData.orderId;
				dataLayer.payment_mode = orderSummaryEventData.billingInfo[0].payMethodId;
				// dataLayer.checkout_type = 
				// dataLayer.coupon = 
				dataLayer.items = JSON.stringify(orderSummaryEventData.orderSummary);
				dataLayer.device = gaDataLayerJS.deviceCheck();
				dataLayer.quantity = orderSummaryEventData.deliveryDetails.totalOrderQuantity;
				dataLayer.product_count = orderSummaryEventData.deliveryDetails.totalOrderQuantity;
				dataLayer.subtotal = orderSummaryEventData.totalBreakDown.subtotal;
				dataLayer.total = orderSummaryEventData.totalBreakDown.total;
				dataLayer.shipping = parseInt(orderSummaryEventData.totalBreakDown.shippingCharge);
				dataLayer.discount = parseInt(orderSummaryEventData.totalBreakDown.discount);
				dataLayer.email = orderSummaryEventData.deliveryDetails.contactDetails.email1;
				dataLayer.mobile = orderSummaryEventData.deliveryDetails.contactDetails.mobilePhone1;
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//payment_info Event
		paymentInformationEventResponse: function (paymentInformationEventData) {
			var dataLayer = {};
			dataLayer.event = "payment_info";
			if (typeof (paymentInformationEventData) != "undefined") {
				dataLayer.payment_mode = paymentInformationEventData.paymentOption;
				dataLayer.payment_option = paymentInformationEventData.paymentMode;
				dataLayer.device = paymentInformationEventData.device.toLowerCase();
				dataLayer.total = parseFloat(paymentInformationEventData.total);
				dataLayer.encircle_redeem = paymentInformationEventData.encircle;
				dataLayer.giftcard_use = paymentInformationEventData.gitfcardSubmit;
				dataLayer.website = gaDataLayerJS.brand.toLowerCase();
				if (paymentInformationEventData.encircle) {
					for (var j = 0; j < paymentInformationEventData.extradetails.otherPayment.length; j++) {
						if (paymentInformationEventData.extradetails.otherPayment[j].paymentTab === "Encircle") {
							dataLayer.encircle_amount = parseFloat(paymentInformationEventData.extradetails.otherPayment[j].piAmount);
						}
					}
				}
				if (paymentInformationEventData.gitfcardSubmit) {
					for (var j = 0; j < paymentInformationEventData.extradetails.otherPayment.length; j++) {
						if (paymentInformationEventData.extradetails.otherPayment[j].paymentTab === "GiftCard") {
							dataLayer.giftcard_amount = parseFloat(paymentInformationEventData.extradetails.otherPayment[j].piAmount);
						}
					}
				}

			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},
		//payment_success Event
		paymentSuccessEventResponse: function (paymentSuccessEventData) {
			var dataLayer = {};
			dataLayer.event = "payment_success";
			if (typeof (paymentSuccessEventData) != "undefined") {
				dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase;
				dataLayer.payment_mode = paymentInformationEventData.paymentMode;
				dataLayer.payment_option = paymentInformationEventData.paymentOption;
				dataLayer.total = parseFloat(paymentInformationEventData.total);
				dataLayer.website = window.location.hostname;
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},

		//payment_failure Event
		paymentFailureEventResponse: function (paymentFailureEventData) {
			var dataLayer = {};
			dataLayer.event = "payment_failure";
			if (typeof (paymentFailureEventData) != "undefined") {
				dataLayer.device = gaDataLayerJS.deviceCheck();
			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},

		//share-event data
		shareResponse: function (shareEventData) {
			var dataLayer = {};
			dataLayer.event = "share_product";
			dataLayer.url = window.location.href;
			dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
			dataLayer.website = gaDataLayerJS.brand.toLowerCase();
			if (shareEventData === 'Facebook' || shareEventData === 'Twitter' || shareEventData === 'Gplus' || shareEventData === 'Pinterest') {
				dataLayer.medium = shareEventData;
			} else {
				dataLayer.medium = 'Email';
			}
			gaDataLayerJS.dataLayer.push(dataLayer);

		},
		//aDDToCmpare
		addToCompareResponse: function (uniqueId, productViewData) {
			var dataLayer = {};
			dataLayer.event = "add_to_compare";
			dataLayer.uniqueId = uniqueId;
			for (var j = 0; j < productViewData.catalogEntryView.length; j++) {

				if (productViewData.catalogEntryView[j].uniqueID === uniqueId) {

					dataLayer.sku = productViewData.catalogEntryView[j].partNumber;
					dataLayer.brand = gaDataLayerJS.brand;

					for (var i = 0; i < productViewData.catalogEntryView[j].price.length; i++) {

						if (productViewData.catalogEntryView[j].price[i].usage === "Display") {
							dataLayer.mrp = parseFloat(productViewData.catalogEntryView[j].price[i].value);
						}
					}

					for (var i = 0; i < productViewData.catalogEntryView[j].price.length; i++) {
						if (productViewData.catalogEntryView[j].price[i].usage === "Offer") {
							dataLayer.price = parseFloat(productViewData.catalogEntryView[j].price[i].value);
						}
					}

					dataLayer.produrl = productViewData.catalogEntryView[j].productUrl;
					dataLayer.website = gaDataLayerJS.brand.toLowerCase();
					dataLayer.name = productViewData.catalogEntryView[j].name;


					if (typeof (productViewData.catalogEntryView.parentCatalogGroupID) != "undefined" && typeof (window.flattenedCategoryList) != "undefined") {
						for (var i = 0; i < productViewData.catalogEntryView[j].parentCatalogGroupID.length; i++) {
							var parentCategoryID = productViewData.catalogEntryView[j].parentCatalogGroupID[i].split("_")[1];
							if (typeof (window.flattenedCategoryList[parentCategoryID]) != "undefined" && window.flattenedCategoryList[parentCategoryID].leafCategory == "true") {
								dataLayer.category = window.flattenedCategoryList[parentCategoryID].identifier;
							}
						}
					}

					dataLayer.tags_ribbons = [];
					if (typeof productViewData.catalogEntryView.ribbons != "undefined") {
						for (var i = 0; i < productViewData.catalogEntryView[j].ribbons.length; j++) {
							dataLayer.tags_ribbons[i] = productViewData.catalogEntryView[j].ribbons[i].values[0].value;
						}
					}
					dataLayer.discount = parseFloat(productViewData.catalogEntryView[j].amountSaved.amountSaved);
					dataLayer.image_url = productViewData.catalogEntryView[j].fullImage;
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					if (productViewData.catalogEntryView.oosFlag == "Available") {
						dataLayer.stock_status = productViewData.catalogEntryView[j].oosFlag;
					} else {
						dataLayer.stock_status = productViewData.catalogEntryView[j].oosFlag;
					}
					dataLayer.description = productViewData.catalogEntryView[j].shortDescription;
				}
				gaDataLayerJS.dataLayer.push(dataLayer);
			}
		},
		//checkedout_product Event
		orderSummaryItemEventResponse: function (orderSummaryEventData) {
			if (typeof (orderSummaryEventData) != "undefined" && typeof (orderSummaryEventData.orderSummary) != "undefined" && orderSummaryEventData.orderSummary.length > 0) {
				for (var key in orderSummaryEventData.orderSummary) {
					var orderSummaryItemEventData = orderSummaryEventData.orderSummary[key];
					var dataLayer = {};
					dataLayer.event = "checkedout_product";
					dataLayer.category = orderSummaryItemEventData.productCategory;
					dataLayer.price = parseFloat(orderSummaryItemEventData.unitPrice);
					dataLayer.order_id = orderSummaryEventData.orderId;
					dataLayer.total_amount = parseFloat(orderSummaryEventData.totalBreakDown.total);
					dataLayer.sku = orderSummaryItemEventData.partNumber;
					dataLayer.quantity = parseInt(orderSummaryItemEventData.unitQuantity);
					dataLayer.device = gaDataLayerJS.deviceCheck().toLowerCase();
					dataLayer.name = orderSummaryItemEventData.name;
					dataLayer.product_url = orderSummaryItemEventData.productUrl;
					dataLayer.image_url = orderSummaryItemEventData.thumbnail;
					dataLayer.url = window.location.href;
					dataLayer.website = window.location.hostname;
				}
				dataLayer.payment_mode = orderSummaryEventData.billingInfo[0].payMethodId;

			}
			gaDataLayerJS.dataLayer.push(dataLayer);
		},

		fetchUserInfoFromCookie: function () {
			var ca = document.cookie.split(';');
			var cookieName = "UserInfo";
			var userInfoFromCookie = "";
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(cookieName) === 0) {
					userInfoFromCookie = c.substring(
						cookieName.length + 1, c.length);
				}
			}
			if (userInfoFromCookie != "" && userInfoFromCookie.length > 0) {
				var parsedUserInfo = JSON.parse(decodeURIComponent(userInfoFromCookie));
				return parsedUserInfo;
			}
			return;
		},
		init: function (initProperties) {
			if (typeof (initProperties) != "undefined") {
				for (var i = 0; i < initProperties.length; i++) {
					gaDataLayerJS[initProperties[i].name] = initProperties[i].value;
				}
			}
			gaDataLayerJS.createDataLayer();
		}
	}
}
}catch(e){console.log("Module 'st_titan': ",e);}
try{var globalPropertiesList = ({

	FIRSTNAME_MAX_LENGTH: 30,
	LASTNAME_MAX_LENGTH: 30,
	EMAILADDRESS_MAX_LENGTH: 64,
	CITY_MAX_LENGTH: 20,
	STATE_MAX_LENGTH: 50,
	ADDRESS_MAX_LENGTH: 210,
	ADDRESS_SHIPPING_MAX_LENGTH: 300,
	NICKNAME_MAX_LENGTH: 20,
	WISHLISTNAME_MAX_LENGTH: 30,
	ADDITIONALMSG_MAX_LENGTH: 250,
	CARTPROMOCODE_MIN_LENGTH: 1,
	CARTPROMOCODE_MAX_LENGTH: 127,
	CARTQUANTITY_MIN_LENGTH: 1,
	CARTQUANTITY_MAX_LENGTH: 2,
	GIFTMSG_MAX_LENGTH: 250,
	CITYNAME_MAX_LENGTH: 20,
	SHIPPINGINSTRUCTION_MAX_LENGTH: 250,
	PASSWORD_MAX_LENGTH: 16,
	PASSWORD_MIN_LENGTH: 6,
	PDPQUANTITY_MIN_LENGTH: 1,
	PDPQUANTITY_MAX_LENGTH: 2,
	LOCATION_MAX_LENGTH: 30,
	PLPQUANTITY_MIN_LENGTH: 1,
