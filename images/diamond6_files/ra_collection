			_satellite.track('Checkout_Payment_Gift_Carrd_Balance');
		},
		//Common Order Details function
		commonOrderDetails: function (orderReviewEventData) {
			var product = adobeDataLayerJS.initializeProductObject();
			product.productInfo.delMethod = [];
			for (var j = 0; j < orderReviewEventData.orderItem.length; j++) {
				product.productInfo.ID.push(orderReviewEventData.orderItem[j].partNumber);
				product.productInfo.parentID.push(orderReviewEventData.orderItem[j].productDetail.parentPartNumber);
				product.productInfo.wcsid.push(orderReviewEventData.orderItem[j].productId);
				product.productInfo.name.push(orderReviewEventData.orderItem[j].productDetail.itemName);
				product.productInfo.quantity.push(parseFloat(orderReviewEventData.orderItem[j].quantity));
				product.productInfo.currency.push(orderReviewEventData.orderItem[j].currency);				
				//product.productInfo.category.push(adobeDataLayerJS.getCategory(orderReviewEventData.orderItem[j].productDetail.productUrl));
				var price = 0;
				if (orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice != null) {
					price = parseFloat(orderReviewEventData.orderItem[j].productDetail.itemStrikeOffPrice);
				} else {
					price = parseFloat(orderReviewEventData.orderItem[j].unitPrice);
				}
				if (orderReviewEventData.orderItem[j].currency == "INR") {
					product.productInfo.price.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPrice.push(price);
					product.productInfo.totalPrice.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discount.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
					product.productInfo.actualPriceUSD.push(adobeDataLayerJS.convertorUSD(price));
					product.productInfo.totalPriceUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(orderReviewEventData.orderItem[j].orderItemPrice)));
					product.productInfo.discountUSD.push(adobeDataLayerJS.convertorUSD(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
				} else {
					product.productInfo.price.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPrice.push(adobeDataLayerJS.convertorINR(price));
					product.productInfo.totalPrice.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j]))));
					product.productInfo.discount.push(adobeDataLayerJS.convertorINR(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j]))));
					product.productInfo.actualPriceUSD.push(price);
					product.productInfo.totalPriceUSD.push(parseFloat(adobeDataLayerJS.calculateFinalPrice(orderReviewEventData.orderItem[j])));
					product.productInfo.discountUSD.push(parseFloat(adobeDataLayerJS.calculateProductDiscount(orderReviewEventData.orderItem[j])));
				}
				product.productInfo.url.push(orderReviewEventData.orderItem[j].productDetail.productUrl);
				product.productInfo.brand.push(orderReviewEventData.orderItem[j].productDetail.motherBrand);
				product.productInfo.stockStatus.push(orderReviewEventData.orderItem[j].orderItemInventoryStatus);
				product.productInfo.delMethod.push(orderReviewEventData.orderItem[j].selectedDeliveryMethodName);
				var arr = orderReviewEventData.orderItem[j].productDetail.productUrl.split("/");
				var domain = arr[0] + "//" + arr[2];
				product.productInfo.domain.push(domain);
				product.productInfo.platform.push(adobeDataLayerJS.deviceCheck().toLowerCase());
			}
			return product;
		},
		//Home Page:Banner Tracking
		bannerTrackingEventResponse: function (bannerTrackingData) {
			adobeDataLayerJS.adobeDigitalData.page = {};
			adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
			adobeDataLayerJS.adobeDigitalData.account = {};
			adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
			if (bannerTrackingData.type == "Hero Banner") {
				adobeDataLayerJS.adobeDigitalData.page.banner = {};
				var str = bannerTrackingData.name;
				var bannerName = "";
				var mediaType = "";
				if (str.indexOf('.jpg') > 0 || str.indexOf('.png') > 0 || str.indexOf('.jpeg') > 0) {
					if (str.indexOf('.jpg') > 0) {
						mediaType = '.jpg';
					} else if (str.indexOf('.png') > 0) {
						mediaType = '.png';
					} else if (str.indexOf('.jpeg')) {
						mediaType = '.jpeg';
					}
					if (str.indexOf('desktop/') > 0) {
						bannerName = str.substring(str.indexOf('desktop/') + 8, str.indexOf(mediaType) + mediaType.length);
					} else if (str.indexOf('smartphone/') > 0) {
						bannerName = str.substring(str.indexOf('smartphone/') + 11, str.indexOf(mediaType) + mediaType.length);
					} else if (str.indexOf('tablet/') > 0) {
						bannerName = str.substring(str.indexOf('tablet/') + 7, str.indexOf(mediaType) + mediaType.length);
					} else {
						bannerName = str.substring(str.lastIndexOf('/') + 1, str.indexOf(mediaType) + mediaType.length);
					}
				}
				adobeDataLayerJS.adobeDigitalData.page.banner.name = bannerName;
				adobeDataLayerJS.adobeDigitalData.page.banner.position = bannerTrackingData.position;
				_satellite.track('Banner_Clicks');
			} else if (bannerTrackingData.type == "Shop For Category") {
				adobeDataLayerJS.adobeDigitalData.page.shopFor = {};
				adobeDataLayerJS.adobeDigitalData.page.shopFor.category = bannerTrackingData.category;
				_satellite.track('Shop_For_Category');
			} else if (bannerTrackingData.type == "Promotional Banner") {
				adobeDataLayerJS.adobeDigitalData.page.promo = {};
				var str = bannerTrackingData.name;
				var promoBanner = "";
				var mediaType = "";
				if (str.indexOf('.jpg') > 0 || str.indexOf('.png') > 0 || str.indexOf('.jpeg') > 0 || str.indexOf('.mp4') > 0) {
					if (str.indexOf('.jpg') > 0) {
						mediaType = '.jpg';
					} else if (str.indexOf('.png') > 0) {
						mediaType = '.png';
					} else if (str.indexOf('.jpeg') > 0) {
						mediaType = '.jpeg';
					} else if (str.indexOf('.mp4') > 0) {
						mediaType = '.mp4';
					}
					if (str.indexOf('desktop/') > 0) {
						promoBanner = str.substring(str.indexOf('desktop/') + 8, str.indexOf(mediaType) + mediaType.length);
					} else if (str.indexOf('smartphone/') > 0) {
						promoBanner = str.substring(str.indexOf('smartphone/') + 11, str.indexOf(mediaType) + mediaType.length);
					} else if (str.indexOf('tablet/') > 0) {
						promoBanner = str.substring(str.indexOf('tablet/') + 7, str.indexOf(mediaType) + mediaType.length);
					} else {
						promoBanner = str.substring(str.lastIndexOf('/') + 1, str.indexOf(mediaType) + mediaType.length);
					}
				}
				adobeDataLayerJS.adobeDigitalData.page.promo.banner = promoBanner;
				_satellite.track('Promotional_Banner');
			} else if (bannerTrackingData.type == "Collections") {
				adobeDataLayerJS.adobeDigitalData.page.collection = {};
				adobeDataLayerJS.adobeDigitalData.page.collection.category = bannerTrackingData.category;
				adobeDataLayerJS.adobeDigitalData.page.collection.subCat = bannerTrackingData.subCat;
				_satellite.track('Collections_Clicks');
			}
		},
		//Form Tracking: Track Form Start
		trackFormStartEventResponse: function (trackFormStartData) {
			if (trackFormStartData == "bookAppointmentForm") {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				adobeDataLayerJS.adobeDigitalData.page.form = {};
				adobeDataLayerJS.adobeDigitalData.page.form.name = trackFormStartData;
				_satellite.track('Form_Start');
			}
		},
		//Form Tracking: Track Form Submission
		trackFormSubmissionEventResponse: function (trackFormSubmissionData) {
			if (typeof (trackFormSubmissionData !== "undefined")) {
				adobeDataLayerJS.adobeDigitalData.page = {};
				adobeDataLayerJS.adobeDigitalData.page = adobeDataLayerJS.setPageObject();
				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account = adobeDataLayerJS.setAccountObject();
				var loginStatus = adobeDataLayerJS.adobeDigitalData.account.info.loginStatus;
				adobeDataLayerJS.adobeDigitalData.page.form = {};
				adobeDataLayerJS.adobeDigitalData.page.form.name = trackFormSubmissionData.formName;

				adobeDataLayerJS.adobeDigitalData.account = {};
				adobeDataLayerJS.adobeDigitalData.account.info = {};
				adobeDataLayerJS.adobeDigitalData.account.info.loginStatus = loginStatus;
				adobeDataLayerJS.adobeDigitalData.account.info.custID = adobeDataLayerJS.piEncryption(trackFormSubmissionData.storeDetails.emailId);
				adobeDataLayerJS.adobeDigitalData.account.info.mobilenumber = adobeDataLayerJS.piEncryption(trackFormSubmissionData.storeDetails.phone);
				
				_satellite.track('Form_Submit');
			}
		},
