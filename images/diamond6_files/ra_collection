try{schemaJS = {
    constants: {
			"fastrackLogoUrl": "https://staticimg.titan.co.in/Common_Assets/promotion/FastrackLogo-200818.jpg",
            "titanLogoUrl": "home-page"
		},
    parseProductViewByIdData: function (productViewData) {
        var item = {};
        item['@context'] = "http://schema.org";
        item['@type'] = "Product";
        var brandList = ['traq', 'fastrack','titan','Sonata'];
        if(brandList.indexOf(schemaJS.brand) >= 0){
            item.name = productViewData.catalogEntryView[0].name;
            item.image = productViewData.catalogEntryView[0].thumbnail;
            item.description = productViewData.catalogEntryView[0].shortDescription;
            for (var i = 0; i < productViewData.catalogEntryView[0].descriptiveAttributes.length; i++) {
                if (productViewData.catalogEntryView[0].descriptiveAttributes[i].identifier == 'WT_TX_MOTHER_BRAND') {
                    item.brand = productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value;
                } else if (productViewData.catalogEntryView[0].descriptiveAttributes[i].identifier == 'WT_ERP_EAN11') {
                  item.gtin8 = productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value;
              }
            }
            if(productViewData.catalogEntryView[0].sKUs.length >0){
                item.sku = productViewData.catalogEntryView[0].sKUs[0].skuPartNumber;
                // item.gtin8 = productViewData.catalogEntryView[0].sKUs[0].skuPartNumber;
            }
            var offers = {};
            var availability, itemCondition;
            var highprice, lowprice;
            var curr;
            availability = "https://schema.org/OnlineOnly";
            itemCondition = "https://schema.org/NewCondition"
            if (productViewData.catalogEntryView[0].price[1].usage == "Offer") {
                lowprice = productViewData.catalogEntryView[0].price[1].value;
            } else {    
                lowprice = productViewData.catalogEntryView[0].price[0].value;
            }
            curr = productViewData.catalogEntryView[0].price[0].currency;
            highprice = productViewData.catalogEntryView[0].price[0].value;
        
            offers['@type'] = "Offer";
            offers.url = ddxDataLayerJS.getPDPURLPrefix() + productViewData.catalogEntryView[0].urlKeyword;
            offers.priceCurrency = curr;
            offers.price = lowprice;
            offers.priceValidUntil = "9999-12-31";
            offers.availability = availability;
            offers.itemCondition = itemCondition;
            item.offers = offers;
        }
        else {
        item.brand = {};
        item.brand['@type'] = "Brand";
        item.brand['logo'] = schemaJS.constants.titanLogoUrl;

        for (var i = 0; i < productViewData.catalogEntryView[0].descriptiveAttributes.length; i++) {
            if (productViewData.catalogEntryView[0].descriptiveAttributes[i].identifier == 'WT_TX_MOTHER_BRAND') {
                item.brand['name'] = productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value;
            }
			if (productViewData.catalogEntryView[0].descriptiveAttributes[i].identifier == 'JW_TX_MOTHER_BRAND') {
                item.brand['name'] = productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value;
				if(null != productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value && (productViewData.catalogEntryView[0].descriptiveAttributes[i].values[0].value.toLowerCase() == "mia")){
					item.brand['name'] = "Mia by Tanishq" ;
				}
            }
        }
        item.description = productViewData.catalogEntryView[0].shortDescription;
        item.name = productViewData.catalogEntryView[0].name;
        item.image = productViewData.catalogEntryView[0].fullImage;
        item.url = ddxDataLayerJS.getPDPURLPrefix() + productViewData.catalogEntryView[0].urlKeyword;
        //Child SKU partnumber is fetched by traversing through SKU list
        if(productViewData.catalogEntryView[0].sKUs.length >0){
            item.sku = productViewData.catalogEntryView[0].sKUs[0].skuPartNumber;
			 if(schemaJS.brand.toLowerCase()=="tanishq" || schemaJS.brand.toLowerCase()=="mia"){
				item.mpn =  productViewData.catalogEntryView[0].sKUs[0].skuPartNumber;
				item.aggregateRating = {};
				item.aggregateRating['@type'] =  "AggregateRating" ;
				item.aggregateRating['ratingValue'] =  "4.5" ;
				item.aggregateRating['reviewCount'] =  "89" ;
			 }else{
				item.gtin8 = productViewData.catalogEntryView[0].sKUs[0].skuPartNumber;			
			 }
        }
        var offers = {};
        var availability;
        var price;
        var curr;
        if (productViewData.catalogEntryView[0].oosFlag == "Available") {
            availability = "http://schema.org/InStock";
        } else {
            availability = "http://schema.org/OutOfStock";
        }
        if (productViewData.catalogEntryView[0].price[1].usage == "Offer") {
            curr = productViewData.catalogEntryView[0].price[1].currency;
            price = productViewData.catalogEntryView[0].price[1].value;
        } else {
            curr = productViewData.catalogEntryView[0].price[0].currency;
            price = productViewData.catalogEntryView[0].price[0].value;
        }
        offers['@type'] = "Offer";
        offers.availability = availability;
        offers.price = price;
        offers.priceCurrency = curr;
		if(schemaJS.brand=="tanishq" || schemaJS.brand.toLowerCase()=="mia"){
			offers.itemCondition = "New" ;
			offers.priceValidUntil = new Date(currentDate.getFullYear(), currentDate.getMonth(), currentDate.getDate() + 6) ;
			offers.seller = {} ;
			offers.seller['@type'] = "Organization" ;		
			
			if(schemaJS.brand.toLowerCase()=="mia"){
				offers.seller['name'] = "Mia by Tanishq" ;
			}else{
				offers.seller['name'] = schemaJS.brand ;
			}
			
		}
        offers.url = ddxDataLayerJS.getPDPURLPrefix() + productViewData.catalogEntryView[0].urlKeyword;
        item.offers = offers;
    }
		//add mia specific metatags - MIA SEO TASK CR changes
		if(schemaJS.brand.toLowerCase()=="mia"){
		var miaMetags = "<meta property=\"og:type\" content=\"product.item\" />" + "<meta property=\"og:image\" content=\"" +productViewData.catalogEntryView[0].fullImage +"?pView=pdp\" />" + "<meta property=\"product:condition\" content=\"new\" />" ;
		miaMetags += "<meta property=\"og:site_name\" content=\"Mia by Tanishq\" />";
		  if (productViewData.catalogEntryView[0].oosFlag == "Available") {
			  miaMetags += "<meta property=\"product:availability\" content=\"in stock\"/> ";
		  }else{
			  miaMetags += "<meta property=\"product:availability\" content=\"out of stock\"/> ";
		  }
		  miaMetags += "<meta property=\"product:price:amount\" content=\"" + price +"\"/>";
		  miaMetags += "<meta property=\"product:price:currency\" content=\"" + curr +"\"/>";
		  miaMetags += "<meta property=\"product:retailer_item_id\" content=\"" +  item.mpn + "\"/>";                
		document.head.innerHTML += miaMetags ;
		}
		
        var el = document.createElement('script');
        el.type = 'application/ld+json';
        el.text = JSON.stringify(item);
        if(brandList.indexOf(schemaJS.brand) >= 0){
          document.querySelector('body').appendChild(el);
        } else{
          document.querySelector('head').appendChild(el);
        }
        
    },

    parseProductViewByCategory: function (productViewData) {
        var obj = {};
		var catObjName = "";
        obj['@context'] = "http://schema.org";
        obj['@type'] = "ItemList";
        obj.numberOfItems = productViewData.catalogEntryView.length;
        obj.url = document.url;
        for (var i = 0; i < productViewData.catalogEntryView.length; i++) {
            var item = {};
            item['@type'] = "ListItem";
            if(schemaJS.brand == "Sonata" || schemaJS.brand== "traq"){
                item.position = i+1;
                item.url = ddxDataLayerJS.getPDPURLPrefix() + productViewData.catalogEntryView[i].urlKeyword;
                item.name = productViewData.catalogEntryView[i].name;
                catObjName = productViewData.catalogEntryView[i].name;
                if (typeof (obj.itemListElement) != "undefined" && obj.itemListElement.length > 0) {
                    obj.itemListElement.push(item);
                } else {
                    obj.itemListElement = [];
                    obj.itemListElement.push(item);
                }
            }
            else{
                item.position = i;
                item.name = productViewData.catalogEntryView[i].name;
                catObjName = productViewData.catalogEntryView[i].name;
                item.description = productViewData.catalogEntryView[i].shortDescription;
                item.image = productViewData.catalogEntryView[i].fullImage;
                item.url = ddxDataLayerJS.getPDPURLPrefix() + productViewData.catalogEntryView[i].urlKeyword;
                if (typeof (obj.itemListElement) != "undefined" && obj.itemListElement.length > 0) {
                    obj.itemListElement.push(item);
                } else {
                    obj.itemListElement = [];
                    obj.itemListElement.push(item);
                }
            }
        }
        var el = document.createElement('script');
        el.type = 'application/ld+json';
        el.text = JSON.stringify(obj);
        if(schemaJS.brand == "Sonata" || schemaJS.brand== "traq"){
            document.querySelector('body').appendChild(el);
        } else{
            document.querySelector('head').appendChild(el);
        }
        
		 var lowPrice = "1000";
		 var highPrice = "50000";
		 for (var i = 0; i < productViewData.facetView.length; i++) {
			 if(productViewData.facetView[i].name == "OfferPrice_INR_jewellery"){
				for (var i = 0; i < productViewData.facetView[i].entry.length; i++) {
					lowPrice = productViewData.facetView[i].entry[0].customLabel.split('and')[0].replace('Between','');
					highPrice = productViewData.facetView[i].entry[productViewData.facetView[i].entry.length-1].customLabel.split('and')[1];
				}					
			 }
		 }
		 
		 if(schemaJS.brand.toLowerCase()=="mia"){
		var miaMetags = "<meta property=\"og:type\" content=\"article\" />" ;
		miaMetags += "<meta property=\"og:site_name\" content=\"Mia by Tanishq\" />";
		miaMetags += "<meta property=\"og:image\" content=\"https://www.miabytanishq.com/wps/wcm/connect/mia/1654bf33-ee27-4925-8901-463c75099413/Mia-Logo.svg?MOD=AJPERES&CACHEID=ROOTWORKSPACE.Z18_90IA1H80O04180QIVM4BVG04F3-1654bf33-ee27-4925-8901-463c75099413-mUX4RdV\" />"
		                
		document.head.innerHTML += miaMetags ;
		}
		
		 
		if(schemaJS.brand=="tanishq" || schemaJS.brand.toLowerCase()=="mia"){
		var catObj = {} ;
		 catObj['@context'] = "http://schema.org";
         catObj['@type'] = "Product";
		 catObj['description'] = "Category Description";
		 catObj['name'] = catObjName;
		  
		  if(schemaJS.brand.toLowerCase()=="mia"){
				catObj['brand'] = "Mia by Tanishq" ;
			}else{
				catObj['brand'] = schemaJS.brand ;
			}
			
		  var offers = {};
		    offers['@type'] = "AggregateOffer";
			offers['url'] = document.URL;
			offers['priceCurrency'] = "INR" ;
			offers['lowPrice'] = lowPrice ;
			offers['highPrice'] = highPrice ;
			offers['offerCount'] = "1" ;
			catObj.offers = offers ;
			catObj.aggregateRating = {};
			catObj.aggregateRating['@type'] =  "AggregateRating" ;
			catObj.aggregateRating['ratingValue'] =  "4.5" ;
			catObj.aggregateRating['ratingCount'] =  "300" ;
		 var el1 = document.createElement('script');
        el1.type = 'application/ld+json';
        el1.text = JSON.stringify(catObj);
        document.querySelector('head').appendChild(el1);
		}
    },

    parseBreadCrumbOnPages: function (breadCrumbObject) {
        var breadcrumb = {};
        breadcrumb['@context'] = "http://schema.org";
        breadcrumb['@type'] = "BreadcrumbList";
        breadcrumb['itemListElement'] = [];
        var breadCrumbTrailList = breadCrumbObject.list;

        if(schemaJS.brand == "Sonata" || schemaJS.brand== "traq"){
            breadcrumbItemList = {};
            breadcrumbItemList['@type'] = "ListItem";
            breadcrumbItemList['position'] = 1;            
            breadcrumbItemList["name"] = "Home";
            breadcrumbItemList["item"] = window.location.origin;
            
            breadcrumb['itemListElement'].push(breadcrumbItemList);
            for (var i = 0; i < breadCrumbTrailList.length; i++) {
                breadcrumbItemList = {};
                breadcrumbItemList['@type'] = "ListItem";
                breadcrumbItemList['position'] = i + 2;
                breadcrumbItemList["name"] = typeof breadCrumbTrailList[i].label != "undefined" ? breadCrumbTrailList[i].label :
                    breadCrumbTrailList[i].name;
                if (typeof breadCrumbTrailList[i].url != "undefined" && breadCrumbTrailList[i].url.length > 0) {
                    breadcrumbItemList["item"] = window.location.origin + breadCrumbTrailList[i].url;
                } else {
                    breadcrumbItemList["item"] = window.location.href;
                }   
                breadcrumb['itemListElement'].push(breadcrumbItemList);
            }
        }
        else{
            breadcrumbItemList = {};
            breadcrumbItemList['@type'] = "ListItem";
            breadcrumbItemList['position'] = 1;

            itemDetails = {};
            itemDetails["@id"] = schemaJS.brandHostname;
            itemDetails["name"] = "Home";
            breadcrumbItemList['item'] = itemDetails;
            breadcrumb['itemListElement'].push(breadcrumbItemList);
            for (var i = 0; i < breadCrumbTrailList.length; i++) {
                breadcrumbItemList = {};
                breadcrumbItemList['@type'] = "ListItem";
                breadcrumbItemList['position'] = i + 2;
    
    
                itemDetails = {};
                if (typeof breadCrumbTrailList[i].url != "undefined" && breadCrumbTrailList[i].url.length > 0) {
                    itemDetails["@id"] = breadCrumbTrailList[i].url;
                } else {
                    itemDetails["@id"] = window.location.href;
                }
                itemDetails["name"] = typeof breadCrumbTrailList[i].label != "undefined" ? breadCrumbTrailList[i].label :
                    breadCrumbTrailList[i].name;
                breadcrumbItemList['item'] = itemDetails;
    
                breadcrumb['itemListElement'].push(breadcrumbItemList);
            }
        }

        var el1 = document.createElement('script');
        el1.type = 'application/ld+json';
        el1.text = JSON.stringify(breadcrumb);
        document.querySelector('head').appendChild(el1);
    },
