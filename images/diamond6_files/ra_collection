				yfretDataLayerJS.deleteProductFromWishlistEventResponse(analyticsJS.deleteProductFromWishlistEventData);
			}


		},

		//"Sign In" event handler
		userSignInHandler: function(userSignInEventResponseData){
			analyticsJS.userSignInEventData = userSignInEventResponseData;
			//1.ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
				adobeDataLayerJS.userSignInEventResponse(analyticsJS.userSignInEventData);
			}
			//TODO:2. YFRET Handler invocation
			//GA handler invocation
			if(analyticsJS.isGAEnabled && typeof(gaDataLayerJS) != "undefined"){
				gaDataLayerJS.userSignInEventResponse(analyticsJS.userSignInEventData);
			}
		},
			//User Logout event handler
		userlogoutHandler:function(){
			//1.ADOBE handler invocation
			if(analyticsJS.isAdobeAnalyticsEnabled && typeof(adobeDataLayerJS) != "undefined"){
